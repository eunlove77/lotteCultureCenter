	this.excelImportExport = this.application.getVariable("excelImportExport");
};

this.fn_list = function(page)
{
	this.ds_stlm_rfnd_list.clearData();
	this.ds_stlm_rfnd_excel_list.clearData();
	var strSvcId    = "list";
	var strSvcUrl   = this.application.Admin_svc + "::stlmRfndMng/stlmRfndList.do";
	var inData      = "search=this.parent.parent.parent.parent.Div00.form.Div00.form.ds_search";
	var outData     = "this.ds_stlm_rfnd_list=output1 this.ds_stlm_rfnd_excel_list=output2";
	var strArg      = "";
	var callBackFnc = "fnCallback";
	var isAsync     = true;
	
	var tranObj = {
		strSvcId : strSvcId,
		strSvcUrl : strSvcUrl,
		inData : inData,
		outData : outData,
		strArg : strArg,
		callBackFnc : callBackFnc,
