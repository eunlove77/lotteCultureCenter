}

this.fnCallback = function(svcID, errorCode, errorMsg)
{
	// 에러 시 화면 처리 내역
  if(errorCode != 0)
  {
    this.alert(errorCode+"\n"+errorMsg);
	return;
  }

  switch(svcID)
  {
	case "list":
		this.Static01.set_text("(" + this.ds_stlm_rfnd_list.getRowCount() + "개)");
		
		break;
  }
}

// 그리드 셀 클릭
this.Div02_Grid00_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	if(this.ds_stlm_rfnd_list.getColumn(e.row, "atlctRsvNo") != "소계"){
		// 주문번호 클릭 시 결제내역 상세 팝업
		if(e.row != null && e.cell == "0"){
			this.cnt++;
			var oArg = {
				atlctRsvNo : this.ds_stlm_rfnd_list.getColumn(e.row, "atlctRsvNo"),
				menuSeq : this.getOwnerFrame().param.MENU_ID
			};
			
			this.gfn_openPopup("popupOpen" + this.cnt, oArg, "StlmMng::StlmCmpltDtl_popup.xfdl", this, 1280, 900, true, true, null);
		}
		
		// 회원번호, 회원명 클릭 시 회원 상세 팝업
		if(e.row != null && (e.cell == "1" || e.cell == "2")){
			this.cnt++;
			var oArg = {
					mbrNo : this.ds_stlm_rfnd_list.getColumn(e.row, "mbrNo")
					, menuId : this.getOwnerFrame().param.MENU_ID
					};
		
			this.gfn_openPopup("mbrDtlPop" + this.cnt, oArg, "MemberMng::MemberDtl.xfdl", this, null, null, true, true, null, "회원 상세");
		}
		
		// 강좌코드, 강좌명
		if(e.row != null && (e.cell == "8" || e.cell == "9")){
			var pblPmprcustParntBrchCd = new String(this.gfn_nvl(this.ds_stlm_rfnd_list.getColumn(e.row, "pblPmprcustParntBrchCd"))); 
			var pblPmprcustParntLectCd = new String(this.gfn_nvl(this.ds_stlm_rfnd_list.getColumn(e.row, "pblPmprcustParntLectCd"))); 
		
			var arrArguments = {
				brchCd : pblPmprcustParntBrchCd == "" ? this.ds_stlm_rfnd_list.getColumn(e.row, "brchCd") : pblPmprcustParntBrchCd,
				yy : this.ds_stlm_rfnd_list.getColumn(e.row, "yy"),
				lectSmsterCd : this.ds_stlm_rfnd_list.getColumn(e.row, "lectSmsterCd"),
				lectCd : pblPmprcustParntLectCd == "" ? this.ds_stlm_rfnd_list.getColumn(e.row, "lectCd") : pblPmprcustParntLectCd,
				menuSeq : this.getOwnerFrame().param.MENU_ID
			}
			
			this.cnt++;
			this.gfn_openPopup("ClassDtl" + this.cnt, arrArguments, "ClassMng::ClassDtl.xfdl", this, 1280, 720, true, true, null);
		}
		
		// 환불규정 확인서명
		if(e.row != null && e.cell == "44"){
			this.cnt++; 
			var oArg = {
				atlctRsvNo : this.ds_stlm_rfnd_list.getColumn(e.row, "atlctRsvNo"),
				menuSeq : this.getOwnerFrame().param.MENU_ID
			};
			
			this.gfn_openPopup("popupOpen" + this.cnt, oArg, "StlmMng::StlmCmpltMng/RfndRuleSign_popup.xfdl", this, 460, 450, true, true, null);
		}
	}
};

