    <Layouts>
      <Layout height="1000" mobileorientation="landscape" width="1280">
        <Static id="title" taborder="0" text="결제완료 관리" left="10" top="10" width="120" height="60" onclick="title_onclick"/>
        <Static id="navigator" taborder="1" text="Home" left="1050" top="10" height="60" right="0"/>
        <Div id="Div00" taborder="2" text="Div00" left="10" top="80" right="10" bottom="10">
          <Layouts>
            <Layout>
              <Div id="Div00" taborder="4" text="Div00" left="0" top="0" height="210" url="StlmMng::StlmCmpltMng/StlmCmpltMngSearch.xfdl" right="0"/>
              <Div id="Div01" taborder="1" text="Div01" left="0" top="220" url="StlmMng::StlmCmpltMng/StlmCmpltMngList.xfdl" right="0" bottom="0"/>
            </Layout>
          </Layouts>
        </Div>
