    <Script type="xscript5.1"><![CDATA[/**************************************************************************
 * include 영역 
 **************************************************************************/
include "Base::libCommon.xjs";
include "Base::popup.xjs";


/**************************************************************************
 * FORM 변수 선언 영역
 **************************************************************************/
this.application = null;
var objPageingDs;
this.cnt = 0;
var searchFrm = this.parent.parent.parent.parent.Div00.form.Div00.form;
this.excelFileName = "결제완료_관리_";

/**************************************************************************
 * FORM EVENT 영역(onload)
 **************************************************************************/
this.StlmCmpltMngList_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	this.excelImportExport = this.application.getVariable("excelImportExport");
	
	this.gfnSetCheckBoxNoDataset(this.ds_stlm_cmplt_list, this.Div02.form.Grid00, 0);
	
	// 그리드 개인화
	//this.fn_personalization("getPsnlztn");
	
	//톡전송 버튼 세팅
	var talkObj = {
		columnObj : {
						mbrNo : "mbrNo"		//trgtDs의 회원번호 컬럼 매칭
					,	korNm : "korNm"		//trgtDs의 회원명 컬럼 매칭
					,	hpNo : "hpNo"		//trgtDs의 수신번호 컬럼 매칭
					,	smsRcvAgrYn : "smsRcvAgrYn"	//trgtDs의 수신여부 컬럼 매칭
					},
		trgtDs : this.ds_rcv_obj,	
		menuId : this.getOwnerFrame().param.MENU_ID,
		_this : this		//별도의 유효성체크 함수가 있는 경우 선언하고 fn_talk_check 함수 생성
	}
	this.Div03.form.fn_init(talkObj);	//톡전송 버튼div의 init 실행
};

this.fn_list = function(page)
{
	this.ds_stlm_cmplt_list.clearData();
	
	var strSvcId    = "list";
	var strSvcUrl   = this.application.Admin_svc + "::stlmMng/stlmCmpltList.do";
	var inData      = "search=this.parent.parent.parent.parent.Div00.form.Div00.form.ds_search";
	var outData     = "ds_stlm_cmplt_list=output1";
	var strArg      = "";
	var callBackFnc = "fnCallback";
	var isAsync     = true;
	
	var tranObj = {
		strSvcId : strSvcId,
		strSvcUrl : strSvcUrl,
		inData : inData,
		outData : outData,
		strArg : strArg,
		callBackFnc : callBackFnc,
		isAsync : isAsync,
		isPopup : false,
		menuSeq : this.getOwnerFrame().param.MENU_ID
	};
	
	this.gfn_transaction(tranObj);
}

this.fnCallback = function(svcID, errorCode, errorMsg)
{
	// 에러 시 화면 처리 내역
  if(errorCode != 0)
  {
    this.alert(errorCode+"\n"+errorMsg);
	return;
  }

  switch(svcID)
  {
	case "list":
		this.Static01.set_text("(" + this.ds_stlm_cmplt_list.getRowCount() + "개)");
		break;
	case "getPsnlztn":
		var format = this.ds_grid_psnlztn.getColumn(0, "format");
		if(format !== undefined){
			this.Div02.form.Grid00.set_formats(format);
		}
		break;
  }
}

// 그리드 셀 클릭 이벤트
this.Div02_Grid00_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	if(this.ds_stlm_cmplt_list.getColumn(e.row, "atlctRsvNo") != "소계"){
		// 주문번호 클릭 시 결제내역 상세 팝업
		if(e.row != null && e.cell == "1"){
			this.cnt++;
			var oArg = {
				atlctRsvNo : obj.getCellText(e.row, e.cell),
				menuSeq : this.getOwnerFrame().param.MENU_ID
			};
			
			this.gfn_openPopup("popupOpen" + this.cnt, oArg, "StlmMng::StlmCmpltDtl_popup.xfdl", this, 1280, 900, true, true, null);
		}
		
		// 회원번호, 회원명 클릭 시 회원 상세 팝업
		if(e.row != null && (e.cell == "2" || e.cell == "3")){
			this.cnt++;
			var oArg = {
					mbrNo : this.ds_stlm_cmplt_list.getColumn(e.row, "mbrNo")
					, menuId : this.getOwnerFrame().param.MENU_ID
					};
		
			this.gfn_openPopup("mbrDtlPop" + this.cnt, oArg, "MemberMng::MemberDtl.xfdl", this, null, null, true, true, null, "회원 상세");
		}
		
		// 강좌코드, 강좌명
		if(e.row != null && (e.cell == "9" || e.cell == "10")){
			
			var pblPmprcustParntBrchCd = new String(this.gfn_nvl(this.ds_stlm_cmplt_list.getColumn(e.row, "pblPmprcustParntBrchCd"))); 
			var pblPmprcustParntLectCd = new String(this.gfn_nvl(this.ds_stlm_cmplt_list.getColumn(e.row, "pblPmprcustParntLectCd"))); 
			
			var arrArguments = {
				brchCd : pblPmprcustParntBrchCd == "" ? this.ds_stlm_cmplt_list.getColumn(e.row, "brchCd") : pblPmprcustParntBrchCd,
				yy : this.ds_stlm_cmplt_list.getColumn(e.row, "yy"),
				lectSmsterCd : this.ds_stlm_cmplt_list.getColumn(e.row, "lectSmsterCd"),
				lectCd : pblPmprcustParntLectCd == "" ? this.ds_stlm_cmplt_list.getColumn(e.row, "lectCd") : pblPmprcustParntLectCd,
				menuSeq : this.getOwnerFrame().param.MENU_ID
			}
			
			this.cnt++;
			this.gfn_openPopup("ClassDtl" + this.cnt, arrArguments, "ClassMng::ClassDtl.xfdl", this, 1280, 720, true, true, null);
		}
		
		// 환불규정 확인서명
		if(e.row != null && e.cell == "41"){
			this.cnt++; 
			var oArg = {
				atlctRsvNo : this.ds_stlm_cmplt_list.getColumn(e.row, "atlctRsvNo"),
				menuSeq : this.getOwnerFrame().param.MENU_ID
			};
			
			this.gfn_openPopup("popupOpen" + this.cnt, oArg, "StlmMng::StlmCmpltMng/RfndRuleSign_popup.xfdl", this, 460, 450, true, true, null);
		}
	}
};

