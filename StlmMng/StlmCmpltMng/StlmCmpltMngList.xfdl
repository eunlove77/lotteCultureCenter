// 톡발송 유효성 함수
this.fn_talk_check = function()
{
	var chkYn = false;
	var nRow = -1;
	this.ds_rcv_obj.clearData();
	this.ds_rcv_obj.checkarray = new Array();
	for(var i=0; i<this.ds_stlm_cmplt_list.getRowCount(); i++){
		if(this.ds_stlm_cmplt_list.checkarray[i] > 0){
			chkYn = true;
			var duplYn = false;
			if(nRow > -1){
				for(var j=0;j<=this.ds_rcv_obj.getRowCount();j++){
					if(this.ds_rcv_obj.getColumn(j, "mbrNo") == this.ds_stlm_cmplt_list.getColumn(i, "mbrNo")){
						duplYn = true;
					}
				}
			}
			
			if(!duplYn){
				nRow = this.ds_rcv_obj.addRow();
				this.ds_rcv_obj.setColumn(nRow, "mbrNo", this.ds_stlm_cmplt_list.getColumn(i, "mbrNo"));
				this.ds_rcv_obj.setColumn(nRow, "korNm", this.ds_stlm_cmplt_list.getColumn(i, "mbrNm"));
				this.ds_rcv_obj.setColumn(nRow, "hpNo", this.ds_stlm_cmplt_list.getColumn(i, "hpNo"));
				this.ds_rcv_obj.setColumn(nRow, "smsRcvAgrYn", this.ds_stlm_cmplt_list.getColumn(i, "smsRcvAgrYn"));
				
				this.ds_rcv_obj.setColumn(nRow, "lectNm", this.ds_stlm_cmplt_list.getColumn(i, "lectNm"));
				this.ds_rcv_obj.setColumn(nRow, "lectStDtmVal", this.ds_stlm_cmplt_list.getColumn(i, "lectStDtmVal"));
				this.ds_rcv_obj.setColumn(nRow, "lectRmNm", this.ds_stlm_cmplt_list.getColumn(i, "lectRmNm"));
				this.ds_rcv_obj.setColumn(nRow, "lectEndDtmVal", this.ds_stlm_cmplt_list.getColumn(i, "lectEndDtmVal"));
				this.ds_rcv_obj.checkarray[nRow] = 1;
			}
		}
	}
};

