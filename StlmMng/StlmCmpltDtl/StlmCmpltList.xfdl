	dsFgftList.addColumn("lectSmsterCd", "int", 256);
	this.addChild("ds_fgft_list", dsFgftList);
	var arrPrmtnSeqno = [];
	var fgftIndex = 0;
	for(var i=0;i<this.ds_stlm_cmplt_list.getRowCount();i++){
		if(this.gfn_nvl(this.ds_stlm_cmplt_list.getColumn(i, "fgftAppNo")) == "" && this.ds_stlm_cmplt_list.getColumn(i, "prmtnSeqno").length > 0){
		trace(this.ds_stlm_cmplt_list.getColumn(i, "prmtnSeqno"));
			fgftApplYn = true;
			this.ds_fgft_list.set_enableevent(false);
			this.ds_fgft_list.insertRow(fgftIndex);
			this.ds_fgft_list.setColumn(fgftIndex, "actlAtlctNpleNm", this.ds_stlm_cmplt_list.getColumn(i, "actlAtlctNpleNm"));
			this.ds_fgft_list.setColumn(fgftIndex, "brchCdNm", this.ds_stlm_cmplt_list.getColumn(i, "brchCdNm"));
			this.ds_fgft_list.setColumn(fgftIndex, "lectTpCdNm", this.ds_stlm_cmplt_list.getColumn(i, "lectTpCdNm"));
			this.ds_fgft_list.setColumn(fgftIndex, "lectCd", this.ds_stlm_cmplt_list.getColumn(i, "lectCd"));
			this.ds_fgft_list.setColumn(fgftIndex, "lectNm", this.ds_stlm_cmplt_list.getColumn(i, "lectNm"));
			this.ds_fgft_list.setColumn(fgftIndex, "tcNm", this.ds_stlm_cmplt_list.getColumn(i, "tcNm"));
			this.ds_fgft_list.setColumn(fgftIndex, "actlAtlctNpleSeqno", this.ds_stlm_cmplt_list.getColumn(i, "actlAtlctNpleSeqno"));
			this.ds_fgft_list.setColumn(fgftIndex, "atlctRsvNo", this.ds_stlm_cmplt_list.getColumn(i, "atlctRsvNo"));
			this.ds_fgft_list.setColumn(fgftIndex, "mbrNo", this.ds_stlm_cmplt_list.getColumn(i, "mbrNo"));
			this.ds_fgft_list.setColumn(fgftIndex, "brchCd", this.ds_stlm_cmplt_list.getColumn(i, "brchCd"));
			this.ds_fgft_list.setColumn(fgftIndex, "yy", this.ds_stlm_cmplt_list.getColumn(i, "yy"));
			this.ds_fgft_list.setColumn(fgftIndex, "lectSmsterCd", this.ds_stlm_cmplt_list.getColumn(i, "lectSmsterCd"));
			this.ds_fgft_list.set_enableevent(true);
			
			for(var j=0;j<this.ds_stlm_cmplt_list.getColumn(i, "prmtnSeqno").split(",").length;j++){
				var duplYn = false;
				for(var k=0;k<arrPrmtnSeqno.length;k++){
					if(arrPrmtnSeqno[k] == this.ds_stlm_cmplt_list.getColumn(i, "prmtnSeqno").split(",")[j]){
						duplYn = true;
					}
				}
				if(!duplYn){
					arrPrmtnSeqno.push(this.ds_stlm_cmplt_list.getColumn(i, "prmtnSeqno").split(",")[j]);
				}
			}
			
			
			fgftIndex++;
		}
	}
	
	if(fgftApplYn){
		var oArg = {
			menuId : this.menuSeq
			, ds_fgft_list : this.ds_fgft_list
			, ds_stlm_cmplt_list : this.ds_stlm_cmplt_list
			, parent_list_form : this.parent.parent.parent.parent.parent.opener.divWork.form
