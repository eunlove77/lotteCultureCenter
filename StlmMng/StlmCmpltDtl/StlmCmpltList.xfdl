this.menuSeq = this.parent.parent.parent.parent.parent.menuSeq;

/**************************************************************************
 * FORM EVENT 영역(onload)
 **************************************************************************/
this.PayCmpltList_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	this.excelImportExport = this.application.getVariable("excelImportExport");
	//this.parent.parent.parent.parent.parent.opener.divWork.form.fn_list()
};

// 관리자 취소 버튼 클릭
this.Button04_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if(this.lookup("admCnclDiv") === undefined && this.lookup("orderInfoModifyDiv") === undefined){
		this.parent.parent.parent.parent.actlAtlctNpleSeqno = this.ds_stlm_cmplt_list.getColumn(this.ds_stlm_cmplt_list.rowposition, "actlAtlctNpleSeqno");
		
		this.parent.parent.parent.form.Div03.setOffsetTop(this.parent.parent.parent.form.Div03.getOffsetTop() + 470);
		this.parent.parent.parent.form.Div04.setOffsetTop(this.parent.parent.parent.form.Div04.getOffsetTop() + 470);
		this.parent.parent.parent.form.Div05.setOffsetTop(this.parent.parent.parent.form.Div05.getOffsetTop() + 470);
		var divObj = new Div("admCnclDiv");
		divObj.set_left(0);
		divObj.set_right(0);
		divObj.set_top(495);
		divObj.set_height(470);
		divObj.set_url("StlmMng::StlmCmpltDtl/AdmCncl.xfdl");
		
		this.parent.parent.parent.form.addChild("admCnclDiv", divObj);
		divObj.show();
		
		this.parent.parent.parent.setOffsetHeight(this.parent.parent.parent.getOffsetHeight() + 470); // div
		this.parent.parent.parent.parent.resetScroll(); // form
	}
	
	
};

// 주문정보 수정 버튼 클릭
this.Button05_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if(this.lookup("orderInfoModifyDiv") === undefined && this.lookup("orderInfoModifyDiv") === undefined){
		this.parent.parent.parent.parent.actlAtlctNpleSeqno = this.ds_stlm_cmplt_list.getColumn(this.ds_stlm_cmplt_list.rowposition, "actlAtlctNpleSeqno");
		
		this.parent.parent.parent.form.Div03.set_top(this.parent.parent.parent.form.Div03.getPixelTop() + 610);
		this.parent.parent.parent.form.Div04.set_top(this.parent.parent.parent.form.Div04.getPixelTop() + 610);
		this.parent.parent.parent.form.Div05.set_top(this.parent.parent.parent.form.Div05.getPixelTop() + 610);
		var divObj = new Div("orderInfoModifyDiv");
		divObj.set_left(0);
		divObj.set_right(0);
		divObj.set_top(495);
		divObj.set_height(610);
		divObj.set_url("StlmMng::StlmCmpltDtl/OrderInfoModify.xfdl");
		
		this.parent.parent.parent.form.addChild("orderInfoModifyDiv", divObj);
