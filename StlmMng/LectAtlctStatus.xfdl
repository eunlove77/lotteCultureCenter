this.LectAtlctStatus_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();	
	this.txtMenuNm.set_text(obj.parent.parent.fvMenuNM);
	
	searchFrm.fn_data_list();
