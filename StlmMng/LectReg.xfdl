		var tranObj = {
			strSvcId : strSvcId,
			strSvcUrl : strSvcUrl,
			inData : inData,
			outData : outData,
			strArg : strArg,
			callBackFnc : callBackFnc,
			isAsync : isAsync,
			isPopup : false,
			menuSeq : this.getOwnerFrame().param.MENU_ID
		};

		this.gfn_transaction(tranObj);
