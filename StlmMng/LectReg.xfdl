// 금액필드 입력 포맷
this.fn_num_format = function(obj:nexacro.Edit,e:nexacro.InputEventInfo)
{
	var num = nexacro.replaceAll(obj.value.toString(), ",", "");
	if(num.startsWith("0") && num.length > 1){
		num = num.substring(1);
	}
	
	if(this.gsStlmAmt < Number(num)) num = this.gsStlmAmt.toString();
	obj.set_value(this.gfn_setComma(num));
};

// 결제 버튼 클릭
this.Div00_Div04_Button02_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if(this.Div00.form.Div04.form.CheckBox00.value == 0){
		alert("환불 규정확인을 받아야 결제가 가능합니다. 환불규정 확인 후 서명을 해주세요.");
		return;
	}

	var gsStlmAmt = 0, gsLectAmt = 0, gsAddAmt = 0, gsDcAmt = 0;
	var duplYn = false;
	var brchCd = "", yy = "", lectSmsterCd = "", lectCd = "", actlAtlctNpleNm = "", sexCd = "", fmlyRelCd = "", bday = "";
	var lectNm = "";
	for(var i=0;i<this.ds_class_list.getRowCount();i++){
		gsStlmAmt += this.ds_class_list.getColumn(i, "lectAmt");
		gsLectAmt += this.ds_class_list.getColumn(i, "lectAmt");
		gsStlmAmt += this.ds_class_list.getColumn(i, "optnAmt");
		gsAddAmt += this.ds_class_list.getColumn(i, "optnAmt");
		gsStlmAmt -= this.ds_class_list.getColumn(i, "oflnDcAmt");
		gsDcAmt += this.ds_class_list.getColumn(i, "oflnDcAmt");
		gsStlmAmt -= this.ds_class_list.getColumn(i, "grdeDcAmt");
		gsDcAmt += this.ds_class_list.getColumn(i, "grdeDcAmt");
		gsStlmAmt -= this.ds_class_list.getColumn(i, "cpnDcAmt");
		gsDcAmt += this.ds_class_list.getColumn(i, "cpnDcAmt");
		gsStlmAmt -= this.ds_class_list.getColumn(i, "optnDcAmt");
		gsDcAmt += this.ds_class_list.getColumn(i, "optnDcAmt");
		
		brchCd = this.ds_class_list.getColumn(i, "brchCd");
		yy = this.ds_class_list.getColumn(i, "yy");
		lectSmsterCd = this.ds_class_list.getColumn(i, "lectSmsterCd");
		lectCd = this.ds_class_list.getColumn(i, "lectCd");
		actlAtlctNpleNm = this.ds_class_list.getColumn(i, "actlAtlctNpleNm");
		sexCd = this.ds_class_list.getColumn(i, "sexCd");
		fmlyRelCd = this.ds_class_list.getColumn(i, "fmlyRelCd");
		bday = this.ds_class_list.getColumn(i, "bday");
		var count = 0;
		for(var j=0;j<this.ds_class_list.getRowCount();j++){
			if(this.ds_class_list.getColumn(j, "brchCd") == brchCd
				&& this.ds_class_list.getColumn(j, "yy") == yy
				&& this.ds_class_list.getColumn(j, "lectSmsterCd") == lectSmsterCd
				&& this.ds_class_list.getColumn(j, "lectCd") == lectCd
				&& this.ds_class_list.getColumn(j, "actlAtlctNpleNm") == actlAtlctNpleNm
				&& this.ds_class_list.getColumn(j, "sexCd") == sexCd
				&& this.ds_class_list.getColumn(j, "fmlyRelCd") == fmlyRelCd
				&& this.ds_class_list.getColumn(j, "bday") == bday){
				count++;
				if(count > 1){
					lectNm = this.ds_class_list.getColumn(j, "lectNm");
					duplYn = true;
				}
			}
		}
	}
	
	if(duplYn){
		alert('강좌명 : ' + lectNm + '\n같은 강좌에 중복된 실수강자가 있어 결제 하실 수 없습니다.');
		return;
	}
	
	var crdStlmAmt = nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div00.form.Edit00.value), ",", "") == "" ? 0 : Number(nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div00.form.Edit00.value), ",", ""));
	var lpntUseAmt = nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div04.form.Edit00.value), ",", "") == "" ? 0 : Number(nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div04.form.Edit00.value), ",", ""));
	var cshStlmAmt = nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div01.form.Edit00.value), ",", "") == "" ? 0 : Number(nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div01.form.Edit00.value), ",", ""));
	var ltteGiftStlmAmt = nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div05.form.Edit00.value), ",", "") == "" ? 0 : Number(nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div05.form.Edit00.value), ",", ""));
	var otgiftStlmAmt = nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div02.form.Edit00.value), ",", "") == "" ? 0 : Number(nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div02.form.Edit00.value), ",", ""));
	var etcStlmAmt = nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div06.form.Edit00.value), ",", "") == "" ? 0 : Number(nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div06.form.Edit00.value), ",", ""));
	var chngAmt = nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div03.form.Edit00.value), ",", "") == "" ? 0 : Number(nexacro.replaceAll(this.gfn_nvl(this.Div00.form.Div04.form.Div03.form.Edit00.value), ",", ""));
	
	var crdcoCd = this.Div00.form.Div04.form.Div00.form.Combo00.value;
	var chngMthdCd = this.Div00.form.Div04.form.Div03.form.Combo00.value;
	var posRcptNo = this.gfn_nvl(this.Div00.form.Div04.form.Div07.form.Edit00.value);
	
	if(gsStlmAmt < crdStlmAmt + lpntUseAmt + cshStlmAmt + ltteGiftStlmAmt + otgiftStlmAmt + etcStlmAmt){
		if(chngAmt == 0){
			alert("초과 된 총 결제예정금액 만큼 거스름돈을 입력하세요.");
			return;
		}else{
			if(gsStlmAmt < crdStlmAmt + lpntUseAmt + cshStlmAmt + ltteGiftStlmAmt + otgiftStlmAmt + etcStlmAmt - chngAmt){
				alert("입력된 금액이 총 결제금액보다 초과되어 결제 할 수 없습니다.");
				this.Div00.form.Div04.form.Div00.form.Edit00.set_value("");
				this.Div00.form.Div04.form.Div01.form.Edit00.set_value("");
				this.Div00.form.Div04.form.Div02.form.Edit00.set_value("");
				this.Div00.form.Div04.form.Div03.form.Edit00.set_value("");
				this.Div00.form.Div04.form.Div04.form.Edit00.set_value("");
				this.Div00.form.Div04.form.Div05.form.Edit00.set_value("");
				this.Div00.form.Div04.form.Div06.form.Edit00.set_value("");
				this.Div00.form.Div04.form.Div07.form.Edit00.set_value("");
				return;
			}
		}
	}
	
	if(gsStlmAmt > crdStlmAmt + lpntUseAmt + cshStlmAmt + ltteGiftStlmAmt + otgiftStlmAmt + etcStlmAmt - chngAmt){
		alert("총 결제금액 만큼 결제수단별 금액을 입력하세요.");
		this.Div00.form.Div04.form.Div00.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div01.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div02.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div03.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div04.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div05.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div06.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div07.form.Edit00.set_value("");
		return;
	}
	
	// 카드, L.Point, 기타 결제금액과 거스름돈만 입력
	// 2023.02.21 제한 풀었음
// 	if((crdStlmAmt + lpntUseAmt + etcStlmAmt > 0) && chngAmt > 0){
// 		alert("카드, L.Point, 기타 결제금액과 거스름돈을 같이 입력 할 수 없습니다.");
// 		this.Div00.form.Div04.form.Div00.form.Edit00.set_value("");
// 		this.Div00.form.Div04.form.Div03.form.Edit00.set_value("");
// 		this.Div00.form.Div04.form.Div04.form.Edit00.set_value("");
// 		this.Div00.form.Div04.form.Div06.form.Edit00.set_value("");
// 		return;
// 	}
	
	// 카드사 미선택
	if(crdcoCd == "" && crdStlmAmt > 0){
		alert("카드사를 선택하세요.");
		return;
	}
	
	// 거스름돈 지급방식 미선택
	if(chngMthdCd == "" && chngAmt > 0){
		alert("거스름돈 지급 방식을 선택하세요.");
		this.Div00.form.Div04.form.Div03.form.Edit00.set_value("");
		return;
	}
	
	if(confirm("결제하시겠습니까?")){
		this.removeChild("ds_payment_param");
		var dsPaymentParam = new Dataset();
		dsPaymentParam.name = "ds_payment_param";
		dsPaymentParam.addColumn("atchFileId", "string", 256);
		dsPaymentParam.addColumn("gsStlmAmt", "int", 256);
		dsPaymentParam.addColumn("crdcoCd", "string", 256);
		dsPaymentParam.addColumn("crdStlmAmt", "int", 256);
		dsPaymentParam.addColumn("lpntUseAmt", "int", 256);
		dsPaymentParam.addColumn("cshStlmAmt", "int", 256);
		dsPaymentParam.addColumn("ltteGiftStlmAmt", "int", 256);
		dsPaymentParam.addColumn("otgiftStlmAmt", "int", 256);
		dsPaymentParam.addColumn("etcStlmAmt", "int", 256);
		dsPaymentParam.addColumn("gsLectAmt", "int", 256);
		dsPaymentParam.addColumn("gsAddAmt", "int", 256);
		dsPaymentParam.addColumn("gsDcAmt", "int", 256);
		dsPaymentParam.addColumn("chngMthdCd", "string", 256);
		dsPaymentParam.addColumn("chngAmt", "int", 256);
		dsPaymentParam.addColumn("posRcptNo", "string", 256);
		this.addChild("ds_payment_param", dsPaymentParam);
		
		this.ds_payment_param.set_enableevent(false);
		this.ds_payment_param.insertRow(0);
		this.ds_payment_param.setColumn(0, "atchFileId", this.Div00.form.Div04.form.Static03.text);
		this.ds_payment_param.setColumn(0, "gsStlmAmt", gsStlmAmt);
		this.ds_payment_param.setColumn(0, "crdcoCd", crdcoCd);
		this.ds_payment_param.setColumn(0, "crdStlmAmt", crdStlmAmt);
		this.ds_payment_param.setColumn(0, "lpntUseAmt", lpntUseAmt);
		this.ds_payment_param.setColumn(0, "cshStlmAmt", cshStlmAmt);
		this.ds_payment_param.setColumn(0, "ltteGiftStlmAmt", ltteGiftStlmAmt);
		this.ds_payment_param.setColumn(0, "otgiftStlmAmt", otgiftStlmAmt);
		this.ds_payment_param.setColumn(0, "etcStlmAmt", etcStlmAmt);
		this.ds_payment_param.setColumn(0, "gsLectAmt", gsLectAmt);
		this.ds_payment_param.setColumn(0, "gsAddAmt", gsAddAmt);
		this.ds_payment_param.setColumn(0, "gsDcAmt", gsDcAmt);
		this.ds_payment_param.setColumn(0, "chngMthdCd", chngMthdCd);
		this.ds_payment_param.setColumn(0, "chngAmt", chngAmt);
		this.ds_payment_param.setColumn(0, "posRcptNo", posRcptNo);
		this.ds_payment_param.set_enableevent(true);
		this.ds_err_info.clearData();
		
		var strSvcId    = "payment";
		var strSvcUrl   = this.application.Admin_svc + "::lectReg/payment.do";
		var inData      = "classList=this.ds_class_list paymentParam=this.ds_payment_param mbrParam=this.ds_member_info";
		var outData     = "this.ds_err_info=output1 this.ds_grde_list=output2 this.ds_ex_grde_list=output3 this.ds_all_dc_dtl=output4";
		var strArg      = "";
		var callBackFnc = "fnCallback";
