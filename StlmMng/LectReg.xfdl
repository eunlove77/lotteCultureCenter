    <Script type="xscript5.1"><![CDATA[/**************************************************************************
 * include 영역 
 **************************************************************************/
include "Base::libCommon.xjs";
include "Base::popup.xjs";

/**************************************************************************
 * FORM 변수 선언 영역
 **************************************************************************/
this.application = null;
this.cnt = 0;
this.menuSeq = this.getOwnerFrame().param.MENU_ID;

this.brchCd = "";
this.yy = "";
this.lectSmsterCd = "";

/**************************************************************************
 * FORM EVENT 영역(onload)
 **************************************************************************/
this.LectReg_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	this.txtMenuNm.set_text(obj.parent.parent.fvMenuNM);
	
	this.fn_data_list();
};

this.fn_data_list = function()
{
	var strSvcId    = "dataList";
	var strSvcUrl   = this.application.Admin_svc + "::lectReg/getLectRegDataList.do";
	var inData      = "";
	var outData     = "this.ds_smster_list=output1";
	var strArg      = "";
	var callBackFnc = "fnCallback";
	var isAsync     = true;
	
	var tranObj = {
		strSvcId : strSvcId,
		strSvcUrl : strSvcUrl,
		inData : inData,
		outData : outData,
		strArg : strArg,
		callBackFnc : callBackFnc,
		isAsync : isAsync,
		isPopup : false,
		menuSeq : this.getOwnerFrame().param.MENU_ID
	};
	
	this.gfn_transaction(tranObj);
};

this.fnCallback = function(svcID, errorCode, errorMsg)
{
	// 에러 시 화면 처리 내역
	  if(errorCode != 0)
	  {
		this.alert(errorCode+"\n"+errorMsg);
		return;
	  }

	  switch(svcID)
	  {
		case "dataList":
			// 학기
			if(this.lectSmsterCd == ""){
				var month = new nexacro.Date().getMonth()+1;
		
				if(3 <= month && month <= 5)
				{
					//봄
					this.lectSmsterCd = 0;	
				}
				else if(6 <= month && month <= 8)
				{
					//여름
					this.lectSmsterCd = 1;	
				}
				else if(9 <= month && month <= 11)
				{
					//가을
					this.lectSmsterCd = 2;	
				}	
				else
				{
					//겨울
					this.lectSmsterCd = 3;	
				}
			}
			this.ds_search.setColumn(0, "lectSmsterCd", this.lectSmsterCd);
			
			//년도
			this.fn_year_add();
			
			// 대기자 현황 -> 수강등록으로 넘어오는 회원정보 세팅
			if(new String(this.application.LECT_REG).valueOf() != "undefined"){
				this.fn_set_waitMember();
			}
	
			break;
		case "setMember":
			this.ds_tot_grde_list.clear();
		
			this.ds_tot_grde_list.set_enableevent(false);
			for(var i=0;i<this.ds_grde_list.getRowCount();i++){
				this.ds_tot_grde_list.addColumn("cdNm" + i, "string", 256);
			}
			this.ds_tot_grde_list.insertRow(0);
			for(var i=0;i<this.ds_grde_list.getRowCount();i++){
				this.ds_tot_grde_list.setColumn(0, "cdNm" + i, this.ds_grde_list.getColumn(i, "cont"));
			}
			for(var i=0;i<this.ds_ex_grde_list.getRowCount();i++){
				this.ds_tot_grde_list.insertRow(i+1);
				this.ds_tot_grde_list.setColumn(i+1, "cdNm0", this.ds_ex_grde_list.getColumn(i, "cont"));
			}
			this.ds_tot_grde_list.set_enableevent(true);
			this.Div00.form.Div03.form.Static00.set_visible(true);
			this.Div00.form.Div03.form.Static00.set_text("▶ 에누리 전체 사용횟수: " + this.ds_all_dc_dtl.getColumn(0, "allUseCnt") + "/" + this.ds_all_dc_dtl.getColumn(0, "allDcCnt"));
			
			this.Div00.form.Div03.form.Grid00.set_visible(true);
			this.Div00.form.Div03.form.Grid00.set_binddataset(this.ds_tot_grde_list);
			this.Div00.form.Div03.form.Grid00.createFormat();
			for(var i=0;i<this.ds_grde_list.getRowCount();i++){
				this.Div00.form.Div03.form.Grid00.setFormatColProperty(i, "size", 200);
				this.Div00.form.Div03.form.Grid00.setCellProperty("head", i, "text", this.ds_grde_list.getColumn(i, "cdNm"));
			}
			
			
			this.Div00.form.Div03.form.Button00.set_top();
			this.Div00.form.Div03.form.Button00.set_bottom(0);
			this.Div00.form.Div03.form.Button01.set_top();
			this.Div00.form.Div03.form.Button01.set_bottom(0);
			
			// 대기자 현황 -> 수강등록으로 넘어오는 수강정보 세팅
			if(new String(this.application.LECT_REG).valueOf() != "undefined"){
				this.fn_set_waitAtlct();
			}
			
			break;
		case "payment":
			// 에러
			if(this.ds_err_info.getRowCount() > 0){
				if(this.ds_err_info.getColumn(0, "errCd") == "CAPA_CNT"){
					alert("강좌명 : " + this.ds_err_info.getColumn(0, "errLectNm") + "\n정원이 마감된 강좌가 있어 결제 하실 수 없습니다.");
					return;
				}else if(this.ds_err_info.getColumn(0, "errCd") == "EXIST_CMPLT"){
					alert("강좌명 : " + this.ds_err_info.getColumn(0, "errLectNm") + "\n수강자 : " + this.ds_err_info.getColumn(0, "errActlAtlctNpleNm") + "\n수강신청 내역이 존재합니다.");
					return;
				}else if(this.ds_err_info.getColumn(0, "errCd") == "EXIST_WAITING"){
					alert("강좌명 : " + this.ds_err_info.getColumn(0, "errLectNm") + "\n대기자 : " + this.ds_err_info.getColumn(0, "errActlAtlctNpleNm") + "\n대기신청 내역이 존재합니다.");
					return;
				}else if(this.ds_err_info.getColumn(0, "errCd") == "LECT_STAT_CD"){
					alert("강좌명 : " + this.ds_err_info.getColumn(0, "errLectNm") + "\n접수 불가 강좌가 있어 결제 하실 수 없습니다.");
					return;
				}else if(this.ds_err_info.getColumn(0, "errCd") == "CPN_USE_FAILED"){
					alert(this.ds_err_info.getColumn(0, "errMsg"));
					return;
				}
			}
		
			alert("결제되었습니다.");
			
			// 강좌목록 초기화
			this.ds_class_list.clearData();
			
			// 결제정보 초기화 및 숨김
			this.Div00.form.Div04.set_visible(false);
			this.Div00.form.Div04.set_height(80);
			this.Div00.set_height(670);
			this.Div00.form.Div04.form.Div00.form.Combo00.set_index(0);
			this.Div00.form.Div04.form.Div03.form.Combo00.set_index(0);
			this.Div00.form.Div04.form.Div00.form.Edit00.set_value("");
			this.Div00.form.Div04.form.Div01.form.Edit00.set_value("");
			this.Div00.form.Div04.form.Div02.form.Edit00.set_value("");
			this.Div00.form.Div04.form.Div03.form.Edit00.set_value("");
			this.Div00.form.Div04.form.Div04.form.Edit00.set_value("");
			this.Div00.form.Div04.form.Div05.form.Edit00.set_value("");
			this.Div00.form.Div04.form.Div06.form.Edit00.set_value("");
			this.Div00.form.Div04.form.Div07.form.Edit00.set_value("");
			this.Div00.form.Div04.form.Static02.set_text("0원");
			this.Div00.form.Div04.form.Static03.set_text("");
			this.Div00.form.Div04.form.CheckBox00.set_value(0);
			
			// 회원 등급할인정보 갱신
			this.ds_tot_grde_list.set_enableevent(false);
			this.ds_tot_grde_list.clearData();
			this.ds_tot_grde_list.insertRow(0);
			for(var i=0;i<this.ds_grde_list.getRowCount();i++){
				this.ds_tot_grde_list.setColumn(0, "cdNm" + i, this.ds_grde_list.getColumn(i, "cont"));
			}
			for(var i=0;i<this.ds_ex_grde_list.getRowCount();i++){
				this.ds_tot_grde_list.insertRow(i+1);
				this.ds_tot_grde_list.setColumn(i+1, "cdNm0", this.ds_ex_grde_list.getColumn(i, "cont"));
			}
			this.ds_tot_grde_list.set_enableevent(true);
			this.Div00.form.Div03.form.Static00.set_visible(true);
			this.Div00.form.Div03.form.Static00.set_text("▶ 에누리 전체 사용횟수: " + this.ds_all_dc_dtl.getColumn(0, "allUseCnt") + "/" + this.ds_all_dc_dtl.getColumn(0, "allDcCnt"));
			
			this.resetScroll();
			
			break;
		case "setWaitAtlct":
			if(!this.Div00.form.Div04.visible){
				this.gfnSetCheckBoxNoDataset(this.ds_class_list, this.Div00.form.Div02.form.Grid00, 0);
			}			
			this.Div00.form.Div04.set_visible(true);
			this.Div00.form.Div04.set_height(210);
			this.Div00.set_height(780);
			this.Div00.form.Div04.form.Div00.form.Combo00.set_index(0);
			this.Div00.form.Div04.form.Div03.form.Combo00.set_index(0);
			
			for(var i=0;i<this.ds_class_list.getRowCount();i++){
				for(var j=0;j<this.ds_class_list.getColCount();j++){
					this.ds_class_list.setColumn(i, this.ds_class_list.getColumnInfo(j).name, this.gfn_nvl(this.ds_class_list.getColumn(i, this.ds_class_list.getColumnInfo(j).name)));
				}
			}
			
			var lectStlmAmt = 0;
			for(var i=0;i<this.ds_class_list.getRowCount();i++){
				lectStlmAmt += this.ds_class_list.getColumn(i, "lectAmt") == "" ? 0 : Number(this.ds_class_list.getColumn(i, "lectAmt"));
				lectStlmAmt += this.ds_class_list.getColumn(i, "optnAmt") == "" ? 0 : Number(this.ds_class_list.getColumn(i, "optnAmt"));
				lectStlmAmt -= this.ds_class_list.getColumn(i, "oflnDcAmt") == "" ? 0 : Number(this.ds_class_list.getColumn(i, "oflnDcAmt"));
				lectStlmAmt -= this.ds_class_list.getColumn(i, "grdeDcAmt") == "" ? 0 : Number(this.ds_class_list.getColumn(i, "grdeDcAmt"));
				lectStlmAmt -= this.ds_class_list.getColumn(i, "cpnDcAmt") == "" ? 0 : Number(this.ds_class_list.getColumn(i, "cpnDcAmt"));
				lectStlmAmt -= this.ds_class_list.getColumn(i, "optnDcAmt") == "" ? 0 : Number(this.ds_class_list.getColumn(i, "optnDcAmt"));
			}
			this.Div00.form.Div04.form.Static02.set_text(this.gfn_setComma(lectStlmAmt) + "원");
			this.resetScroll();
			
			delete this.application.LECT_REG;
			
			break;
	  }
};

// 회원검색 버튼 클릭
this.Div00_Div01_Button01_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if(this.gfn_nvl(this.Div00.form.Div01.form.Div00.form.Static01.text) != ""){
		if(confirm("수강등록 회원을 변경하시겠습니까?")){
			this.Div00_Div01_Button00_onclick();
			var oArg = {
				_this : this
				, type : 2
				, maxCnt : 1
				, callBackFnc : this.memberCallback
				, menuId : this.menuSeq
			};
			this.cnt++;
			this.gfn_openPopup("popupOpen" + this.cnt, oArg, "MemberMng::MemberList_popup.xfdl", this, 1280, 720, true, true, null);
		}
	}else{
		var oArg = {
			_this : this
			, type : 2
			, maxCnt : 1
			,callBackFnc : this.memberCallback
			, menuId : this.menuSeq
		};
		this.cnt++;
		this.gfn_openPopup("popupOpen" + this.cnt, oArg, "MemberMng::MemberList_popup.xfdl", this, 1280, 720, true, true, null);
	}
};

// 회원팝업에서 확인 눌렀을때 콜백
this.memberCallback = function(_this, chkDs)
{	
	var yy = _this.Div00.form.Div00.form.Div00.form.Combo00.value;
	var lectSmsterCd = _this.Div00.form.Div00.form.Div00.form.Combo01.value;
	
	_this.ds_grde_list.clearData();
	_this.ds_ex_grde_list.clearData();
	_this.ds_all_dc_dtl.clearData();
	
	// 등급할인 목록
	var strSvcId    = "setMember";
	var strSvcUrl   = _this.application.Admin_svc + "::lectReg/setMember.do";
	var inData      = "";
	var outData     = "this.ds_grde_list=output1 this.ds_ex_grde_list=output2 this.ds_all_dc_dtl=output3 this.ds_member_info=output4";
	var strArg      = "mbrNo=" + chkDs.getColumn(0, "mbrNo") + " yy=" + yy + " lectSmsterCd=" + lectSmsterCd + " mbrGrdeCd=" + chkDs.getColumn(0, "mbrGrdeCd");
	var callBackFnc = "fnCallback";
	var isAsync     = true;
	
	var tranObj = {
		strSvcId : strSvcId,
		strSvcUrl : strSvcUrl,
		inData : inData,
		outData : outData,
		strArg : strArg,
		callBackFnc : callBackFnc,
		isAsync : isAsync,
		isPopup : false,
		menuSeq : _this.getOwnerFrame().param.MENU_ID
	};
	
	_this.gfn_transaction(tranObj);
};

// 회원 초기화 버튼 클릭
this.Div00_Div01_Button00_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if(this.Div00.form.Div01.form.Div00.form.Static01.text == ""){
		alert("등록된 회원이 없습니다.");
		return;
	}else{
		if(obj !== undefined && e !== undefined){
			if(!confirm("수강등록을 취소하시겠습니까? 입력된 내용은 초기화 됩니다.")){
				return;
			}
		}
		
		this.ds_member_info.clearData();
		this.Div00.form.Div01.form.Div00.form.Static01.set_text("");
		this.Div00.form.Div01.form.Div00.form.Static03.set_text("");
		this.Div00.form.Div01.form.Div00.form.Static05.set_text("");
		this.Div00.form.Div01.form.Div01.form.Static01.set_text("");
		this.Div00.form.Div01.form.Div01.form.Static03.set_text("");
		
		for(var i=0;i<this.ds_smster_list.getRowCount();i++){
			if(this.ds_smster_list.getColumn(i, "currSmsterCdYn") == "Y"){
				this.Div00.form.Div00.form.Div00.form.Combo01.set_index(i);
				
				//년도
				this.removeChild("ds_yy_list");
				var dsYyList = new Dataset();
				dsYyList.name = "ds_yy_list";
				dsYyList.addColumn("cd", "string", 256);
				dsYyList.addColumn("cdNm", "string", 256);
				
				this.addChild("ds_yy_list", dsYyList);
				
				var currYear = Number(this.ds_smster_list.getColumn(i, "yy"));
				this.ds_yy_list.set_enableevent(false);
				for(var i=0;i<16;i++){
					this.ds_yy_list.insertRow(i);
					this.ds_yy_list.setColumn(i, "cd", (currYear - i).toString());
					this.ds_yy_list.setColumn(i, "cdNm", (currYear - i) + "년");
				}
				this.ds_yy_list.set_enableevent(true);
				this.Div00.form.Div00.form.Div00.form.Combo00.setInnerDataset(this.ds_yy_list);
				this.Div00.form.Div00.form.Div00.form.Combo00.set_codecolumn("cd");
				this.Div00.form.Div00.form.Div00.form.Combo00.set_datacolumn("cdNm");
				this.Div00.form.Div00.form.Div00.form.Combo00.set_index(0);
			}
		}
		
		// 등급할인목록 초기화
		this.Div00.form.Div03.form.Grid00.set_visible(false);
		this.Div00.form.Div03.form.Static00.set_visible(false);
		this.Div00.form.Div03.form.Button00.set_bottom();
		this.Div00.form.Div03.form.Button00.set_top(0);
		this.Div00.form.Div03.form.Button01.set_bottom();
		this.Div00.form.Div03.form.Button01.set_top(0);
		this.ds_tot_grde_list.clearData();
		
		// 강좌목록 초기화
		this.ds_class_list.clearData();
		
		// 결제정보 초기화
		this.Div00.form.Div04.set_visible(false);
		this.Div00.form.Div04.form.Div00.form.Combo00.set_index(0);
		this.Div00.form.Div04.form.Div03.form.Combo00.set_index(0);
		this.Div00.form.Div04.form.Div00.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div01.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div02.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div03.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div04.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div05.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div06.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Div07.form.Edit00.set_value("");
		this.Div00.form.Div04.form.Static03.set_text("");
		this.Div00.form.Div04.form.CheckBox00.set_value(0);
		
		this.Div00.form.Div04.set_height(80);
		this.Div00.set_height(670);
		this.resetScroll();
	}
	
};

// 강좌검색 버튼 클릭
this.Div00_Div02_Button02_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if(this.gfn_nvl(this.Div00.form.Div01.form.Div00.form.Static01.text) != ""){
		var yy = this.Div00.form.Div00.form.Div00.form.Combo00.value;
