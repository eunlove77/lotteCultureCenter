// 강좌목록 실수강자 변경
this.Div00_Div02_Grid00_oncloseup = function(obj:nexacro.Grid,e:nexacro.GridEditEventInfo)
{
	if(e.col == 3){
		var oldValue = this.ds_class_list.getColumn(e.row, "fmlyCd");
		this.ds_class_list.set_enableevent(false);
		this.ds_class_list.setColumn(e.row, "fmlyCd", e.value);
		this.ds_class_list.set_enableevent(true);
		
		if(oldValue == e.value){
			alert("변경할 실수강자를 선택하세요.");
			return;
		}
		
		var lectNm = "";
		for(var i=0;i<this.ds_class_list.getRowCount();i++){
			if(i != e.row && this.ds_class_list.getColumn(i, "fmlyCd") == this.ds_class_list.getColumn(e.row, "fmlyCd")){
				lectNm = this.ds_class_list.getColumn(e.row, "lectNm");
			}
		}
		
		if(lectNm != ""){
			alert("강좌명 : " + lectNm + "\n해당 실수강자는 이미 강좌목록에 등록된 수강자 입니다.");
			this.ds_class_list.set_enableevent(false);
			this.ds_class_list.setColumn(e.row, "fmlyCd", oldValue);
			this.ds_class_list.set_enableevent(true);
			return;
		}
		
		var fmlyRelCd = e.value.substring(0, 2);
		var actlAtlctNpleNm = e.value.substring(2);
		lectNm = "";
		for(var i=0;i<this.ds_atlct_list.getRowCount();i++){
			if(this.ds_atlct_list.getColumn(i, "actlAtlctNpleNm") == actlAtlctNpleNm && this.ds_atlct_list.getColumn(i, "fmlyRelCd") == fmlyRelCd){
				lectNm = this.ds_atlct_list.getColumn(i, "lectNm");
			}
		}
		if(lectNm != ""){
			alert("강좌명 : " + lectNm + "\n해당 실수강자는 수강신청 내역이 존재합니다.");
			this.ds_class_list.set_enableevent(false);
			this.ds_class_list.setColumn(e.row, "fmlyCd", oldValue);
			this.ds_class_list.set_enableevent(true);
			return;
		}
		
		// 데이터셋이 변경된 경우
		if(this.ds_class_list.getRowType(e.row) == 4){
			if(confirm("실수강자를 변경하시겠습니까?")){
				alert("저장되었습니다.");
			}else{
				this.ds_class_list.set_enableevent(false);
				this.ds_class_list.setColumn(e.row, "fmlyCd", oldValue);
				this.ds_class_list.set_enableevent(true);
			}
		}
	}
};

// 옵션 변경 버튼 클릭
this.Div00_Div02_Button00_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var chkYn = false;
	var brchCd, yy, lectSmsterCd, lectCd, optnSeqno, optnTypNm;
	for(var i=0; i<this.ds_class_list.getRowCount(); i++){
		if(this.ds_class_list.checkarray[i] == "1"){
			chkYn = true;
			brchCd = this.ds_class_list.getColumn(i, "brchCd");
			yy = this.ds_class_list.getColumn(i, "yy");
			lectSmsterCd = this.ds_class_list.getColumn(i, "lectSmsterCd");
			lectCd = this.ds_class_list.getColumn(i, "lectCd");
			optnSeqno = this.ds_class_list.getColumn(i, "optnSeqno");
			optnTypNm = this.ds_class_list.getColumn(i, "optnTypNm");
			break;
		}
	}
	
	if(!chkYn){
		alert("재료비 또는 대여료 옵션이 포함된 동일 강좌의 실수강자 모두를 선택 후 옵션을 변경 할 수 있습니다.");
		return;
	}
	
	var lectCnt = 0;
	for(var i=0; i<this.ds_class_list.getRowCount(); i++){
		if(this.ds_class_list.getColumn(i, "brchCd") == brchCd && this.ds_class_list.getColumn(i, "yy") == yy
		&& this.ds_class_list.getColumn(i, "lectSmsterCd") == Number(lectSmsterCd) && this.ds_class_list.getColumn(i, "lectCd") == lectCd){
			lectCnt++;
		}
	}
	
	var cmpCnt = 0;
	for(var i=0; i<this.ds_class_list.getRowCount(); i++){
		if(this.ds_class_list.checkarray[i] == "1"){
			// 옵션이 없는 강좌 체크
			if(this.ds_class_list.getColumn(i, "optnUseYn") == "N"){
				alert("재료비 또는 대여료 옵션이 없는 강좌입니다.");
				return;
			}
			if(this.ds_class_list.getColumn(i, "brchCd") == brchCd && this.ds_class_list.getColumn(i, "yy") == yy
				&& this.ds_class_list.getColumn(i, "lectSmsterCd") == Number(lectSmsterCd) && this.ds_class_list.getColumn(i, "lectCd") == lectCd){
				cmpCnt++;
			}else{
				// 서론 다른 강좌 체크
				alert("동일 강좌의 실수강자를 모두 선택 후 이용 가능합니다.");
				return;
			}
		}
	}
	
	// 동일 강좌에 대한 모든 실수강자 미체크
	if(lectCnt != cmpCnt){
		alert("동일 강좌의 실수강자를 모두 선택 후 이용 가능합니다.");
		return;
	}else{
		var oArg = {
			optnSeqno : optnSeqno
			, brchCd : brchCd
			, yy : yy
			, lectSmsterCd : lectSmsterCd
			, lectCd : lectCd
			, optnTypNm : optnTypNm
			, menuId : this.menuSeq
		};
		this.cnt++;
		this.gfn_openPopup("popupOpen" + this.cnt, oArg, "StlmMng::LectReg/Optn_popup.xfdl", this, 640, 160, true, true, null);
	}
	
};

