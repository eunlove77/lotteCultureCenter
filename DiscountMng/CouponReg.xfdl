/**************************************************************************
 * 사용자 함수
 **************************************************************************/

this.fn_detail = function ()
{	
	var tranObj = {
		strSvcId : "detail",
		strSvcUrl : this.baseSvrUrl + "detail.do",
		inData : "detail=ds_detail",
		outData : "ds_detail=infoMap ds_appl_str=applStrList ds_random_list=cpnRndmList ds_mem_grade=mbrGrdeList ds_mem_excel=mbrExclList ds_appl_ctgr=applCtgrList ds_appl_lect=applLectList",
		strArg : "",
		callBackFnc : "fnCallback",
		menuSeq : this.menuId,
		isAsync : true,
		isPopup : true
	}
	this.gfn_transaction(tranObj);
};

/* 상세화면 세팅 */
this.fn_set_detail = function()
{
	this.fn_sync_checkBox();
	this.fn_sync_mbrGrde();
	
	//발급방식 변경불가
	this.Div01.form.Div00.form.radioIssuCd.set_readonly(true);
	//발급수량 변경불가
	this.Div01.form.Div02.form.MaskEdit00.set_readonly(true);
	
	//라디오버튼 값에 따른 div 변경 이벤트
	var itemInfo = new nexacro.ItemChangeEventInfo();
	
	itemInfo.postvalue = this.ds_detail.getColumn(0, "issueObjClCd");
	this.Div01_Div01_radioTrgtMem_onitemchanged(this.Div01.form.Div01.form.radioTrgtMem, itemInfo);
	
	itemInfo.postvalue = this.ds_detail.getColumn(0, "applLectClCd");
	this.Div01_Div03_radioTrgtLect_onitemchanged(this.Div01.form.Div03.form.radioTrgtLect, itemInfo);
	
	itemInfo.postvalue = this.ds_detail.getColumn(0, "issueMthdClCd");
	this.Div01_Div00_radioIssuCd_onitemchanged(this.Div01.form.Div00.form.radioIssuCd, itemInfo);
	
	itemInfo.postvalue = this.ds_detail.getColumn(0, "dcAmtClCd");
	this.Div01_Div04_radioDiscount_onitemchanged(this.Div01.form.Div04.form.radioDiscount, itemInfo);
	
	itemInfo.postvalue = this.ds_detail.getColumn(0, "usePrdClCd");
	this.Div01_Div05_radioPeriod_onitemchanged(this.Div01.form.Div05.form.radioPeriod, itemInfo);
	
	//등록자, 수정자 정보
	var rgprNm = this.gfn_isNull(this.ds_detail.getColumn(0, "rgprNm")) ? "" : "(" + this.ds_detail.getColumn(0, "rgprNm") + ")";
	var rgprInfo = this.ds_detail.getColumn(0, "rgprId") + rgprNm;
	var mdfNm = this.gfn_isNull(this.ds_detail.getColumn(0, "mdfNm")) ? "" : "(" + this.ds_detail.getColumn(0, "mdfNm") + ")";
	var mdfInfo = this.ds_detail.getColumn(0, "mdfId") + mdfNm;
	
	this.Div04.form.Div00.form.Static01.set_text(rgprInfo);
	this.Div04.form.Div02.form.Static01.set_text(mdfInfo);
	
	if(this.parent.type == "copy")
	{
		this.Div01.form.Div05.form.Div00.form.strtDtm.set_value("");
		this.Div01.form.Div05.form.Div00.form.endDtm.set_value("");
		this.ds_detail.setColumn(0, "cpnSeqno", "");
	}
	else
	{
		this.Div00.form.Div10.set_height(33);
		this.Div00.form.resetScroll();
	}
}

/* 지점 체크박스 세팅 */
this.fn_set_spot = function()
{
	var content = ["chkSpotAll"];
	var list = this.application.gdsSpotCdList;
	
	for(var i=0; i < list.getRowCount(); i++) 
	{
		var id = "chkSpot" + i;
		var objComp = new CheckBox();
		objComp.init(id, content[i] + ":10", 9, 133, 20);
		this.Div00.form.Div00.form.Div00.form.addChild(id, objComp);
		objComp.set_text(list.getColumn(i, "cdNm"));
		objComp.set_truevalue(list.getColumn(i, "cd"));
		objComp.set_falsevalue("N");
		objComp.insertEventHandler("onclick", 0, this.fn_checkbox, this);
		objComp.show();	
		content.push(id);
		
		this.Div00.form.Div00.form.Div00.form.resetScroll()
	}
}

/* '전체' 체크박스 이벤트 */
this.fn_checkboxAll = function(pObj) 
{
	var arrObj = pObj.parent.components;
		
	if(pObj.isChecked())
	{		
		for(var i=0; i<arrObj.length; i++)
		{
			if(arrObj[i] == "[object CheckBox]")
			{
				arrObj[i].set_value(arrObj[i].truevalue);
			}
		}
	}
	else 
	{
		for(var i=0; i<arrObj.length; i++)
		{
			if(arrObj[i] == "[object CheckBox]" && arrObj[i].name.indexOf("All") == -1)
			{				
				arrObj[i].set_value(arrObj[i].falsevalue);
			}
		}
	}
}

/* 체크박스 이벤트 */
this.fn_checkbox = function(pObj) 
{
	var arrObj = pObj.parent.components;
		
	if(pObj.isChecked())
	{
		var len = 0;
		var chkLen = 0;
		var chkAll;
		for(var i=0; i<arrObj.length; i++)
		{
			if(arrObj[i] == "[object CheckBox]" && arrObj[i].name.indexOf("All") > -1)
			{				
				chkAll = arrObj[i];
			}
			
			if(arrObj[i] == "[object CheckBox]" && arrObj[i].name.indexOf("All") == -1)
			{
				len++;
				if(arrObj[i].isChecked())
				{
					chkLen++;
				}
			}
		}
		
		if(len == chkLen) 
		{
			chkAll.set_value(true);
		}
	}
	else 
	{
		for(var i=0; i<arrObj.length; i++)
		{
			if(arrObj[i] == "[object CheckBox]" && arrObj[i].name.indexOf("All") > -1)
			{				
				arrObj[i].set_value(false);	
				return;
			}			
		}
	}
}

/* 지점 체크박스 동기화 */
this.fn_sync_checkBox = function()
{
	var list = this.ds_appl_str;
	var spotObj = this.Div00.form.Div00.form.Div00.form.components;
	var len = 0;
	var chkLen = 0;
	var chkAll;
	

	for(var j=0; j<spotObj.length; j++)
	{
		if(spotObj[j] == "[object CheckBox]" && spotObj[j].name.indexOf("All") > -1)
		{				
			chkAll = spotObj[j];
		}
		
		if(spotObj[j] == "[object CheckBox]" && spotObj[j].name.indexOf("All") < 0)
		{		
			len++;
			if(list.findRow("strCd", spotObj[j].truevalue) > -1)
			{
				spotObj[j].set_value(spotObj[j].truevalue);
				chkLen++;
			}
		}	
	}
	
	if(len == chkLen) 
	{
		chkAll.set_value(true);
	}
}

/* 회원등급 체크박스 동기화 */
this.fn_sync_mbrGrde = function()
{
	for(var i=0; i < this.ds_mem_grade.getRowCount(); i++)
	{
		if(this.gfn_nvl(this.ds_mem_grade.getColumn(i, "mbrGrdeCd")) != "")
		{
			this.ds_mem_grade.checkarray[i] = 1;
		}
	}
	this.Div01.form.Div01.form.Grid00.redrawExprCell()
}

/**************************************************************************
 * 발급방식 라디오 버튼 이벤트
 **************************************************************************/

this.Div01_Div00_radioIssuCd_onitemchanged = function(obj:nexacro.Radio,e:nexacro.ItemChangeEventInfo)
{
	var discountItmChngInf = new nexacro.ItemChangeEventInfo();
	
	if(e.postvalue == 'NORMAL')
	{
		
		this.Div01.form.Div02.set_height(0);	//발급 수량
		this.Div01.form.Div01.set_height(270);	//발급 대상
		this.Div01.form.Div05.form.Div01.form.Static00.set_text("발급일로부터");
		this.Div01.form.Div05.form.radioPeriod.set_value("PERIOD");
		this.Div01.form.Div05.form.radioPeriod.set_readonly(false);
	}
	else if(e.postvalue == 'RANDOM')
	{
		this.Grid00.set_visible(false);
		this.Div01.form.Div01.set_height(0);
		this.Div01.form.Div02.set_height(33);
		this.Div01.form.Div05.form.Div01.form.Static00.set_text("발급일로부터");
		this.Div01.form.Div05.form.radioPeriod.set_value("PERIOD");
		this.Div01.form.Div05.form.radioPeriod.set_readonly(true);
	}
	else
	{
		this.Grid00.set_visible(false);
		this.Div01.form.Div01.set_height(0);
		this.Div01.form.Div02.set_height(0);
		this.Div01.form.Div05.form.Div01.form.Static00.set_text("신규가입 일로부터");
		this.Div01.form.Div05.form.radioPeriod.set_value("DEADLINE");
		this.Div01.form.Div05.form.radioPeriod.set_readonly(true);
	}
	
	discountItmChngInf.postvalue = this.Div01.form.Div05.form.radioPeriod.value;
	this.Div01_Div05_radioPeriod_onitemchanged(this.Div01.form.Div05.form.radioPeriod, discountItmChngInf);
		
	this.Div01.set_fittocontents("height");	
	this.Div01.form.Div05.form.Div01.form.resetScroll();
	this.Div01.form.resetScroll();
	this.resetScroll();
