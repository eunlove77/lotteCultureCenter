	this.fn_checkValidation();
};

this.fn_checkValidation = function(){
	//지점 선택여부
	var spotObj = this.Div00.form.Div00.form.Div00.form.components;
	var spotStr = "";
	var spotFlag = false;
	var spotList = this.ds_appl_str
	
	spotList.deleteAll()
	
	for(var i=0; i<spotObj.length; i++)
	{
		if(spotObj[i] == "[object CheckBox]" && spotObj[i].name.indexOf("All") < 0 && spotObj[i].value != "N")
		{		
			var nRowIdx = spotList.addRow();
					
			spotList.setColumn(nRowIdx, "strCd", spotObj[i].value);
			spotFlag = true;
		}	
	}
	if(!spotFlag)
	{
		this.alert("지점을 선택하세요.");
		this.Div00.form.Div00.form.Div00.form.chkSpotAll.setFocus();
		return;
	}
	if(this.gfn_nvl(this.Div00.form.Div01.form.Edit00.value) == "")
	{
		this.alert("쿠폰명을 입력하세요.");
		this.Div00.form.Div01.form.Edit00.setFocus();
		return;
	}
	if(this.Div01.form.Div00.form.radioIssuCd.value == 'NORMAL')
	{
		if(this.Div01.form.Div01.form.radioTrgtMem.value != 'EXCEL')
		{
			//회원등급 선택여부
			var gradeFlag = false;
			
			for(var i=0; i < this.ds_mem_grade.checkarray.length; i++){
				if(this.ds_mem_grade.checkarray[i] == 1)
				{	
					this.ds_mem_grade.setColumn(i, "mbrGrdeCd", this.ds_mem_grade.getColumn(i, "cd"));
					gradeFlag = true;
				}
				else
				{
					this.ds_mem_grade.setColumn(i, "mbrGrdeCd", null);
				}
			}
			
			if(!gradeFlag)
			{
				this.alert("대상 회원등급을 선택하세요.");
				this.Div01.form.Div01.form.Grid00.setFocus();
				return;
			}
		}
		else
		{
			if(this.ds_mem_excel.getRowCount() == 0)
			{
				alert("회원리스트를 첨부하세요.");
				return;
			}
		}
				
	}
	else if(this.Div01.form.Div00.form.radioIssuCd == 'RANDOM' && this.gfn_isNull(this.Div01.form.Div02.form.MaskEdit00.value))
	{
		this.alert("발급 수량을 입력하세요.");
		this.Div01.form.Div02.form.MaskEdit00.setFocus();
		return;
	}
	
	if(this.Div01.form.Div03.form.radioTrgtLect.value == 'CTGR' && this.ds_appl_ctgr.getRowCount() == 0)
	{
		this.alert("대상 카테고리를 선택하세요.");
		this.Div01.form.Div03.form.Div00.form.Grid00.setFocus();
		return;
	}
	else if(this.Div01.form.Div03.form.radioTrgtLect.value == 'CLASS' && this.ds_appl_lect.getRowCount() == 0)
	{
		this.alert("대상 강좌를 선택하세요.");
		this.Div01.form.Div03.form.Div01.form.Grid00.setFocus();
		return;
	}
	
	if(this.gfn_nvl(this.Div01.form.Div04.form.MaskEdit00.value) == "")
	{
		this.alert("할인 금액(율)을 입력하세요.");
		this.Div01.form.Div04.form.MaskEdit00.setFocus();
		return;
	}
	
	if(this.Div01.form.Div05.form.radioPeriod.value == 'PERIOD')
	{
		var strtDt = this.Div01.form.Div05.form.Div00.form.strtDtm.value;
		var endDt = this.Div01.form.Div05.form.Div00.form.endDtm.value;
		
		if(this.gfn_nvl(strtDt) == "")
		{
			this.alert("사용 시작일을 입력하세요.");
			this.Div01.form.Div05.form.Div00.form.strtDtm.setFocus();
			return;
		}
		else if(this.gfn_nvl(endDt) == "")
		{
			this.alert("사용 종료일을 입력하세요.");
			this.Div01.form.Div05.form.Div00.form.endDtm.setFocus();
			return;
		}
		else if(strtDt > endDt)
		{
			alert("사용 종료일은 사용 시작일 이후로 설정하세요.");
			return;
		}
	}
	else if(this.gfn_nvl(this.Div01.form.Div05.form.Div01.form.Edit00.value) == "")
	{
		this.alert("사용 가능일을 입력하세요.");
		this.Div01.form.Div05.form.Div01.form.Edit00.setFocus();
		return;
	}

	this.fn_save();
}

this.fn_save = function(){
	var tranObj = {
		strSvcId : "save",
		strSvcUrl : this.baseSvrUrl + "insert.do",
		inData : "infoData=ds_detail applStrData=ds_appl_str mbrGrdeData=ds_mem_grade mbrExclData=ds_mem_excel applCtgrData=ds_appl_ctgr applLectData=ds_appl_lect",
		outData : "",
		strArg : "",
		callBackFnc : "fnCallback",
		menuSeq : this.menuId,
		isAsync : true,
		isPopup : true
	}
	if(this.gfn_btn_auth(this.menuId, "accSaveYn")){return;}
	this.gfn_transaction(tranObj);
}

/**************************************************************************
 * 콜백
 **************************************************************************/

this.fnCallback = function(svcID, errorCode, errorMsg)
{
	// 에러 시 화면 처리 내역
	if(errorCode != 0)
	{
		this.alert(errorCode+"\n"+errorMsg);
		return;
	}
	
	switch(svcID)
	{
		case "detail":    
			this.fn_set_detail();
			break;
		case "checkMbr":
			this.fn_set_mbr_list();
			break;
		case "save":
			this.alert("저장되었습니다.");
			this.opener.components[0].form.fn_list();
			this.close();
			break;
	}

};]]></Script>
