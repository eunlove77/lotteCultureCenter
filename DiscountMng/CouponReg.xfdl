    <Script type="xscript5.1"><![CDATA[/**************************************************************************
 * include 영역 
 **************************************************************************/
include "Base::libCommon.xjs";
include "Base::popup.xjs";

/**************************************************************************
 * FORM 변수 선언 영역
 **************************************************************************/
this.application = null;
this.baseSvrUrl = "";
this.menuId = "";
this.popCnt = 0;

this.rsltMsg = ""

/**************************************************************************
 * FORM EVENT 영역(onload)
 **************************************************************************/

this.CouponReg_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	this.baseSvrUrl = this.application.Admin_svc + "::discount/coupon/";
	this.domain = this.application.Domain;
	
	if(this.parent.menuId != "")
	{
		this.menuId = this.parent.menuId;
	}
	else
	{
		this.menuId = this.parent.parent.param.MENU_ID;
	}
	
	//지점선택 체크박스 세팅
	this.fn_set_spot();	

	var tranObj = {
		strSvcId : "codeList",
		strSvcUrl : this.baseSvrUrl + "codeList.do",
		inData : "",
		outData : "ds_category01=ctgrList ds_mem_grade=mbrGrdeList",
		strArg : "ctegryId=CTEGRY ctegryLvlNo=0",
		callBackFnc : "fnCallback",
		menuSeq : this.menuId,
		isAsync : true,
		isPopup : true
	}
	
	this.gfn_transaction(tranObj);
	
	//발급대기 상태인 경우
	if(this.parent.cpnSeqno != ""){	
		this.ds_detail.setColumn(0, "cpnSeqno", this.parent.cpnSeqno);
		this.txtMenuNm.set_text("쿠폰 상세");
		this.fn_detail();
		this.Div04.set_height(95);
		this.resetScroll();
	}
		
	this.gfnSetCheckBoxNoDataset(this.ds_mem_grade, this.Div01.form.Div01.form.Grid00, 0); 
	this.Div01.form.Div05.form.Div00.form.strtDtm.calendaredit.set_readonly(true);	//캘린더 키인 막기
	this.Div01.form.Div05.form.Div00.form.endDtm.calendaredit.set_readonly(true);	//캘린더 키인 막기
	
	//파일 다운로드경로
	this.FileDownTransfer00.set_url(this.domain + "/admin/file/filedownload.do");
};

