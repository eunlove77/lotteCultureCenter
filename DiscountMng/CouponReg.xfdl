this.fn_sync_checkBox = function()
{
	var brchCdArr = this.ds_detail.getColumn(0, "cpnUsePsblBrch").split("|");
	var brchObj = this.Div00.form.Div00.form.Div00.form.components;
	
	for(var i=0; i<brchCdArr.length; i++)
	{
		for(var j=0; j<brchObj.length; j++)
		{
			if(brchObj[j] == "[object CheckBox]" && brchObj[j].name.indexOf("All") < 0)
