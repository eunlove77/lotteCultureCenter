    <Script type="xscript5.1"><![CDATA[/**************************************************************************
 * include 영역 
 **************************************************************************/
include "Base::libCommon.xjs";
include "Base::lib_date.xjs";
include "Base::popup.xjs";

/**************************************************************************
 * FORM 변수 선언 영역
 **************************************************************************/
this.application = null;
this.baseSvrUrl = "";
this.popCnt = 0;

/**************************************************************************
 * FORM EVENT 영역(onload)
 **************************************************************************/
/**
 * @description 화면 onload시 처리내역(필수)
*/

this.CouponList_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	this.txtMenuNm.set_text(obj.parent.parent.fvMenuNM);	
	this.baseSvrUrl = this.application.Admin_svc + "::discount/coupon/";
	
	this.fn_list(1);
	
	this.gfnSetCheckBoxNoDataset(this.ds_coupon_list, this.Grid00, 0); 
};

this.insBtn_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.parent.parent.parent.param.TAB_CLOSE = false;
	this.popCnt ++;
	
	var oArg = {
				cpnSeqno : ""
				};
	
	this.gfn_openPopup("couponDtlPop"+this.popCnt, oArg, "DiscountMng::CouponReg.xfdl", this, 600, 350, true, true, null, "쿠폰 등록");
};

/**************************************************************************
 * 사용자 FUNCTION 영역
 **************************************************************************/

this.fn_list = function (page)
{	
	var tranObj = {
		strSvcId : "list",
		strSvcUrl : this.baseSvrUrl + "list.do",
		inData : "search=ds_search",
		outData : "ds_coupon_list=couponList",
		strArg : "",
		callBackFnc : "fnCallback",
		menuSeq : this.getOwnerFrame().param.MENU_ID,
		isAsync : true
	}
	
	this.gfn_transaction(tranObj);	
};

this.fnCallback = function(svcID, errorCode, errorMsg)
{
	// 에러 시 화면 처리 내역
	if(errorCode != 0)
	{
		this.alert(errorCode+"\n"+errorMsg);
	}
	
	switch(svcID)
	{
		case "list":
			this.txtTotal.set_text("목록 (" + this.gfn_setComma(this.ds_coupon_list.getCount()) + "개)")
			break;
		case "save":
			this.alert("저장되었습니다.");
			this.fn_list();
			break;
		case "detail":    
						
			break;
	}

};

this.Grid00_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	if(e.cell == 2){	
		this.popCnt ++;
		var oArg = {
				type : "detail",
				cpnSeqno : obj.getBindDataset().getColumn(e.row, "cpnSeqno")
				};
	
		if(obj.getBindDataset().getColumn(e.row, "issueStatCd") == "00")
		{
			this.gfn_openPopup("couponDtlPop"+this.popCnt, oArg, "DiscountMng::CouponReg.xfdl", this, 600, 350, true, true, null, "쿠폰 상세");
		}
		else
		{
			this.gfn_openPopup("couponDtlPop"+this.popCnt, oArg, "DiscountMng::CouponDtl.xfdl", this, 600, 350, true, true, null, "쿠폰 상세");
		}		
	}
};

this.Div01_Div00_04_btnSearch_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.fn_list(1);
};

this.Div01_Div00_04_btnInit_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var cnt = this.ds_search.getColCount();
	for(var i=0; i<cnt; i++)
	{
		this.ds_search.setColumn(0, i, "");
	}
	this.fn_list(1);
};

this.copyBtn_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var list = this.ds_coupon_list;
	var chkCnt = 0;
	var chkRow = -1;
	
	for(var i=0; i<list.getRowCount(); i++)
	{
		if(list.checkarray[i] == 1)
		{
			chkCnt ++;
			chkRow = i;
		}
	}
	
	if(chkCnt == 0)
	{
		alert("항목을 선택하세요.");
	}
	else if(chkCnt > 1)
	{
		alert("1개만 선택하세요.");
	}
	else
	{
		this.popCnt ++;
		var oArg = {
				cpnSeqno : list.getColumn(chkRow, "cpnSeqno")
				};
	
		this.gfn_openPopup("couponDtlPop"+this.popCnt, oArg, "DiscountMng::CouponReg.xfdl", this, 600, 350, true, true, null, "쿠폰 복사");
	}
};

this.excelDownBtn_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var list = this.ds_coupon_list;
	var excelList = this.ds_coupon_excel;
	var chkCnt = 0;
	
	excelList.deleteAll();
	
	for(var i=0; i<list.getRowCount(); i++)
	{
		if(list.checkarray[i] == 1)
		{
			//체크행 엑셀데이터로 복사
			excelList.addRow();
			chkCnt++;
		}
	}
	
	if(chkCnt == 0)
	{
		alert("항목을 선택하세요.");
	}
	else
	{
		this.exportObj = new ExcelExportObject("Export00", this);
		
		this.exportObj.set_exporturl(this.application.Domain + this.application.getVariable("excelImportExport"));
		this.exportObj.addExportItem(nexacro.ExportItemTypes.GRID, this.Grid_Excel, "Sheet1!A1", "allband", "allrecord", "merge", "allstyle", "none", "none", "both");
		this.exportObj.set_exportfilename("쿠폰_목록_" + this.gfnGetToDay());

		this.exportObj.exportData();
	}
};
]]></Script>
