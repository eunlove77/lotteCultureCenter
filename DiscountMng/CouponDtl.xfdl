this.Div03_insBtn_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{	
	this.fn_checkValidation();
};

this.fn_checkValidation = function(){

	if( this.gfn_isNull(this.param.seq) &&  this.atchFileDiv.FileUpTransfer00.filelist.length == 0 ){	//파일첨부 필수일 경우
		this.alert("쿠폰이미지를 선택해주세요.");
		this.atchFileDiv.Button00.setFocus();
		return;
	}else{								
		//파일첨부 업로드
		// 수정일 경우
		if(!this.gfn_isNull(this.param.seq)){
			var atchFileList = this.atchFileDiv.FileUpTransfer00.filelist;
			
			if( atchFileList.length > 0 ){
				this.atchFileDiv.FileUpTransfer00.upload(this.application.domain + '/admin/file/fileupload.do?fileGb=atchFileId&atchFileId='+this.atchFileId);
			}
		
		}else{
			this.atchFileDiv.FileUpTransfer00.upload(this.application.domain + '/admin/file/fileupload.do?fileGb=atchFileId&atchFileId='+this.atchFileId);
		}
		
	}	
}

//파일첨부 완료 후 실행할 함수 (atchFileId 처리)
this.onSuccess = function(obj:nexacro.FileUpTransfer,e:nexacro.FileUpTransferEventInfo)
{
	this.atchFileId = e.datasets[0].getColumn(0,"atchFileId");
	this.ds_detail.setColumn(0, "atchFileId", this.atchFileId);
	
	this.fn_save();
};

this.fn_save = function(){
	this.ds_atch_file = this.atchFileDiv.ds_atch_file; //파일첨부 폼 데이터복사
	
	var strSvcId    = "save";
	var strSvcUrl   = this.application.Admin_svc + "::sample/insertSample.do";
	var inData      = "data=ds_detail updateFileData=ds_atch_file:U";
	var outData     = "";
	var strArg      = "";
	var callBackFnc = "fnCallback";
	var isAsync     = true;

	this.transaction(strSvcId ,   // transaction을 구분하기 위한 svc id값
			strSvcUrl ,       // trabsaction을 요청할 주소
			inData ,         // 입력값으로 보낼 dataset id , a=b형태로 실제이름과 입력이름을 매칭
			outData ,         // 처리결과값으로 받을 dataset id, a=b형태로 실제이름과 입력이름을 매칭
			strArg,         // 입력값으로 보낼 arguments, a=b
			callBackFnc,       // transaction의 결과를 받을 Function 이름
			isAsync);         // 비동기통신 여부 [생략가능]
}
]]></Script>
