 **************************************************************************/
/**
 * @description 화면 onload시 처리내역(필수)
*/

this.CouponDtl_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	this.baseSvrUrl = this.application.Admin_svc + "::discount/coupon/";
	this.param = this.parent.param;
	
	var tranObj = {
		strSvcId : "codeList",
		strSvcUrl : this.baseSvrUrl + "codeList.do",
		inData : "",
		outData : "ds_category01=ctgrList ds_spot_list=spotList",
		strArg : "cdId=027",
		callBackFnc : "fnCallback",
		isAsync : false
	}
	
	this.gfn_transaction(tranObj);
	
	if(this.param.seq != ""){	
		this.ds_detail.setColumn(0, "seq", this.param.seq);
		this.fn_detail();
	}
		
	this.gfnSetCheckBoxNoDataset(this.ds_mem_grade, this.Div01.form.Div01.form.Grid00, 0); 
};

this.fn_detail = function ()
{	
	var strSvcId	= "detail"
	var strSvcUrl   = this.baseSvrUrl + "codeList.do";
	var inData      = "detail=ds_detail";
	var outData     = "ds_detail=detail ds_atch_file=atchFileList";
	var strArg      = "";
	var callBackFnc = "fnCallback";
	var isAsync     = true;

	this.transaction(strSvcId ,   // transaction을 구분하기 위한 svc id값
            strSvcUrl ,       // trabsaction을 요청할 주소
            inData ,         // 입력값으로 보낼 dataset id , a=b형태로 실제이름과 입력이름을 매칭
            outData ,         // 처리결과값으로 받을 dataset id, a=b형태로 실제이름과 입력이름을 매칭
            strArg,         // 입력값으로 보낼 arguments, a=b
            callBackFnc,       // transaction의 결과를 받을 Function 이름
            isAsync);         // 비동기통신 여부 [생략가능]
};

this.fnCallback = function(svcID, errorCode, errorMsg)
{
	// 에러 시 화면 처리 내역
	if(errorCode != 0)
	{
		this.alert(errorCode+"\n"+errorMsg);
	}
	
	switch(svcID)
	{
		case "codeList":
			this.fn_set_spot();
			
			break;
		case "save":
			this.alert("저장되었습니다.");
			this.fn_list();
			break;
		case "detail":    
						
			break;
	}

};

//발급 방식 라디오 버튼
this.Div01_Div00_radioIssuCd_onitemchanged = function(obj:nexacro.Radio,e:nexacro.ItemChangeEventInfo)
{
	var discountItmChngInf = new nexacro.ItemChangeEventInfo();
	
	if(e.postvalue == 'NORMAL')
	{
		
		this.Div01.form.Div02.set_height(0);	//발급 수량
		this.Div01.form.Div01.set_height(270);	//발급 대상
		this.Div01.form.Div05.form.Div01.form.Static00.set_text("발급일로부터");
		this.Div01.form.Div05.form.radioPeriod.set_value("PERIOD");
		this.Div01.form.Div05.form.radioPeriod.set_readonly(false);
	}
	else if(e.postvalue == 'RANDOM')
	{
		this.Div01.form.Div01.set_height(0);
		this.Div01.form.Div02.set_height(33);
		this.Div01.form.Div05.form.Div01.form.Static00.set_text("발급일로부터");
		this.Div01.form.Div05.form.radioPeriod.set_value("PERIOD");
		this.Div01.form.Div05.form.radioPeriod.set_readonly(true);
	}
	else
	{
		this.Div01.form.Div01.set_height(0);
		this.Div01.form.Div02.set_height(0);
		this.Div01.form.Div05.form.Div01.form.Static00.set_text("신규가입 일로부터");
		this.Div01.form.Div05.form.radioPeriod.set_value("DEADLINE");
		this.Div01.form.Div05.form.radioPeriod.set_readonly(true);
	}
	
	discountItmChngInf.postvalue = this.Div01.form.Div05.form.radioPeriod.value;
	this.Div01_Div05_radioPeriod_onitemchanged(this.Div01.form.Div05.form.radioPeriod, discountItmChngInf);
		
	this.Div01.set_fittocontents("height");	
	this.Div01.form.Div05.form.Div01.form.resetScroll();
	this.Div01.form.resetScroll();
	this.resetScroll();
};

//할인 금액 라디오 버튼
