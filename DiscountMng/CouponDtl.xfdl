};

this.fn_checkValidation = function(){
	//지점 선택여부
	var spotObj = this.Div00.form.Div00.form.Div00.form.components;
	var spotStr = "";
	var spotFlag = false;
	
	for(var i=0; i<spotObj.length; i++)
	{
		if(spotObj[i] == "[object CheckBox]" && spotObj[i].name.indexOf("All") < 0 && spotObj[i].value != "N")
		{		
			spotFlag = true;
			spotStr += spotObj[i].value + "|";
		}	
	}
	
	if(!spotFlag)
	{
		this.alert("지점을 선택하세요.");
		this.Div00.form.Div00.form.Div00.form.chkSpotAll.setFocus();
		return;
	}
	if(this.gfn_isNull(this.Div00.form.Div01.form.Edit00.value))
	{
		this.alert("쿠폰명을 입력하세요.");
		this.Div00.form.Div01.form.Edit00.setFocus();
		return;
	}
	if(this.Div01.form.Div00.form.radioIssuCd == 'NORMAL' && this.Div01.form.Div01.form.radioTrgtMem.value == 'GRADE')
	{
		//회원등급 선택여부
		var gradeFlag = false;
		
		for(var i=0; i < this.ds_mem_grade.checkarray.length; i++){
			if(this.ds_mem_grade.checkarray[i] == 1)
			{
				this.ds_mem_grade.setColumn(i, "chk", 1);
				gradeFlag = true;
			}
		}
		
		if(!gradeFlag)
		{
			this.alert("대상 회원등급을 선택하세요.");
			this.Div01.form.Div01.form.Grid00.setFocus();
			return;
		}
	}
	else if(this.Div01.form.Div00.form.radioIssuCd == 'RANDOM' && this.gfn_isNull(this.Div01.form.Div02.form.MaskEdit00.value))
	{
		this.alert("발급 수량을 입력하세요.");
		this.Div01.form.Div02.form.MaskEdit00.setFocus();
		return;
	}
	
	if(this.Div01.form.Div03.form.radioTrgtLect.value == 'CTGR' && this.ds_appl_ctgr.getRowCount() == 0)
	{
		this.alert("대상 카테고리를 선택하세요.");
		this.Div01.form.Div03.form.Grid00.setFocus();
		return;
	}
	
	if(this.gfn_isNull(this.Div01.form.Div04.form.MaskEdit00.value))
	{
		this.alert("할인 금액(율)을 입력하세요.");
		this.Div01.form.Div04.form.MaskEdit00.setFocus();
		return;
	}
	
	if(this.Div01.form.Div05.form.radioPeriod.value == 'PERIOD')
	{
		if(this.gfn_isNull(this.Div01.form.Div05.form.Div00.form.strtDtm.value))
		{
			this.alert("사용 시작일을 입력하세요.");
			this.Div01.form.Div05.form.Div00.form.strtDtm.dropdown();
			return;
		}
		else if(this.gfn_isNull(this.Div01.form.Div05.form.Div00.form.endDtm.value))
		{
			this.alert("사용 종료일을 입력하세요.");
			this.Div01.form.Div05.form.Div00.form.strtDtm.dropdown();
			return;
		}
	}
	else if(this.gfn_isNull(this.Div01.form.Div05.form.Div01.form.Edit00.value))
	{
		this.alert("사용 가능일을 입력하세요.");
		this.Div01.form.Div05.form.Div01.form.Edit00.setFocus();
		return;
	}
	else
	{
		this.ds_detail.setColumn(0, "cpnUsePsblBrch", spotStr);
		this.fn_save();
	}

}


this.fn_save = function(){
	
	var strSvcId    = "save";
	var strSvcUrl   = this.baseSvrUrl + "insert.do";
	var inData      = "infoData=ds_detail applCtgrData=ds_appl_ctgr mbrGrdeData=ds_mem_grade";
	var outData     = "";
	var strArg      = "";
	var callBackFnc = "fnCallback";
	var isAsync     = true;

	this.transaction(strSvcId ,   // transaction을 구분하기 위한 svc id값
			strSvcUrl ,       // trabsaction을 요청할 주소
			inData ,         // 입력값으로 보낼 dataset id , a=b형태로 실제이름과 입력이름을 매칭
			outData ,         // 처리결과값으로 받을 dataset id, a=b형태로 실제이름과 입력이름을 매칭
			strArg,         // 입력값으로 보낼 arguments, a=b
			callBackFnc,       // transaction의 결과를 받을 Function 이름
			isAsync);         // 비동기통신 여부 [생략가능]
}

//지점 체크박스 세팅
this.fn_set_spot = function()
{
	var content = ["chkSpotAll"];
	var list = this.ds_spot_list;
	
	for(var i=0; i < list.getRowCount(); i++) 
	{
		var id = "chkSpot" + i;
		var objComp = new CheckBox();
		objComp.init(id, content[i] + ":10", 9, 130, 20);
		this.Div00.form.Div00.form.Div00.form.addChild(id, objComp);
		objComp.set_text("["+list.getColumn(i, "cd")+"]"+list.getColumn(i, "cdNm"));		
		objComp.set_truevalue(list.getColumn(i, "cd"));
		objComp.set_falsevalue("N");
		objComp.show();	
		content.push(id);
		
		this.Div00.form.Div00.form.Div00.form.resetScroll()
	}
}

