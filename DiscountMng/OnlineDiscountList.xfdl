this.Div00_Div06_Calendar00_oncloseup = function(obj:nexacro.Calendar,e:nexacro.CalendarCloseUpEventInfo)
{
	var endDtm = this.Div00.form.Div06.form.Calendar01.value;
	
	if(endDtm && obj.value > endDtm)
	{
		obj.set_value(endDtm);
	}
};

this.Div00_Div06_Calendar01_oncloseup = function(obj:nexacro.Calendar,e:nexacro.CalendarCloseUpEventInfo)
{
	var strtDtm = this.Div00.form.Div06.form.Calendar00.value;
	
	if(strtDtm > obj.value)
	{
		this.Div00.form.Div06.form.Calendar00.set_value(obj.value);
	}
};

this.Div01_Grid00_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	if(e.col == 5 || e.col == 6)
	{
		var arrArguments = {
			brchCd : this.ds_class_list.getColumn(e.row, "mstBrchCd"),
			yy : this.ds_class_list.getColumn(e.row, "yy"),
			lectSmsterCd : this.ds_class_list.getColumn(e.row, "lectSmsterCd"),
			lectCd : this.ds_class_list.getColumn(e.row, "mstLectCd"),
			menuSeq : this.getOwnerFrame().param.MENU_ID,
			searchButton : this.Div00.form.Div09.form.btnSearch
		}
		
		this.popCnt++;
		this.gfn_openPopup("ClassDtl" + this.popCnt, arrArguments, "ClassMng::ClassDtl.xfdl", this, null, null, true, true, null);
	}
	
	if(e.col == 7)
	{
		this.popCnt++;
		var oArg = {
				type : "detail",
				tcCdNo : this.ds_class_list.getColumn(e.row, "tcCdNo"),
				menuSeq : this.getOwnerFrame().param.MENU_ID
		};
		this.gfn_openPopup("TeacherDtlPop"+this.popCnt, oArg, "TeacherMng::TeacherDtl.xfdl", this, null, null, true, true, null, "강사 상세");
	}
};

/**************************************************************************
 * 할인 일괄설정
 **************************************************************************/
this.Div01_Button00_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var list = this.ds_class_list;
	var copyList = this.ds_class_chk;
	var chkCnt = 0;
	
	copyList.deleteAll();
	
	for(var i=0; i<list.getRowCount(); i++)
	{
		if(list.checkarray[i] == 1)
		{
			//체크행 복사
			copyList.addRow();
			copyList.copyRow(chkCnt, list, i);
			chkCnt++;
		}
	}
	
	if(chkCnt == 0)
	{
		alert("항목을 선택하세요.");
	}
	else
	{
		var oArg = { 
					  chkCnt : chkCnt 
					 ,cksDs : copyList
					};
		this.popCnt++;
		this.gfn_openPopup("onlineDiscountPop"+this.popCnt, oArg, "DiscountMng::OnlineDiscountPop.xfdl", this, null, null, true, true, null);
	}
};


