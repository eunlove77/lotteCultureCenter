 * 콜백
 **************************************************************************/
this.fnCallback = function(svcID, errorCode, errorMsg)
{
  // 에러 시 화면 처리 내역
  if(errorCode != 0)
  {
    this.alert(errorCode+"\n"+errorMsg);
  }

  switch(svcID)
  {
    case "codeList":
		this.fn_set_chkbox(this.ds_classType_list, this.Div00.form.Div02, "chkType");	//강좌유형 추가
		this.fn_set_chkbox(this.ds_classState_list,this.Div00.form.Div05, "chkState");	//강좌상태 추가
		this.fn_set_chkbox(this.ds_classGb_list, this.Div00.form.Div04, "chkGb");		//강좌구분 추가
		this.fn_set_chkbox(this.ds_mbrGrade_list,this.Div00.form.Div08.form.Div00, "chkMbrGrde");	//회원등급 추가
		this.fn_sesterInfo();
		this.fn_year_add();
		this.fn_semester_set();
		this.fn_add_grid_column();
		this.fn_classCtgy_set();
	break;
	
	case "list":
		this.Div01.form.Static01.set_text('(' + this.ds_class_list.getRowCount() + '개)');
	break;

  }
};

this.fn_searchEnter = function(obj:nexacro.Edit,e:nexacro.KeyEventInfo)
{
	if(e.keycode == 13)
	{
		obj.updateToDataset();
		this.fn_list();
	}
};
