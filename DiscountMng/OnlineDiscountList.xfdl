//전체 체크박스 이벤트
this.fn_checkboxAll = function(pObj) 
{
	var arrObj = pObj.parent.components;
		
	if(pObj.isChecked())
	{		
		for(var i=0; i<arrObj.length; i++)
		{
			if(arrObj[i] == "[object CheckBox]")
			{
				arrObj[i].set_value(arrObj[i].truevalue);
			}
		}
	}
	else 
	{
		for(var i=0; i<arrObj.length; i++)
		{
			if(arrObj[i] == "[object CheckBox]" && arrObj[i].name.indexOf("All") == -1)
			{				
				arrObj[i].set_value(arrObj[i].falsevalue);
			}
			
		}
	}
	
}

//개별 체크박스 이벤트
this.fn_checkbox = function(pObj) 
{
	var arrObj = pObj.parent.components;
		
	if(pObj.isChecked())
	{
		var len = 0;
		var chkLen = 0;
		var chkAll;
		for(var i=0; i<arrObj.length; i++)
		{
			if(arrObj[i] == "[object CheckBox]" && arrObj[i].name.indexOf("All") > -1)
			{				
				chkAll = arrObj[i];
			}
			
			if(arrObj[i] == "[object CheckBox]" && arrObj[i].name.indexOf("All") == -1)
			{
				len++;
				if(arrObj[i].isChecked())
				{
					chkLen++;
				}
			}
		}
		
		if(len == chkLen) 
		{
			chkAll.set_value(true);
		}
	}
	else 
	{
		for(var i=0; i<arrObj.length; i++)
		{
			if(arrObj[i] == "[object CheckBox]" && arrObj[i].name.indexOf("All") > -1)
			{				
				arrObj[i].set_value(false);	
				return;
			}			
		}
	}
}


/**************************************************************************
 * 리스트 관련
 **************************************************************************/
this.fn_list = function ()
{	
	if(this.gfn_btn_auth(this.getOwnerFrame().param.MENU_ID, "accSrchYn")){return;}
	
	//체크된 코드 처리
	var typeObj = this.Div00.form.Div02.form.components;
	var clObj = this.Div00.form.Div04.form.components;
	var statObj = this.Div00.form.Div05.form.components;
	var gradeObj = this.Div00.form.Div08.form.components;
	var gradeStr = "";
	var statStr = "";
	var clStr = "";
	var typeStr = "";
	
	for(var i=0; i<typeObj.length; i++)
	{
		if(typeObj[i] == "[object CheckBox]" && typeObj[i].name.indexOf("All") < 0 && typeObj[i].value != "false")
		{		
			typeStr += typeObj[i].value + ",";
		}	
	}
	for(var i=0; i<clObj.length; i++)
	{
		if(clObj[i] == "[object CheckBox]" && clObj[i].name.indexOf("All") < 0 && clObj[i].value != "false")
		{		
			clStr += clObj[i].value + ",";
		}	
	}
	for(var i=0; i<statObj.length; i++)
	{
		if(statObj[i] == "[object CheckBox]" && statObj[i].name.indexOf("All") < 0 && statObj[i].value != "false")
		{		
			statStr += statObj[i].value + ",";
		}	
	}
	for(var i=0; i<gradeObj.length; i++)
	{
		if(gradeObj[i] == "[object CheckBox]" && gradeObj[i].name.indexOf("All") < 0 && gradeObj[i].value != "false")
		{		
			gradeStr += gradeObj[i].value + ",";
		}	
	}
	
	this.ds_search.setColumn(0, "lectTpCd", typeStr);
	this.ds_search.setColumn(0, "lectClCd", clStr);
	this.ds_search.setColumn(0, "lectStatCd", statStr);
	this.ds_search.setColumn(0, "dcGradeCd", gradeStr);

	var tranObj = {
		strSvcId : "list",
		strSvcUrl : this.baseSvrUrl + "list.do",
		inData : "search=ds_search",
		outData : "ds_class_list=list",
		strArg : "",
		callBackFnc : "fnCallback",
		menuSeq : this.getOwnerFrame().param.MENU_ID,
		isAsync : true
	}
	
	this.gfn_transaction(tranObj);	
};

this.Div00_Div09_btnInit_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var cnt = this.ds_search.getColCount();
	for(var i=0; i<cnt; i++)
	{
		this.ds_search.setColumn(0, i, "");
	}
	
	this.fn_init(this.Div00.form);
	this.ds_class_list.deleteAll();
	
	this.fn_sesterInfo();
	this.fn_year_add();
	this.fn_semester_set();
};

//필터 초기화
this.fn_init = function(obj)
{
	var arrObj = obj.components;
	
	for(var i=0; i<arrObj.length; i++)
	{
		if(arrObj[i] == "[object Div]"){
			this.fn_init(arrObj[i].form);
		}
		else if(arrObj[i] == "[object Combo]"){
			arrObj[i].set_index(0);
		}
		else if(arrObj[i] == "[object CheckBox]"){
			arrObj[i].set_value(false);
		}
	}
}

this.Div00_Div09_btnSearch_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if(this.gfn_btn_auth(this.getOwnerFrame().param.MENU_ID, "accSrchYn")){return;}
	
	if(nexacro.trim(new String(this.gfn_nvl(this.ds_search.getColumn(0, "lectStDtm")))) != "" || nexacro.trim(new String(this.gfn_nvl(this.ds_search.getColumn(0, "lectEndDtm")))) != "")
	{
		if(nexacro.trim(new String(this.gfn_nvl(this.ds_search.getColumn(0, "lectStDtm")))) == "")
		{
			alert("강의기간 시작일을 선택하세요.");
			this.Div00.form.Div06.form.Calendar00.setFocus();
			return;
		}
		
		if(nexacro.trim(new String(this.gfn_nvl(this.ds_search.getColumn(0, "lectEndDtm")))) == "")
		{
			alert("강의기간 종료일을 선택하세요.");
			this.Div00.form.Div06.form.Calendar01.setFocus();
			return;
		}
	}
	
	this.fn_list();
};

