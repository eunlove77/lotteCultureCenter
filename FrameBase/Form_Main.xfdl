    <Script type="xscript5.1"><![CDATA[/**************************************************************************
 * include 영역 
 **************************************************************************/
include "Base::libCommon.xjs";
include "Base::lib_base.xjs";
include "Base::lib_date.xjs";
include "Base::popup.xjs";
/**************************************************************************
 * FORM 변수 선언 영역
 **************************************************************************/
this.application = null;
this.curCalendar = new Date();
this.selectedDate = null;
this.selectedKey = null;
this.grdSchedule = this.Div00.form.grdSchedule;
this.cur_brchCd = null;
/**************************************************************************
 * FORM EVENT 영역(onload)
 **************************************************************************/
this.Form_Main_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	
	this.set_default_data();
	
	this.get_schedule_list();	
};

this.set_default_data = function(){
	var default_val_spot = this.application.gdsSpotCdList.getColumn(0, 'cd');
	this.cur_brchCd = default_val_spot;
	this.Div00.form.Combo00.set_value(default_val_spot);
	
	this.ds_search.setColumn(0, "brchCd", this.cur_brchCd);
}
//지점 변경
this.Div00_Combo00_onitemchanged = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
{
	this.cur_brchCd = obj.value;
	this.ds_search.setColumn(0, "brchCd", this.cur_brchCd);
	
	this.get_schedule_list();
};

// 캘린더 세팅
this.set_calendar = function(){

	this.Div00.form.monthTitle.set_text(this.gfnGetMaskFormatDateToString(this.curCalendar, 'yyyy년 MM월'));
	this.clear_calendar();
	this.set_calendar_daynumber();
	this.set_plan_on_calendar();
};
// 달력 월 이동
this.btnPrev_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.curCalendar = this.gfnAddMonth(this.curCalendar, -1);
	this.set_calendar();
};
this.btnNext_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.curCalendar = this.gfnAddMonth(this.curCalendar, 1);
	this.set_calendar();
};

// 캘린더 clear
this.clear_calendar = function(){
	var c_idx = 0;
	for(var i = 1, j = 0, k = 1; i <= 42; j++){
		c_idx = i * 7 + j;
		this.grdSchedule.setCellProperty('head', c_idx, 'text', '');
		if(j == 6){
			j = -1;
			i++;
		}
	}
}
// 캘린더 날짜 세팅
this.set_calendar_daynumber = function(){
	var first_day = this.gfnGetFirstDate(this.curCalendar).getDay();
	var last_dayNum = this.gfnGetLastDayOfMonth(this.curCalendar);
	var dayNum = 1;
	for(var i = 1, j = first_day; i <= 36 && dayNum <= last_dayNum; j++){
		c_idx = i * 7 + j;
		this.grdSchedule.setCellProperty('head', c_idx, 'text', dayNum++);
		if(j == 6){
			j = -1;
			i += 5;
		}
	}
}
// 캘린더에 일정 세팅
this.set_plan_on_calendar = function(){
	
	//ds filtering
	var nxtMon = this.gfnAddMonth(this.curCalendar, 1);
	var nxtMonFormat = this.gfnGetMaskFormatDateToString(nxtMon, 'yyyyMM') + '01';
	var curMonFormat = this.gfnGetMaskFormatDateToString(this.curCalendar, 'yyyyMM') + '01'
	this.ds_schd_list.filter('schdDt.replace(/-/g, "") >= '+ curMonFormat +' && schdDt.replace(/-/g, "") < ' + nxtMonFormat);
	
	var sdate, title, week, day, c_idx;
	var rowcount = this.ds_schd_list.rowcount;
	for(var i = 0; i < rowcount; i++){
		sdate = this.ds_schd_list.getColumn(i, "schdDt").replace(/-/g, "");
		week = this.get_week_number(sdate) - 1;
		day = new Date(this.string_date_formater(sdate)).getDay();
		
		//한 주에 35개 셀 존재
		c_idx = (35 * week + 14) + day; // +14 index 부터 일정 셀
		for(var j = c_idx, k = 0; k < 4; j += 7, k++){
			if(!this.grdSchedule.getCellText(-1, j)){
				c_idx = j;
				break;
			}
		}
		if(k == 4) c_idx = -1;
		
		if(c_idx > -1){
			var title = this.ds_schd_list.getColumn(i, "schdTitNm");
			this.grdSchedule.setCellProperty('head', c_idx, 'text', title); //그리드 셋
		}
	}
}
// 일정 관리
this.grdSchedule_onheadclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	// ignore
	if(e.cell < 7) return false;
	
	var cell_txt = this.grdSchedule.getCellText(-1, e.cell);
	var dayNum = this.get_daynum_from_cellindex(e.cell);

	// ignore
	if(!dayNum) return false;
	
	this.add_plan(dayNum);
};
this.Div00_Button13_00_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.add_plan();
};
this.add_plan = function(dayNum){
	this.selectedDate = dayNum;
	var oArg = {title : "일정 관리"};
	this.gfn_showModal("popupModal", oArg, "SystemMng::SchdMngDtl_popup.xfdl", "popupCallBack", this, 850, 450, true, false, null);
}
this.show_plan = function(key){
	this.selectedDate = null;
	this.selectedKey = key;
	var oArg = {title : "일정 관리"};
	this.gfn_showModal("popupModal", oArg, "SystemMng::SchdMngDtl_popup.xfdl", "popupCallBack", this, 850, 450, true, false, null);
}
this.popupCallBack = function(){
	this.get_schedule_list();
}

/**************************************************************************
 * UTIL FUNCTION
 **************************************************************************/
this.string_date_formater = function(date){
	return date.slice(0, 4) + '-' + date.slice(4, 6) + '-' + date.slice(6, 8);
}
this.get_daynum_from_cellindex = function(cell_idx){
	//한 주에 35개 셀 존재
	var week = parseInt((cell_idx - 7) / 35);	// +7번째 셀부터 날짜 셀
	var trgt_daynum_cell_index = (35 * week + 7) + (cell_idx % 7);
	var day = this.grdSchedule.getCellText(-1, trgt_daynum_cell_index);
	
	return day ? this.gfnGetMaskFormatDateToString(this.curCalendar, 'yyyy-MM-') + ('0' + day).slice(-2) : '';
}
this.get_week_number = function(in_date){
	var date_format =  this.string_date_formater(in_date);
	var date = new Date(date_format);
	// 해당 날짜 (일)
	const currentDate = date.getDate();
	// 이번 달 1일로 지정
	const startOfMonth = new Date(date.setDate(1));
	// 이번 달 1일이 무슨 요일인지 확인
	const weekDay = startOfMonth.getDay(); // 0: Sun ~ 6: Sat
	// ((요일 - 1) + 해당 날짜) / 7일로 나누기 = N 주차
	return parseInt(((weekDay - 1) + currentDate) / 7) + 1;
};

this.get_schedule_list = function(){
	var tranObj = {};
	tranObj.strSvcId	= "select_schedule";
	tranObj.strSvcUrl   = this.application.Admin_svc + "::scheduleMng/list.do";
	tranObj.inData      = "search=ds_search";
	tranObj.outData     = "ds_schd_list=output1";
	tranObj.strArg      = "";
	tranObj.callBackFnc = "fnCallback_schedule_list";
	tranObj.isAsync     = true;
	
	this.gfn_transaction(tranObj);
};
this.fnCallback_schedule_list = function(svcID, errorCode, errorMsg){
	if(errorCode == 0){
		this.set_calendar();
	} else {
		// 에러 시 화면 처리 내역
		this.alert(errorCode+"\n"+errorMsg);
		return;
	}
}]]></Script>
