//컨텐츠 초기화	
this.contentInit = function(type)
{
	if(type == "1") 
	{
		this.Div00.form.Div00_00_00_00_01_00.form.Div00.form.Div01.form.contentArea0.form.Button00.set_visible(false);
	}
	else
	{
		this.Div00.form.Div00_00_00_00_01_00.form.Div00.form.Div01.form.contentArea0.form.Button00.set_visible(true);
	}
	
	//컨텐츠 초기화	
	if(this.contentArr.length > 0)
	{
		while(this.contentArr.length > 1)
		{
			this.fn_content_remove(this.contentArr[this.contentArr.length-1].divObj);
		}
		
		this.contentArr[0].contentType.set_value(type);
		this.contentArr[0].content.set_value("");
		this.fn_content_chg(this.contentArr[0].contentType.parent.parent.parent, type)
	} 
	else 
	{
		var contentObj = {
			divObj : this.Div00.form.Div00_00_00_00_01_00.form.Div00.form.Div01.form.contentArea0,
			contentType : this.Div00.form.Div00_00_00_00_01_00.form.Div00.form.Div01.form.contentArea0.form.Div00.form.Radio00,
			content : this.Div00.form.Div00_00_00_00_01_00.form.Div00.form.Div01.form.contentArea0.form.templateArea0.form.TextArea00,
			ord : 0
		}
		this.contentArr.push(contentObj);
		this.contentArr[0].contentType.set_value(type);
		this.contentArr[0].content.set_value("");
	}
	this.ds_content_list.clearData();
	this.ds_content_list.addRow();
	
	//첨부파일
	this.atchfileArr = [];
	this.atchfileArr.push(this.Div00.form.Div00_00_00_00_01_00.form.Div00.form.Div01.form.contentArea0.form.templateArea0.form.Div00.form);
	if(this.atchfileArr[0]._url == '')
	{
		this.atchfileArr[0].set_url("FrameBase::include/file_upload.xfdl");
	}
	var atchFileId = this.ds_content_list.getColumn(0, "imgFileId");
	var atchFileObj = {
		_this : this,
		atchFileId : atchFileId,
		fileExtn : "jpg,jpeg,gif,png",
