this.ds_teacher_list_onvaluechanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if(this.ds_teacher_list.getColumn(e.row, "tcTpCd") != undefined && this.ds_teacher_list.getColumn(e.row, "tcTpCd") != 1)
	{
