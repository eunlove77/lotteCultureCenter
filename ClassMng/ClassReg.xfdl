**************************************************************************/
this.fnCallback = function(svcID, errorCode, errorMsg)
{
  this.saveFlag = false;
  // 에러 시 화면 처리 내역
  if(errorCode != 0)
  {
    this.alert(errorCode+"\n"+errorMsg);
  }
  else 
  {
	 switch(svcID)
	  {
		case "codeList":
			this.fn_classCtgy_set();
			this.fn_classType_set();
			this.fn_classGb_set();
			this.fn_classTargetGb_set();
			this.fn_classTime_set();
			this.fn_classOptType_set();
			this.fn_classDispIcon_set();
			this.fn_classSemester_set();
			this.fn_classRoom_set();
		break;
		
		case "scheduleList":
			this.ds_schedule_list.copyData(this.ds_scheduleTemp_list);
			this.fn_schedule_set();
		break;
		
		case "insertClass":
			if (this.insertCode == "02") 
			{
				this.initAtchFile();
				this.alert("등록 불가한 그룹강좌입니다.");
				
				if(this.ds_class_dtl.getColumn(0, "lectTpCd") == "02") 
				{
					this.Div00.form.Div00_00_00.form.Div00.form.Div00.form.Button00.setFocus();
				}
				else 
				{
					this.Div00.form.Div00_00_00_00.form.Div00.form.Div00.form.Button00.setFocus();
				}
			} 
			else 
			{
				this.alert("저장 되었습니다.");
				if(this.parent.searchFlag == 1)
				{
					this.opener.components[0].form.Div02.form.Div00.form.Div00.form.Div00_00_00_00.form.Button00.click();
				}	
				this.close();
			}
		break;
	  }
  }
};

