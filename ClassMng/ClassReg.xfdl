this.ds_schedule_temp_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if(e.col == 0) 
	{
		var year = obj.getColumn(e.row, 0).slice(0, 4)
		var mon = obj.getColumn(e.row, 0).slice(4, 6)
		var day = obj.getColumn(e.row, 0).slice(6, 8)
		var objDate = new nexacro.Date(year+ "-" + mon + "-" + day);
		var nDay = objDate.getDay();
		
		this.ds_schedule_temp.set_enableevent(false);
		
		this.ds_schedule_temp.setColumn(e.row, "dw", nDay);
		
		this.ds_schedule_temp.set_enableevent(true);
	}
