//강의 시간 세팅
this.fn_classTime_set = function()
{
	this.ds_start_hour.set_enableevent(false);
	
	this.ds_start_hour.clearData();
	this.ds_start_hour.addRow();
	this.ds_start_hour.setColumn(0, "cd", "");
	this.ds_start_hour.setColumn(0, "cdNm", "시작시간");
	for(var i=0; i <= 23; i++) 
	{
		this.ds_start_hour.addRow();
		this.ds_start_hour.setColumn(i+1, "cd", ("0"+i).slice(-2));
		this.ds_start_hour.setColumn(i+1, "cdNm", ("0"+i).slice(-2));
	}
	
	this.Div00.form.Div00.form.Div00.form.Div00_09.form.Combo00.set_index(0);
	
	this.ds_start_hour.set_enableevent(true);
	
	this.ds_start_min.set_enableevent(false);
	
	this.ds_start_min.clearData();
	for(var i=0; i <= 55; i++) 
	{
		this.ds_start_min.addRow();
		this.ds_start_min.setColumn(i, "cd", ("0"+i).slice(-2));
		this.ds_start_min.setColumn(i, "cdNm", ("0"+i).slice(-2));
	}	
	
	this.Div00.form.Div00.form.Div00.form.Div00_09.form.Combo01.set_index(0);
	
	this.ds_start_min.set_enableevent(true);
	
	this.ds_end_hour.set_enableevent(false);
	
	this.ds_end_hour.clearData();
	this.ds_end_hour.addRow();
	this.ds_end_hour.setColumn(0, "cd", "");
	this.ds_end_hour.setColumn(0, "cdNm", "종료시간");
	for(var i=0; i <= 23; i++) 
	{
		this.ds_end_hour.addRow();
		this.ds_end_hour.setColumn(i+1, "cd", ("0"+i).slice(-2));
		this.ds_end_hour.setColumn(i+1, "cdNm", ("0"+i).slice(-2));
