**************************************************************************/

this.init = function()
{
	this.Div00.form.Div00_00.set_height(0);
	this.Div00.form.Div00_00.set_top("Div00:15");
	this.Div00.form.Div00_00_00.set_height(0);
	this.Div00.form.Div00_00_00.set_top("Div00:15");
	this.Div00.form.Div00_00_00_00.set_height(0);
	this.Div00.form.Div00_00_00_00.set_top("Div00:15");
	this.Div00.form.Div00_00_00_00_00.set_top("Div00:15");
	
	this.Div00.form.Div00_00_00_00_00.form.Div00.form.Div05.set_top("Div04:0");
	this.Div00.form.Div00_00_00_00_00.form.Div00.form.Div05.set_height(0);
	
	this.Div00.form.Div00_00_00_00_01.set_height(60);
	this.Div00.form.Div00_00_00_00_01.form.Div00.form.Div01.set_height(0);
	this.Div00.form.Div00_00_00_00_01.form.Div00.form.Div02.set_height(0);
	
	this.Div00.form.Div00_00_00_00_00.set_height(520);
	
	this.Div00.form.resetScroll();
}

/**************************************************************************
 * 트랜잭션 callback
**************************************************************************/
this.fnCallback = function(svcID, errorCode, errorMsg)
{
  // 에러 시 화면 처리 내역
  if(errorCode != 0)
  {
    this.alert(errorCode+"\n"+errorMsg);
  }
  else 
  {
	 switch(svcID)
	  {
		case "codeList":
			this.fn_classCtgy_set();
			this.fn_classGb_set();
			this.fn_classTargetGb_set();
			this.fn_classTime_set();
			this.fn_spot_set();
			this.fn_optType_set();
		break;
		
		case "scheduleList":
			this.fn_schedule_set();
		break;
	  }
  }
};

/**************************************************************************
 * 코드 조회
**************************************************************************/

this.fn_code_list = function()
{	
	var outData = "ds_classCtgy_list=classCtgyList ds_classGb_list=classGbList ds_classTargetGb_list=classTargetGbList ds_classOptType_list=classOptTypeList";
		
	var tranObj = {
		strSvcId : "codeList",
		strSvcUrl : this.application.Admin_svc + "::class/getCodeList.do",
		inData : "",
		outData : outData,
		strArg : "cdId=CODE_65||CODE_66||CODE_68||CODE_73 useYn=Y",
		callBackFnc : "fnCallback",
		isAsync : false,
		isPopup : true
	}

	this.gfn_transaction(tranObj);
};

