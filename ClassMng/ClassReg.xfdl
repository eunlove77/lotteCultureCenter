//강좌내용 삭제
this.fn_content_remove = function(obj)
{
	var searchIndex;
	for(var i=0; i < this.contentArr.length; i++)
	{	
		if(this.contentArr[i].divObj.name == obj.name)
		{
			searchIndex=i;
			this.contentArr[i].divObj.destroy();
			
			this.ds_content_list.deleteRow(searchIndex);
			this.contentArr.splice(searchIndex, 1);
			this.atchfileArr.splice(searchIndex, 1);
			this.editorArr.splice(searchIndex, 1);
			
			if(this.contentArr[searchIndex] != undefined)
			{
				this.contentArr[searchIndex].divObj.set_top(this.contentArr[searchIndex-1].divObj.name + ":10");
			}

			this.Div00.form.Div00_00_00_00_01_00.form.Div00.form.Div01.form.Static00.set_height(598 + (600 * (this.contentArr.length -1)));
			this.Div00.form.Div00_00_00_00_01_00.form.Div00.form.Div01.form.resetScroll();
		}
	}
}

/**************************************************************************
 * 저장 영역
**************************************************************************/
this.fn_save = function(_this)
{
	var saveFlag = false;
	
	if(_this.thnlFileUpload.isPass)
	{
		for(var i=0; i < _this.atchfileArr.length; i++)
		{
			if(_this.atchfileArr[i].isPass)
			{
				saveFlag = true;
			}
			else
			{
				return;
			}
		}
	}
	else
	{
		saveFlag = false;
	}
	
	if(saveFlag)
	{
		_this.ds_class_dtl.setColumn(0, "lectRpsntImgFileId", _this.thnlFileUpload.atchFileId);
		for(var i=0; i < _this.atchfileArr.length; i++)
		{
			_this.ds_content_list.setColumn(i, "imgFileId", _this.atchfileArr[i].atchFileId);
		}	
	
		var inData = "classDtl=ds_class_dtl gradeDiscList=ds_gradeDisc_list teacherList=ds_teacher_list scheduleList=ds_schedule_list "
			inData += "schedulePlanList=ds_schedulePlan_list optionList=ds_option_list contentList=ds_content_list unuslList=ds_unusl_list " 
			inData += "cctList=ds_cct_list multtmsList=ds_multtms_list pblpmprcustList=ds_pblpmprcust_list" 
		var tranObj = {
			strSvcId : "insertClass",
			strSvcUrl : _this.application.Admin_svc + "::class/insertClass.do",
			inData : inData,
			outData : "",
			strArg : "",
			callBackFnc : "fnCallback",
			isAsync : false,
			menuSeq : _this.parent.menuSeq,
			isPopup : true
			
		}
		_this.gfn_transaction(tranObj);
	}
}

