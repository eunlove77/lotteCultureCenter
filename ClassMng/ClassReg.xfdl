//스케줄 세팅
this.fn_schedule_set = function(pObjName)
{
	//초기화
	this.fn_schedule_init(this.Div00.form.Div00_00_00_00_00.form, pObjName);
	
	if(this.ds_schedule_list.getRowCount() > 0)
	{
		this.ds_schedulePlan_list.set_keystring("S:+lectStDt, S:+lectDt");
	
		var content = ["Div04"];
		
		for(var i=0; i < this.ds_schedule_list.getRowCount(); i++)
		{
			var id = "scheduelPlanList" + i;
			
			if(pObjName != undefined && pObjName.indexOf(id) > -1)
			{
				this.fn_schedule_set_code(id, i, content, this.ds_schedule_list.getColumn(i, "lectStDt"));
			}
			else
			{
				if(pObjName == undefined)
				{
					this.fn_schedule_set_code(id, i, content, this.ds_schedule_list.getColumn(i, "lectStDt"));
				}
			}
			
			content.push(id);
		}
		
		this.ds_schedulePlan_list.set_keystring("S:+lectDt");
		
		this.ds_class_dtl.setColumn(0, "lectNbcnt", this.ds_schedulePlan_list.getRowCount());
		this.ds_class_dtl.setColumn(0, "lectStDtm", this.ds_schedulePlan_list.getColumn(0, "lectDt"));
		this.ds_class_dtl.setColumn(0, "lectEndDtm", this.ds_schedulePlan_list.getColumn(this.ds_schedulePlan_list.getRowCount()-1, "lectDt"));
		
		var scheduleCal = this.ds_schedule_list.getRowCount() * 115;
		this.Div00.form.Div00_00_00_00_00.form.Div00.form.Div05.set_top("Div04:" + (scheduleCal + 20));
		this.Div00.form.Div00_00_00_00_00.form.Div00.form.Div05.set_height(280);
		this.Div00.form.Div00_00_00_00_00.form.Div00.form.Div05.form.Grid00.setRealRowSize(-1, 0);
		
		var h = 830 + scheduleCal;
		this.Div00.form.Div00_00_00_00_00.set_height(h);	
		this.Div00.form.resetScroll();
	}
}

this.fn_schedule_set_code = function(id, i, content, lectStDt)
{
	var objComp = new Div();
	objComp.init(id, 0, content[i] + ":15", null, 100, 0);
	this.Div00.form.Div00_00_00_00_00.form.Div00.form.addChild(id, objComp);
	objComp.set_cssclass("div_WF_CompWrap");	
	objComp.show();		
	
	var objStatic = new Static();
	objStatic.init("scheduleStatic" + i, 0, 0, null, 40, 0);
	objComp.addChild("scheduleStatic" + i, objStatic);
	objStatic.set_cssclass("sta_WF_TableLabel");
	
	var arrDays = new Array("", "월요일", "화요일", "수요일", "목요일", "금요일", "토요일", "일요일");	
	var text = arrDays[this.ds_schedule_list.getColumn(i, "stDaywCd")];
	
	var arrRow = this.ds_schedulePlan_list.extractRows("pass=='0' && lectStDt=='" + lectStDt + "'");
	
	if(arrRow == "")
	{
		arrRow = [];
	}
	
	text += " (" + arrRow.length  + "/" + this.ds_schedule_list.getColumn(i, "lectNbcnt") + ")"
	objStatic.set_text(text);
	objStatic.show();
		
	var objDiv = new Div();
	objDiv.init("scheduelDiv" + i, 0, "scheduleStatic" + i + ":0", null, null, 0, 0);
	objComp.addChild("scheduelDiv" + i, objDiv);
	objDiv.show();	
	
	var objButton = new Button();
	objButton.init("scheduleButton" + i, null, 0, 100, 40, 0);
	objComp.addChild("scheduleButton" + i, objButton);
	objButton.set_cssclass("btn_WF_Medium");
	objButton.set_text("스케줄변경");
	objButton.show();
	objButton.insertEventHandler("onclick", 0, this.fn_schedule_chg, this);
	
	var calRow = this.ds_schedulePlan_list.extractRows("lectStDt=='" + lectStDt + "'");
	var content2 = [""];
	for(var j=0; j< calRow.length; j++)
	{
		var calId = "scheduleCal" + j
		var objCal = new Calendar();
		objCal.init(calId, content2[j] + ":10", "15", 120, 20)
		objDiv.addChild(calId, objCal);
		objCal.set_value(this.ds_schedulePlan_list.getColumn(calRow[j], "lectDt"));	
		objCal.set_readonly(true);
		if(this.ds_schedulePlan_list.getColumn(calRow[j], "pass") == '0') 
		{
			objCal.set_background("#aea7a8");
		}
		
		objCal.show();
		
		objCal.insertEventHandler("oncloseup", 0, this.fn_schedule_cal, this);
		
		content2.push(calId);			
	}

	objDiv.form.resetScroll();	
}

this.fn_schedule_cal = function(obj)
{
	var calRow = this.ds_schedulePlan_list.extractRows("lectDt==" + obj.value);
	
	if(calRow.length > 0 && obj.value != obj._default_value)
	{
		alert("중복된 날짜");
		obj.set_value(obj._default_value);
	}
	else if(obj.value != obj._default_value)
	{
		var tranObj = {
			strSvcId : "scheduleCheck",
			strSvcUrl : this.application.Admin_svc + "::class/getScheduleCheck.do",
			inData : "",
			outData : "",
			strArg : "brchCd=" + this.brchCd + " lectDt=" + obj.value,
			callBackFnc : "fnCallback",
			isAsync : false,
			menuSeq : this.parent.menuSeq,
			isPopup : true
		}
		this.gfn_transaction(tranObj);
		
		if(this.dateChk == 1)
		{
			alert("휴일인 날짜 입니다.");
			obj.set_value(obj._default_value);
			return
		}
	
		var calRow = this.ds_schedulePlan_list.extractRows("lectDt==" + obj._default_value);
		
		var arrDays = new Array("", "월요일", "화요일", "수요일", "목요일", "금요일", "토요일", "일요일");	
		
		var year = obj.value.slice(0, 4)
		var mon = obj.value.slice(4, 6)
		var day = obj.value.slice(6, 8)
	
		var objDate = new nexacro.Date(year+ "-" + mon + "-" + day);
		var nDay = objDate.getDay();
		nDay = (nDay == 0) ? 7 : nDay;
		
		this.ds_schedulePlan_list.setColumn(calRow, "lectDt", obj.value);
		this.ds_schedulePlan_list.setColumn(calRow, "stDaywCd", nDay);
		this.ds_schedulePlan_list.setColumn(calRow, "stDaywNm", arrDays[nDay]);		
		
		year = new nexacro.Date().getFullYear();
		mon = new nexacro.Date().getMonth()+1;
		mon = ("0" + mon).slice(-2);
		day = new nexacro.Date().getDate(); 
        day = ("0" + day).slice(-2);
		
		var pass = 0;
		if((year+ "" + mon + "" + day) <= obj.value)
		{
			pass = 1;
		}	
		this.ds_schedulePlan_list.setColumn(calRow, "pass", pass);	
	}
};

this.fn_schedule_chg = function(obj)
{
	obj.set_text("저장");
	obj.removeEventHandler("onclick", this.fn_schedule_chg, this)
	obj.insertEventHandler("onclick", 0, this.fn_schedule_save, this);
	this.fn_schedule_cmpl(obj.parent, 2);
}

this.fn_schedule_save = function(obj)
{
	obj.set_text("스케줄변경");
	obj.removeEventHandler("onclick", this.fn_schedule_save, this)
	obj.insertEventHandler("onclick", 0, this.fn_schedule_chg, this);
	
	this.fn_schedule_set(obj.parent.parent.name);
}

this.fn_schedule_cmpl = function(pObj, type)
{
	var arrObj = pObj.components;
	var type = type;
	for(var i=0; i<arrObj.length; i++)
	{	
		if(arrObj[i] == "[object Div]")
		{
			this.fn_schedule_cmpl(arrObj[i].form, type);
		}
		else 
		{
			if(arrObj[i].name.indexOf("scheduleCal") > -1)
			{
				if(type == 1) 
				{
					arrObj[i].set_readonly(true);
				}
				else
				{
					var calRow = this.ds_schedulePlan_list.extractRows("lectDt==" + arrObj[i].value);	
					arrObj[i].set_readonly(false);
					if(this.ds_schedulePlan_list.getColumn(calRow[0], "pass") == '0')
					{
						arrObj[i].set_background("#aea7a8");
					}
					else
					{
						arrObj[i].set_background("");
					}
				}		
			}
		}
	}
}

//스케줄 초기화
this.fn_schedule_init = function(pObj, pObjName)
{
	var arrObj = pObj.components;
	for(var i=0; i<arrObj.length; i++)
	{	
		if(arrObj[i] == "[object Div]" && arrObj[i].name.indexOf("scheduelPlanList") > -1)
		{
			if(pObjName != undefined && arrObj[i].name.indexOf(pObjName) > -1)
			{
				arrObj[i].destroy();
				this.fn_schedule_init(this.Div00.form.Div00_00_00_00_00.form, pObjName);
			}
			else
			{
				if(pObjName == undefined) 
				{
					arrObj[i].destroy();
					this.fn_schedule_init(this.Div00.form.Div00_00_00_00_00.form, pObjName);
				}
			}			
		}
		else 
		{
			if(arrObj[i] == "[object Div]")
			{
				this.fn_schedule_init(arrObj[i].form, pObjName);
			}
		}
	}
}

