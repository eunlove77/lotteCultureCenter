			else
			{
				this.ds_teacher_list.setColumn(i, "rpsntYn", "N");
			}
		}
	}
};

this.Div00_Div00_00_00_00_00_Div00_Div00_Grid00_oncloseup = function(obj:nexacro.Grid,e:nexacro.GridEditEventInfo)
{
	if(e.col == "9")
	{	
		if(e.value != "D8" && this.ds_teacher_list.getColumn(e.row, "tcTpCd") == 1)
		{
			this.ds_teacher_list.setColumn(e.row, "incmClCd", "60");
