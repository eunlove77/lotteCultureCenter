this.Div00_Div00_00_00_00_00_Div00_Div04_Button00_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{	
	var arrRow = this.ds_schedule_temp.extractRows("strtDt== '' || strtDt== undefined || count == '' || count== undefined");
	
	if(arrRow == "")
	{
		var flag = true;
		for(var i=0; i<this.ds_schedule_temp.getRowCount(); i++)
		{
			arrRow = this.ds_schedule_temp.extractRows("dw== " + this.ds_schedule_temp.getColumn(i, "dw"));
			
			if(arrRow.length > 1)
			{
				alert("강의시작일/강의횟수/강의요일을 확인하세요.");
				return flag
			}
		}
		
		this.ds_schedule.clearData();		
		for(var i=0; i<this.ds_schedule_temp.getRowCount(); i++)
		{
			this.ds_schedule.addRow();
			this.ds_schedule.copyRow(i, this.ds_schedule_temp, i);
		}
		
		var tranObj = {
			strSvcId : "scheduleList",
			strSvcUrl : this.application.Admin_svc + "::class/getScheduleList.do",
			inData : "classSchedule=ds_schedule",
			outData : "ds_schedule_list=scheduleList",
			strArg : "brchCd=" + this.brchCd,
			callBackFnc : "fnCallback",
			isAsync : true,
			isPopup : true
		}

		this.gfn_transaction(tranObj);
		
	}
	else 
	{
		alert("강의시작일/강의횟수/강의요일을 확인하세요.");
	}
};

