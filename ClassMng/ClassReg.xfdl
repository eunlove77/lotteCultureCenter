	}
};

this.Div00_Div00_00_00_00_00_Div00_Div00_Grid00_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	if(e.col == "4")
	{
		for(var i=0; i<this.ds_teacher_list.getRowCount(); i++) 
