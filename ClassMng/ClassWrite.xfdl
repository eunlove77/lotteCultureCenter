//강좌내용 삭제
this.fn_content_remove = function(obj, init)
{
	var searchIndex;
	for(var i=0; i < this.contentArr.length; i++)
	{	
		if(this.contentArr[i].divObj.name == obj.name)
		{
			searchIndex=i;
			this.contentArr[i].divObj.destroy();
			
			if(init == undefined)
			{
				this.ds_content_list.deleteRow(searchIndex);
			}
			this.contentArr.splice(searchIndex, 1);
			this.atchfileArr.splice(searchIndex, 1);
			this.editorArr.splice(searchIndex, 1);
			
			if(this.contentArr[searchIndex] != undefined)
			{
				this.contentArr[searchIndex].divObj.set_top(this.contentArr[searchIndex-1].divObj.name + ":10");
			}

