/**************************************************************************
 * 트랜잭션 callback
**************************************************************************/
this.fnCallback = function(svcID, errorCode, errorMsg)
{
  // 에러 시 화면 처리 내역
  if(errorCode != 0)
  {
    this.alert(errorCode+"\n"+errorMsg);
  }
  else 
  {
	 switch(svcID)
	  {
		case "codeList":
			this.fn_classCtgy_set();
			this.fn_classType_set();
			this.fn_classGb_set();
			this.fn_classTargetGb_set();
			this.fn_classTime_set();
			this.fn_classOptType_set();
			this.fn_classDispIcon_set();
			this.fn_classSemester_set();
			this.fn_classRoom_set();
		break;
		
		case "scheduleList":
			this.ds_schedule_list.copyData(this.ds_scheduleTemp_list);
			this.fn_schedule_set();
		break;
		
		case "updateClass":
			if(this.updateFlag == "1") 
			{
				this.alert("저장 되었습니다.");
				this.parent.parent.parent.close();
			}
			else
			{
				this.alert("다시 시도해주세요.");
				
				this.ds_class_dtl.clearData();
				this.ds_class_dtl.addRow();
				this.ds_class_dtl.setColumn(0, "brchCd", this.brchCd);
				this.ds_class_dtl.setColumn(0, "yy", this.yy);
				this.ds_class_dtl.setColumn(0, "lectSmsterCd", this.lectSmsterCd);
				this.ds_class_dtl.setColumn(0, "lectCd", this.lectCd);
				
				this.fn_class_dtl();			
				//화면초기화
				this.init();
				// 코드 데이터
				this.fn_code_list();
				this.Div00.form.Div00.form.Static00.setFocus();
			}
		break;
		
		case "updateClassMdfreq":
			if(this.ds_mdfreqUpdate_list.getColumn(0, "prcsStatCd") == "02")
			{
				alert("요청사항 완료 처리되었습니다.");
			}
			else
			{
				alert("요청사항 반려 처리되었습니다.");
			}
			this.Div00.form.Div02.form.Grid00.setCellProperty("head", 0, "text", 0);
		 break;
		 
		 case "insertClassCopy":
			alert("복사등록이 완료 되었습니다.");
		 break;
	  }
  }
};

