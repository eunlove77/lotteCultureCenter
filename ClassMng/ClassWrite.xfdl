	if(e.col == 0) 
	{
		var year = obj.getColumn(e.row, 0).slice(0, 4)
		var mon = obj.getColumn(e.row, 0).slice(4, 6)
		var day = obj.getColumn(e.row, 0).slice(6, 8)
		var objDate = new nexacro.Date(year+ "-" + mon + "-" + day);
