    <Script type="xscript5.1"><![CDATA[/**************************************************************************
 * include 영역 
 **************************************************************************/
include "Base::libCommon.xjs";
include "Base::popup.xjs";


/**************************************************************************
 * FORM 변수 선언 영역
 **************************************************************************/
this.application = null;
this.domain = "";
this.menuSeq = "";

this.totCnt = 0;
this.cnt = 0;
this.popCnt = 0;

// 수강등록 파라미터로 넘길 데이터
this.mbrNo = "";
this.yy = "";
this.lectSmsterCd = "";
this.mbrGrdeCd = "";
this.atlctRsvNo = "";
this.actlAtlctNpleSeqno = "";
this.brchCd = "";
this.lectCd = "";

this.arrAtlctRsvNo = "";

this.searchButton = null;

/**************************************************************************
 * FORM EVENT 영역(onload)
 **************************************************************************/
this.ClassWaitingList_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	this.domain = this.application.Domain;
	this.menuSeq = this.parent.parent.parent.parent.menuSeq;
	this.searchButton = this.parent.parent.parent.parent.searchButton;
	
	this.ds_class_dtl.copyData(this.parent.parent.Tabpage1.form.ds_class_dtl);
	this.ds_class_dtl.setColumn(0, "yy", this.ds_class_dtl.getColumn(0, "yy").split("||")[0]);
	this.fn_data();
	
	//톡전송 버튼 세팅
	var talkObj = {
		columnObj : {
						mbrNo : "mbrNo"		//trgtDs의 회원번호 컬럼 매칭
					,	korNm : "korNm"		//trgtDs의 회원명 컬럼 매칭
					,	hpNo : "hpNo"		//trgtDs의 수신번호 컬럼 매칭
					,	smsRcvAgrYn : "smsRcvAgrYn"	//trgtDs의 수신여부 컬럼 매칭
					},
		trgtDs : this.ds_rcv_obj,	
		menuId : this.menuSeq,
		_this : this		//별도의 유효성체크 함수가 있는 경우 선언하고 fn_talk_check 함수 생성
	}
	this.Div00.form.Div03.form.fn_init(talkObj);	//톡전송 버튼div의 init 실행
};

// 초기 데이터 세팅
this.fn_data = function(){
	var tranObj = {
		strSvcId : "list",
		strSvcUrl : this.application.Admin_svc + "::class/selectWaitingList.do",
		inData : "classDtl=ds_class_dtl",
		outData : "ds_cnt=cntDtl ds_waiting_list=waitingList totCnt=totCnt",
		strArg : "",
		callBackFnc : "fnCallback",
		isAsync : false,
		menuSeq : this.menuSeq,
		isPopup : true
	}

	this.gfn_transaction(tranObj);
}

this.fnCallback = function(svcID, errorCode, errorMsg)
{
  // 에러 시 화면 처리 내역
  if(errorCode != 0)
  {
	if(svcID == "requestPayment" || svcID == "lectReg"){
		if(errorCode == -1){
			this.alert(errorCode+"\n"+errorMsg);
		}else if(errorCode == -2){
			// 수강상태
			alert("신청불가한 강좌입니다.");
		}else if(errorCode == -3){
			// 수강인원
			alert("해당 강좌는 온라인 재고가 없습니다. 강좌 재고여부 확인 후 재신청 하시기 바랍니다.");
		}
		this.fn_init();
	}else{
		this.alert(errorCode+"\n"+errorMsg);
	}
    
  }
  else 
  {
	 switch(svcID)
	  {
		case "list":
			this.Div00.form.Div00.form.Static08.set_text(this.ds_class_dtl.getColumn(0, "brchNm"));
			this.Div00.form.Div00.form.Static09.set_text(this.ds_class_dtl.getColumn(0, "yy").split("||")[0] + "년 " + this.ds_class_dtl.getColumn(0, "lectSmsterNm"));
			this.Div00.form.Div00.form.Static10.set_text(this.ds_class_dtl.getColumn(0, "lectCd"));
			this.Div00.form.Div00.form.Static11.set_text(this.ds_class_dtl.getColumn(0, "lectNm"));
			var lectStDtm = this.ds_class_dtl.getColumn(0, "lectStDtm");
			lectStDtm = lectStDtm.substring(0, 4) + "-" + lectStDtm.substring(4,6) + "-" + lectStDtm.substring(6,8);
			var lectEndDtm = this.ds_class_dtl.getColumn(0, "lectEndDtm");
			lectEndDtm = lectEndDtm.substring(0, 4) + "-" + lectEndDtm.substring(4,6) + "-" + lectEndDtm.substring(6,8);
			this.Div00.form.Div00.form.Static12.set_text(lectStDtm + " ~ " + lectEndDtm);
			this.Div00.form.Div00.form.Static13.set_text(this.ds_class_dtl.getColumn(0, "lectStHhCd") + ":" + this.ds_class_dtl.getColumn(0, "lectStMiCd"));
			this.Div00.form.Div00.form.Static14.set_text(this.ds_cnt.getColumn(0, "atlctCnt") + "/" + this.ds_class_dtl.getColumn(0, "gsCapaCnt"));
			this.Div00.form.Div00.form.Static15.set_text(this.ds_cnt.getColumn(0, "waitCnt"));
			this.Div00.form.Div00.form.Static16.set_text(this.ds_class_dtl.getColumn(0, "lectAddOptnNm"));
			
			this.Div00.form.Static01.set_text("("+ this.totCnt +"개)");
			
			this.gfnSetCheckBoxNoDataset(this.ds_waiting_list, this.Div00.form.Grid00, 0);
		break;
		case "cancel":
			this.Div00.form.Div00.form.Static14.set_text(this.ds_cnt.getColumn(0, "atlctCnt") + "/" + this.ds_class_dtl.getColumn(0, "gsCapaCnt"));
			this.Div00.form.Div00.form.Static15.set_text(this.ds_cnt.getColumn(0, "waitCnt"));
			this.Div00.form.Static01.set_text("("+ this.totCnt +"개)");
			alert("정상 처리되었습니다.");
		break;
		case "memoUpdate":
			alert("저장 되었습니다.");
		break;
		case "requestPayment":
			alert("정상 처리되었습니다.");
		break;
		case "lectReg":
			if(confirm("수강등록 하시겠습니까?")){
 				var objLectRegMenu = {
 					menuSeq : 209,
 					menuNm : '수강등록',
 					pagePath : 'StlmMng',
 					pageId : 'LectReg'
//  					mbrNo : this.mbrNo,
//  					yy : this.yy,
//  					lectSmsterCd : this.lectSmsterCd,
//  					mbrGrdeCd : this.mbrGrdeCd,
//  					atlctRsvNo : this.atlctRsvNo,
//  					actlAtlctNpleSeqno : this.actlAtlctNpleSeqno,
//  					brchCd : this.brchCd,
//  					lectCd : this.lectCd
 				};
				this.application.LECT_REG = {
					mbrNo : this.mbrNo,
					yy : this.yy,
					lectSmsterCd : this.lectSmsterCd,
					mbrGrdeCd : this.mbrGrdeCd,
					atlctRsvNo : this.atlctRsvNo,
					actlAtlctNpleSeqno : this.actlAtlctNpleSeqno,
					brchCd : this.brchCd,
					lectCd : this.lectCd
				};
 				this.application.LeftFrame.form.fn_openMenu(objLectRegMenu);
 				this.parent.parent.parent.close();
			}
		break;
		
	  }
  }
};

// 대기취소
this.Div00_Button05_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.ds_waiting_param.clearData();
	var chkYn = false;
	var atlctRsvNo = "";
	for(var i=0; i<this.ds_waiting_list.getRowCount(); i++){
		if(this.ds_waiting_list.checkarray[i] > 0 && this.ds_waiting_list.getColumn(i, "stlmStatCd") != "31" && this.ds_waiting_list.getColumn(i, "stlmStatCd") != "41"){
			 if(i == 0){
					chkYn = true;
					atlctRsvNo = this.ds_waiting_list.getColumn(i, "atlctRsvNo");
					this.ds_waiting_param.addRow();
					this.ds_waiting_param.copyRow(this.ds_waiting_param.getRowCount()-1, this.ds_waiting_list, i);
					this.ds_waiting_param.setColumn(this.ds_waiting_param.getRowCount()-1, "stlmStatCd", "41");
					//this.ds_waiting_list.setColumn(i, "stlmStatCd", "41")
			 }else{
				 if(this.ds_waiting_list.getColumn(i-1, "atlctRsvNo") != this.ds_waiting_list.getColumn(i, "atlctRsvNo")){
					chkYn = true;
					atlctRsvNo = this.ds_waiting_list.getColumn(i, "atlctRsvNo");
					this.ds_waiting_param.addRow();
					this.ds_waiting_param.copyRow(this.ds_waiting_param.getRowCount()-1, this.ds_waiting_list, i);
					this.ds_waiting_param.setColumn(this.ds_waiting_param.getRowCount()-1, "stlmStatCd", "41");
					//this.ds_waiting_list.setColumn(i, "stlmStatCd", "41")
				 }
			 }
		}else{
			if(atlctRsvNo == this.ds_waiting_list.getColumn(i, "atlctRsvNo")){
				this.ds_waiting_param.addRow();
				this.ds_waiting_param.copyRow(this.ds_waiting_param.getRowCount()-1, this.ds_waiting_list, i);
				this.ds_waiting_param.setColumn(this.ds_waiting_param.getRowCount()-1, "stlmStatCd", "41");
			}
		}
	}
	
	if(!chkYn){
		alert("대상을 선택하세요.");
		this.fn_init();
// 		this.ds_waiting_list.set_enableevent(false);
// 		for(var i=0; i<this.ds_waiting_list.getRowCount(); i++){
// 			if(this.ds_waiting_list.checkarray[i] > 0){
// 				chkYn = true;
// 				this.ds_waiting_list.setColumn(i, "stlmStatCd", "11")
// 			}
// 		}
// 		this.ds_waiting_list.set_enableevent(true);
		return;
	}else{
		if(confirm("대기상태를 취소로 변경하시겠습니까?")){
			var tranObj = {
				strSvcId : "cancel",
				strSvcUrl : this.application.Admin_svc + "::class/cancelWaitingList.do",
				inData : "classDtl=this.ds_class_dtl waitingList=this.ds_waiting_param",
				outData : "ds_waiting_list=waitingList totCnt=totCnt ds_cnt=cntDtl",
				strArg : "",
				callBackFnc : "fnCallback",
				isAsync : false,
				menuSeq : this.menuSeq,
				isPopup : true
			}

			this.gfn_transaction(tranObj);
			if(this.searchButton !== undefined) this.searchButton.click();
		}
	}
	
	
};

// 메모 저장
this.Div00_Button03_00_00_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
 	var tranObj = {
 		strSvcId : "memoUpdate",
 		strSvcUrl : this.application.Admin_svc + "::class/updateMemoWaitingList.do",
 		inData : "classDtl=ds_class_dtl waitingList=ds_waiting_list:U",
 		outData : "ds_waiting_list=waitingList totCnt=totCnt",
 		strArg : "",
 		callBackFnc : "fnCallback",
 		isAsync : false,
 		menuSeq : this.menuSeq,
 		isPopup : true
 	}
 
 	this.gfn_transaction(tranObj);
};

// 결제 요청
this.Div00_Button03_00_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.ds_waiting_param.clearData();
	var checkarray = [];
	var atlctLimitYn = true;
	var atlctRsvNo = "";
	for(var i=0; i<this.ds_waiting_list.getRowCount(); i++){
		if(this.ds_waiting_list.checkarray[i] > 0 && this.ds_waiting_list.getColumn(i, "stlmStatCd") != "31" && this.ds_waiting_list.getColumn(i, "stlmStatCd") != "41"){
 			if(i == 0){
				checkarray.push(i);
				atlctRsvNo = this.ds_waiting_list.getColumn(i, "atlctRsvNo");
				if(this.ds_waiting_list.getColumn(i, "waitingStatCd") == "30"){
					// 시간초과 시
					atlctLimitYn = false;
				}
				//this.ds_waiting_list.setColumn(i, "stlmStatCd", "70");
				this.ds_waiting_param.addRow();
				this.ds_waiting_param.copyRow(this.ds_waiting_param.getRowCount()-1, this.ds_waiting_list, i);
				this.ds_waiting_param.setColumn(this.ds_waiting_param.getRowCount()-1, "stlmStatCd", "70");
			}else{
				if(this.ds_waiting_list.getColumn(i-1, "atlctRsvNo") != this.ds_waiting_list.getColumn(i, "atlctRsvNo")){
					checkarray.push(i);
					atlctRsvNo = this.ds_waiting_list.getColumn(i, "atlctRsvNo");
					if(this.ds_waiting_list.getColumn(i, "waitingStatCd") == "30"){
						// 시간초과 시
						atlctLimitYn = false;
					}
					//this.ds_waiting_list.setColumn(i, "stlmStatCd", "70");
					this.ds_waiting_param.addRow();
					this.ds_waiting_param.copyRow(this.ds_waiting_param.getRowCount()-1, this.ds_waiting_list, i);
					this.ds_waiting_param.setColumn(this.ds_waiting_param.getRowCount()-1, "stlmStatCd", "70");
				}
			}
		}else{
			if(atlctRsvNo == this.ds_waiting_list.getColumn(i, "atlctRsvNo")){
				this.ds_waiting_param.addRow();
				this.ds_waiting_param.copyRow(this.ds_waiting_param.getRowCount()-1, this.ds_waiting_list, i);
				this.ds_waiting_param.setColumn(this.ds_waiting_param.getRowCount()-1, "stlmStatCd", "70");
			}
		}
	}
	
	if(!atlctLimitYn){
		alert("결제 기한이 만료된 대기자에게 결제요청 할 수 없습니다.");
		this.fn_init();
		return;
	}
	
	if(checkarray.length == 0){
		alert("대상을 선택하세요.");
		this.fn_init();
	}else if(checkarray.length > 1){
		alert("대상을 1명만 선택하세요.");
		this.fn_init();
	}else{
		if(confirm("결제요청 하시겠습니까?")){
			var tranObj = {
				strSvcId : "requestPayment",
				strSvcUrl : this.application.Admin_svc + "::class/requestPayment.do",
				inData : "classDtl=this.ds_class_dtl waitingList=this.ds_waiting_param",
				outData : "ds_waiting_list=waitingList totCnt=totCnt",
				strArg : "",
				callBackFnc : "fnCallback",
				isAsync : false,
				menuSeq : this.menuSeq,
				isPopup : true
			}

			this.gfn_transaction(tranObj);
		}
	}
};

// 수강등록
this.Div00_Button03_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{	
	this.ds_waiting_param.clearData();
	var checkarray = [];
	var atlctRsvNo = "";
	for(var i=0; i<this.ds_waiting_list.getRowCount(); i++){
		if(this.ds_waiting_list.checkarray[i] > 0 && this.ds_waiting_list.getColumn(i, "stlmStatCd") != "31" && this.ds_waiting_list.getColumn(i, "stlmStatCd") != "41"){
			 if(i == 0){
				checkarray.push(i);
				atlctRsvNo = this.ds_waiting_list.getColumn(i, "atlctRsvNo");
				//this.ds_waiting_list.setColumn(i, "stlmStatCd", "20");
				this.ds_waiting_param.addRow();
				this.ds_waiting_param.copyRow(this.ds_waiting_param.getRowCount()-1, this.ds_waiting_list, i);
				this.ds_waiting_param.setColumn(this.ds_waiting_param.getRowCount()-1, "stlmStatCd", "20");
			 }else{
				 if(this.ds_waiting_list.getColumn(i-1, "atlctRsvNo") != this.ds_waiting_list.getColumn(i, "atlctRsvNo")){
					checkarray.push(i);
					atlctRsvNo = this.ds_waiting_list.getColumn(i, "atlctRsvNo");
					//this.ds_waiting_list.setColumn(i, "stlmStatCd", "20");
					this.ds_waiting_param.addRow();
					this.ds_waiting_param.copyRow(this.ds_waiting_param.getRowCount()-1, this.ds_waiting_list, i);
					this.ds_waiting_param.setColumn(this.ds_waiting_param.getRowCount()-1, "stlmStatCd", "20");
				 }
			 }
		}else{
			// 동반회원
			if(atlctRsvNo == this.ds_waiting_list.getColumn(i, "atlctRsvNo")){
				//this.ds_waiting_list.setColumn(i, "stlmStatCd", "20");
				this.ds_waiting_param.addRow();
				this.ds_waiting_param.copyRow(this.ds_waiting_param.getRowCount()-1, this.ds_waiting_list, i);
				this.ds_waiting_param.setColumn(this.ds_waiting_param.getRowCount()-1, "stlmStatCd", "20");
			}
		}
	}

	if(checkarray.length == 0){
		alert("대상을 선택하세요.");
		this.fn_init();
	}else if(checkarray.length > 1){
		alert("대상을 1명만 선택하세요.");
		this.fn_init();
	}else{
		var tranObj = {
			strSvcId : "lectReg",
			strSvcUrl : this.application.Admin_svc + "::class/validateLectReg.do",
			inData : "classDtl=this.ds_class_dtl waitingList=this.ds_waiting_param",
			outData : "",
			strArg : "",
			callBackFnc : "fnCallback",
			isAsync : false,
			menuSeq : this.menuSeq,
			isPopup : true
		}

		this.gfn_transaction(tranObj);
	}
};

this.fn_init = function()
{
	this.ds_waiting_list.set_enableevent(false);
	for(var i=0; i<this.ds_waiting_list.getRowCount(); i++){
		if(this.ds_waiting_list.checkarray[i] > 0 && this.ds_waiting_list.getColumn(i, "stlmStatCd") != "41"){
			this.ds_waiting_list.setColumn(i, "stlmStatCd", "11");
		}
	}
	this.ds_waiting_list.set_enableevent(true);
}

// 톡발송 유효성 함수
this.fn_talk_check = function()
{
	var chkYn = false;
	var nRow = -1;
	this.ds_rcv_obj.clearData();
	this.ds_rcv_obj.checkarray = new Array();
	for(var i=0; i<this.ds_waiting_list.getRowCount(); i++){
		if(this.ds_waiting_list.checkarray[i] > 0){
			chkYn = true;
			var duplYn = false;
			if(nRow > -1){
				for(var j=0;j<=this.ds_rcv_obj.getRowCount();j++){
					if(this.ds_rcv_obj.getColumn(j, "mbrNo") == this.ds_waiting_list.getColumn(i, "mbrNo")){
						duplYn = true;
					}
				}
			}
			
			if(!duplYn){
				nRow = this.ds_rcv_obj.addRow();
				this.ds_rcv_obj.setColumn(nRow, "mbrNo", this.ds_waiting_list.getColumn(i, "mbrNo"));
				this.ds_rcv_obj.setColumn(nRow, "korNm", this.ds_waiting_list.getColumn(i, "mbrNm"));
				this.ds_rcv_obj.setColumn(nRow, "hpNo", this.ds_waiting_list.getColumn(i, "hp"));
				this.ds_rcv_obj.setColumn(nRow, "smsRcvAgrYn", this.ds_waiting_list.getColumn(i, "smsRcvAgrYn"));
				
				this.ds_rcv_obj.setColumn(nRow, "lectNm", this.ds_waiting_list.getColumn(i, "lectNm"));
				this.ds_rcv_obj.setColumn(nRow, "lectStDtm", this.ds_waiting_list.getColumn(i, "lectStDtm"));
				this.ds_rcv_obj.setColumn(nRow, "lectRmNm", this.ds_waiting_list.getColumn(i, "lectRmNm"));
				this.ds_rcv_obj.setColumn(nRow, "optnNm", this.ds_waiting_list.getColumn(i, "optnNm"));
				this.ds_rcv_obj.checkarray[nRow] = 1;
			}
		}
	}
};

