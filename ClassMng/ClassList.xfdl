/**************************************************************************
 * FORM 변수 선언 영역
 **************************************************************************/
this.application = null;
this.popCnt = 0;
this.brchCd = "";
this.yy = "";
this.lectSmsterCd = "";
this.excelImportExport = "";
this.excelFileName = "강좌목록_관리_";
this.searchFlag = 0;
this.personalId = "ds_class_list";

/**************************************************************************
 * FORM EVENT 영역(onload)
 **************************************************************************/
 
this.ClassList_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	this.txtMenuNm.set_text(obj.parent.parent.fvMenuNM);
	this.excelImportExport = this.application.getVariable("excelImportExport");
	
	//지점초기화
	this.brchCd = nexacro.getPrivateProfile("brchCd") == "0000" ? "0001" : nexacro.getPrivateProfile("brchCd");
	this.ds_gdsSpotCd_list.copyData(this.application.gdsSpotCdList);
	this.ds_gdsSpotCd_list.deleteRow(0);
	this.fn_classBrch_set();
	
	//캘린더 초기화
	this.Div02.form.Div00.form.Div00.form.Div00_00_01.form.Calendar00.calendaredit.set_readonly(true); 
	this.Div02.form.Div00.form.Div00.form.Div00_00_01.form.Calendar01.calendaredit.set_readonly(true); 
	this.Div02.form.Div00.form.Div00.form.Div01_00_00.form.Calendar01.calendaredit.set_readonly(true); 
	
	//그리드초기화
	this.gfnSetCheckBoxNoDataset(this.ds_class_list, this.Div02.form.Div01.form.Grid00, 0); 
	this.gfn_onContextMenu(this.Div02.form.Div01.form.Grid00, this.personalId);
	this.init();	
	
	//체크박스 추가
	this.fn_classState_add();
	this.fn_classGb_add();
	this.fn_classAprvState_add();
	this.fn_classType_add();
	this.fn_classDispIcon_add();
	this.fn_click_add(this);
};

/**************************************************************************
 * 초기화 영역
**************************************************************************/

this.init = function() 
{	
	this.ds_search.clearData();
	this.ds_search.addRow();
	this.ds_search.setColumn(0, "brchCd", this.brchCd);
	this.ds_search.setColumn(0, "f", "");
	this.ds_search.setColumn(0, "q", "");
	this.ds_search.setColumn(0, "stDaywCd", "");
	this.Div02.form.Div00.form.Div00.form.Div03.form.chkMdf1.set_falsevalue("");
	this.Div02.form.Div00.form.Div00.form.Div03.form.chkMdf0.set_falsevalue("");
	this.Div02.form.Div00.form.Div00.form.Div01_01.form.chkDis0.set_falsevalue("");
	this.Div02.form.Div00.form.Div00.form.Div01_01.form.chkDis1.set_falsevalue("");
	this.Div02.form.Div00.form.Div00.form.Div05.form.chkGrp0.set_falsevalue("");
