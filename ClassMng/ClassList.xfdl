	if(3 <= month && month <= 5)
	{
		//봄
		this.Div00.form.Div00_00.form.Div00_00_01.form.Combo01.set_index(0);
	}
	else if(6 <= month && month <= 8)
	{
		//여름
		this.Div00.form.Div00_00.form.Div00_00_01.form.Combo01.set_index(1);
	}
	else if(9 <= month && month <= 11)
	{
		//가을
		this.Div00.form.Div00_00.form.Div00_00_01.form.Combo01.set_index(2);
	}	
	else
	{
		//겨울
		this.Div00.form.Div00_00.form.Div00_00_01.form.Combo01.set_index(3);
	}
}

