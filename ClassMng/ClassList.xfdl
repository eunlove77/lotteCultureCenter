 * 코드 조회
**************************************************************************/

this.fn_code_list = function()
{	
	var outData = "ds_classType_list=classTypeList ds_classState_list=classStateList ";
		outData += "ds_classGb_list=classGbList ds_classAprvState_list=classAprvStateList ds_classRoom_list=classRoomList ds_classDispIcon_list=classDispIconList ";
		outData += "ds_classCtgyLarge_list=classCategoryLargeList ds_classCtgyMiddle_list=classCategoryMiddleList ds_classCtgySmall_list=classCategorySmallList";
	
	var tranObj = {
		strSvcId : "codeList",
		strSvcUrl : this.application.Admin_svc + "::class/getCodeList.do",
		inData : "",
		outData : outData,
		strArg : "cdId=114||115||116||117||118||120||ctgy||room brchCd=" + this.brchCd,
		callBackFnc : "fnCallback",
		isAsync : false,
		menuSeq : this.getOwnerFrame().param.MENU_ID
	}

	this.gfn_transaction(tranObj);
};


