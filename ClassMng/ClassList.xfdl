	this.Div02.form.Div01.form.Static00.set_text("강좌목록 (0개)");
	
	// 코드 데이터
	this.fn_code_list();

	//강의 학기정보 조회
	this.fn_sesterInfo();
	this.fn_year_add();
	this.fn_semester_set();
	
	this.searchFlag = 0;
	this.Div02.form.Div01.form.Grid00.setCellProperty("head", 0, "text", 0);
	this.ds_class_list.clearData();
	
	this.gridOrderInit(this.Div02.form.Div01.form.Grid00);
}

/**************************************************************************
 * 트랜잭션 callback
**************************************************************************/
this.fnCallback = function(svcID, errorCode, errorMsg)
{
  // 에러 시 화면 처리 내역
  if(errorCode != 0)
  {
    this.alert(errorCode+"\n"+errorMsg);
  }
  else 
  {
	 switch(svcID)
	  {
		case "codeList":
			this.fn_classCtgy_set();
			this.fn_classRoom_set();
			this.fn_classDispIcon_set();
		break;
		
		case "classList":
			this.Div02.form.Div01.form.Static00.set_text("강좌목록 (" + this.ds_class_list.getRowCount() + "개)");
			this.searchFlag = 1;
			this.ds_classRoom.copyData(this.ds_classRoom_list);
			this.ds_classRoomTemp.copyData(this.ds_classRoom_list);		
		break;
		
		case "classUpdateList":
			alert("저장되었습니다.");
			this.Div02.form.Div00.form.Div00.form.Div00_00_00_00.form.Button00.click();			
		break;
		
		case "classSaveCopyList":
			alert("복사등록이 완료 되었습니다.");	
			this.Div02.form.Div00.form.Div00.form.Div00_00_00_00.form.Button00.click();
		break;
		
		case "classSaveApvlList":
			alert("승인 되었습니다.");
			this.Div02.form.Div00.form.Div00.form.Div00_00_00_00.form.Button00.click();
		break;
		
		case "setPsnlztn":
			this.alert("저장되었습니다.");
		break;
		
		case "getPsnlztn":
			var format = this.ds_grid_psnlztn.getColumn(0, "format");		
			if(format !== undefined){			
				this.Div02.form.Div01.form.Grid00.set_formats(format);
			}
		break;
	  }
  }
};

