this.fn_code_list = function()
{	
	var outData = "ds_dw_list=dwList ds_semester_list=semesterList ds_classState_list=classStateList ds_classCtgy_list=classCtgyList ";
		outData += "ds_classGb_list=classGbList ds_classAprvState_list=classAprvStateList ds_classTargetGb_list=classTargetGbList";
	
	var tranObj = {
		strSvcId : "codeList",
		strSvcUrl : this.application.Class_svc + "::codeList.do",
		inData : "",
		outData : outData,
		strArg : "cdId=008||034||CODE_64||CODE_65||CODE_66||CODE_67||CODE_68",
		callBackFnc : "fnCallback",
		isAsync : false
	}

	this.gfn_transaction(tranObj);
};

this.fn_class_list = function()
{
	var strSvcId    = "classList";
	var strSvcUrl   = this.application.Class_svc + "::classList.do";
	var inData      = "";
	var outData     = "ds_class_list=classList";	
	var strArg      = "";
	var callBackFnc = "fnCallback";
	var isAsync     = false;

	this.transaction(strSvcId ,   // transaction을 구분하기 위한 svc id값
            strSvcUrl ,       // trabsaction을 요청할 주소
            inData ,         // 입력값으로 보낼 dataset id , a=b형태로 실제이름과 입력이름을 매칭
            outData ,         // 처리결과값으로 받을 dataset id, a=b형태로 실제이름과 입력이름을 매칭
            strArg,         // 입력값으로 보낼 arguments, a=b
            callBackFnc,       // transaction의 결과를 받을 Function 이름
            isAsync);         // 비동기통신 여부 [생략가능]
}

this.fnCallback = function(svcID, errorCode, errorMsg)
{
  // 에러 시 화면 처리 내역
  if(errorCode != 0)
  {
    this.alert(errorCode+"\n"+errorMsg);
  }

  switch(svcID)
  {
    case "codeList":
		this.fn_year_add();
		this.fn_classCtgy_add();
		this.fn_classState_add();
		this.fn_classGb_add();
		this.fn_classAprvState_add();
		this.fn_semester_add();
		this.fn_dw_add();
		this.fn_click_add(this);
	break;
  }
};

//강좌 유형 추가
this.fn_classCtgy_add = function()
{
	var content = ["chkCtgyAll"];
	var list = this.ds_classCtgy_list;
	
	for(var i=0; i < list.getRowCount(); i++) 
	{
		var id = "chkCtgy" + i;
		var objComp = new CheckBox();
		objComp.init(id, content[i] + ":10", 0, 90, 31);
		this.Div00.form.Div00_00.form.Div00_00_04.form.addChild(id, objComp);
		objComp.set_text(list.getColumn(i, "cdNm"));		
		objComp.set_truevalue(list.getColumn(i, "cd"));
		objComp.set_falsevalue("N");
		objComp.set_fittocontents("width");
		objComp.show();	
		content.push(id);
	}
}

