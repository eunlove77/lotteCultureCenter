//학기 설정
this.fn_semester_set = function() 
{
	if(this.lectSmsterCd != "")
	{
		var lectSmsterCd = nexacro.toNumber(this.lectSmsterCd);
		this.Div02.form.Div00.form.Div00.form.Div01.form.Combo01.set_index(lectSmsterCd-1);
	}
	else
	{
		var month = new nexacro.Date().getMonth()+1;
	
		if(3 <= month && month <= 5)
		{
			//봄
			this.Div02.form.Div00.form.Div00.form.Div01.form.Combo01.set_index(0);			
		}
		else if(6 <= month && month <= 8)
		{
			//여름
			this.Div02.form.Div00.form.Div00.form.Div01.form.Combo01.set_index(1);
		}
		else if(9 <= month && month <= 11)
		{
			//가을
			this.Div02.form.Div00.form.Div00.form.Div01.form.Combo01.set_index(2);
		}	
		else
		{
			//겨울
			this.Div02.form.Div00.form.Div00.form.Div01.form.Combo01.set_index(3);
		}
	}
}

