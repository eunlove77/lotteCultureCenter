this.set_teacher_info_rrno = function(){
	var rrno = "";
	var tcTpCd = this.ds_teacher_info.getColumn(0, 'tcTpCd');
	
	if(tcTpCd != "3"){
		var obj_jumin1 = this.Div_PayInfo.form.Div_p.form.Div00.form.rrno1.value;
		var obj_jumin2 = this.Div_PayInfo.form.Div_p.form.Div00.form.rrno2.value;
		
		obj_jumin1 = typeof obj_jumin1 == "undefined" ? "" : obj_jumin1;
		obj_jumin2 = typeof obj_jumin2 == "undefined" ? "" : obj_jumin2;
		
		rrno = obj_jumin1 + obj_jumin2;
	} else {
		rrno = this.Div_PayInfo.form.Div_b.form.Div00.form.rrno.value;
	}
	
	this.ds_teacher_info.setColumn(0, 'rrno', rrno);
}
 
/**************************************************************************
 * 유효성 검사
 **************************************************************************/
this.validate_teacher = function(){

	if(!this.validate_teacher_basic()) return false;
	
	
	if(!this.validate_pay_info()) return false;	//지급정보
	
	if(!this.validate_teacher_edu()) return false;
	if(!this.validate_teacher_hist()) return false;
	if(!this.validate_teacher_awrd()) return false;
	if(!this.validate_teacher_athctf()) return false;
	
	return true;
}
this.validate_teacher_basic = function(){

	// 강사 기본정보 검사
	var ds = this.ds_teacher_info;
	// 조건별 필수항목
	var tcNm = "강사명을";
	var bday = ['bday', '생년월일을'];
	var tcTpCd = ds.getColumn(0, "tcTpCd");
	if(tcTpCd == 2){
		tcNm = "사업자명을";
	} else if(tcTpCd == 3){
		tcNm = "사업자명을";
		bday = null;
	}
	var vali_arr = [
		['tcNm', tcNm], ['hpStNo', '휴대전화를'], ['hpMidNo', '휴대전화를'], ['hpLastNo', '휴대전화를'],
		bday, ['addr', '주소를'], ['prinfClctAgrYn', '개인정보 수집 제공 동의를']
	]
	
	for(var vali of vali_arr){
		var value = ds.getColumn(0, vali[0]);
		if(this.gfn_nvl(value) == "" || nexacro.trim(value) == "" || (vali[0] == 'prinfClctAgrYn' && value == 'N')){
			alert(vali[1] + " 입력하세요.");
			return false;
		}
	}

	//전화번호 - 하나입력하면 체크
	var msg = "";
	var isInput = false;
	var vali_arr2 = [
		['telStNo', '전화번호를'], ['telMidNo', '전화번호를'], ['telLastNo', '전화번호를'],
	]
	for(var vali of vali_arr2){
		if(this.gfn_nvl(ds.getColumn(0, vali[0])) == "" || nexacro.trim(ds.getColumn(0, vali[0])) == ""){
			msg = vali[1] + " 입력하세요.";
		} else {
			isInput = true;
		}
	}
	if(isInput && msg){
		alert(msg);
		return false;
	}
	
	return true;
}
//지급정보
this.validate_pay_info = function(){
	var msg = "";
	var pass = false;
	
	var tcTpCd = this.ds_teacher_info.getColumn(0, "tcTpCd");
	if(tcTpCd != "3"){
		var obj_jumin1 = this.Div_PayInfo.form.Div_p.form.Div00.form.rrno1.value;
		var obj_jumin2 = this.Div_PayInfo.form.Div_p.form.Div00.form.rrno2.value;
		
		if(!obj_jumin1 && !obj_jumin2){
			msg = "주민등록번호를 입력하세요.";
			pass = true;
		}
		else if(!obj_jumin1 || !obj_jumin2){
			msg = "주민등록번호를 입력하세요.";
		}
		else if(!this.isRrnoDupChk){
			alert("주민등록번호 중복체크 후 저장하세요.");
			return false;
		}
		
	} else {
		var rrno = this.Div_PayInfo.form.Div_b.form.Div00.form.rrno.value;
		if(!rrno){
			msg = "사업자등록번호를 입력하세요.";
			pass = true;
		}
		else if(!this.isRrnoDupChk){
			alert("사업자번호 중복체크 후 저장하세요.");
			return false;
		}
	}
	
	//계좌정보
	var bnkNm = this.Div_PayInfo.form.Div01.form.bnkNm.value;
	var dpstNm = this.Div_PayInfo.form.Div01.form.Div00.form.dpstNm.value;
	var accNo = this.Div_PayInfo.form.Div01.form.Div00.form.accNo.value;
	if(pass && !bnkNm && !dpstNm && !accNo){ //아무것도 적지 않으면
		return true;
	}
	if(!pass && bnkNm && dpstNm && accNo){ //모두 통과
		return true;
	} 
	else if(msg){
		alert(msg);
	} else {
		alert("계좌정보를 입력하세요");
	}
	
	return false;
}

//학력
this.validate_teacher_edu = function(type){
	var vali_arr= [['yy', '연도를'], ['schlNm', '학교명을'], ['mjrNm', '전공을']];
	return this.validate_teacher_ds_list("ds_tcedu_list", vali_arr, type);
}
//경력
this.validate_teacher_hist = function(type){
	var vali_arr= [['histStDt', '기간을'], ['histEndDt', '기간을'], ['histPlcNm', '기관명을']]
	return this.validate_teacher_ds_list("ds_tchist_list", vali_arr, type);
}
//수상
this.validate_teacher_awrd = function(type){
	var vali_arr= [['issueDt', '기간을'], ['issueAgncNm', '기관명을'], ['issueNm', '수상내역을']]
	return this.validate_teacher_ds_list("ds_tcawrd_list", vali_arr, type);
}
//자격
this.validate_teacher_athctf = function(type){
	var vali_arr= [['issueDt', '기간을'], ['issueAgncNm', '기관명을'], ['issueNm', '자격증명을']];
	return this.validate_teacher_ds_list("ds_tcathctf_list", vali_arr, type);
}

this.validate_teacher_ds_list = function(ds, vali_arr, type){
	var isInput = false;
	var msg = "";
	var ds = this[ds];
	for(var i = 0; i < ds.rowcount; i++){
		for(var vali of vali_arr){
			if(vali && (this.gfn_nvl(ds.getColumn(i, vali[0])) == "" || nexacro.trim(ds.getColumn(i, vali[0])) == "")){
				msg = vali[1] + " 입력하세요.";
			} else {
				isInput = true;
			}
		}
		if(isInput && msg){
			//하나라도 입력했다면 검사대상
			alert(msg);
			return false;
		}
	}
	
	if(type == "addRow" && ds.rowcount == 1 && !isInput){
		//항목 추가시 입력값 없다면
		alert(vali_arr[0][1] + " 입력하세요.");
		return false;
	} else if(ds.rowcount == 1 && !isInput) {
		//저장시 입력값 전혀 없을 경우 로우 삭제
		ds.deleteRow(0);
	}
	
	return true;
}



