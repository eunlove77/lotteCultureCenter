this.get_file_detail = function(ds, svcId){
	var tranObj = {};
	tranObj.strSvcId	= svcId ? svcId : "get_fileDtl";
	tranObj.strSvcUrl   = this.application.Admin_svc + "::teacherMng/selectFileDtl.do";
	tranObj.inData      = "file_detail=" + ds;
	tranObj.outData     = ds + "=file_detail";
	tranObj.strArg      = "";
	tranObj.callBackFnc = "fnCallback";
	tranObj.isAsync     = true;
	tranObj.menuSeq 	= this.menuSeq;
	tranObj.isPopup     = true;
	
  if(this.gfn_btn_auth(this.menuSeq, "accSrchYn")) return;
	this.gfn_transaction(tranObj);
}

this.fnCallback = function(svcId, errorCode, errorMsg){
	if(errorCode != 0){
		this.alert(errorCode+"\n"+errorMsg);
		return;
	}
	
	if(svcId == "select_teacher"){
		this.set_teacher_info();
		
		//기본정보 사진파일 가져오기
		var phtFileId = this.ds_teacher_info.getColumn(0, 'phtFileId');
		if(phtFileId){
			this.ds_thnl_dtl.setColumn(0, 'atchFileId', phtFileId);
			this.get_file_detail('ds_thnl_dtl', 'ds_thnl_dtl');
		}
		
		//서명정보 사진파일 가져오기
		var signImgFileId = this.ds_teacher_info.getColumn(0, 'signImgFileId');
		if(signImgFileId){
			this.ds_sign_dtl.setColumn(0, 'atchFileId', signImgFileId);
			this.get_file_detail('ds_sign_dtl', 'ds_sign_dtl');
		}
		
		
	
		/*this.set_screen_view_after();*/
		
	}
	else if(svcId == 'ds_thnl_dtl'){
		this.Div01.form.Div00.form.ImageViewer00.set_text('');
	}
	else if(svcId == "get_lect_plan"){
		
		this.fn_callback_lectureplan_getFile();
		
	}
	else if(svcId == "update_tcmdfreq_complete"){
		alert("요청사항 완료 처리되었습니다.");
		this.list_teacher_modifyRequest();
		return;
	}
	else if(svcId == "update_tcmdfreq_reject"){
		alert("요청사항 반려 처리되었습니다.");
		this.list_teacher_modifyRequest();
		return;
	}
	else if(svcId == "duplication_check_rrno"){
		var dupYn = this.ds_rrno.getColumn(0, "dupCnt") > 0 ? 'Y' : 'N';
		var rrnoNm = this.ds_teacher_info.getColumn(0, "tcTpCd") != "3" ? "주민등록번호" : "사업자번호";
		if(dupYn == "Y"){
			alert("동일한 "+rrnoNm+"가 존재합니다.");
			this.isRrnoDupChk = false;
		} else {
			alert("사용 가능한 "+rrnoNm+"입니다.");
			this.isRrnoDupChk = true;
		}
	}
	else if(svcId == "update_teacher"){
		alert("저장되었습니다.");
		this.close();
	}
	else if(svcId == "confirm_teacher"){
		alert("승인되었습니다.")
		this.close();
	}
}
