    <Script type="xscript5.1"><![CDATA[/**************************************************************************
 * include 영역 
 **************************************************************************/
include "Base::libCommon.xjs";
include "Base::lib_base.xjs";
include "Base::lib_date.xjs";
include "Base::popup.xjs";
include "TeacherMng::include/common.xjs";

/**************************************************************************
 * FORM 변수 선언 영역
 **************************************************************************/
this.application = null;
this.domain = "";
this.rowcount = 0;
this.fileExtn = "jpg,jpeg,gif,png"; //확장자
this.fileMaxSize= 200; 	//최대 파일크기 200Kb
this.age = 0;
this.menuSeq = 0;

this.isRrnoDupChk = false; //지급정보 중복체크

/**************************************************************************
 * FORM EVENT 영역(onload)
 **************************************************************************/
this.TeacherDtl_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	
	this.domain = this.application.getVariable("domain");
	this.menuSeq = this.parent.menuSeq;

	//그리드의 체크박스를 Dataset 없이 사용하는 함수 실행
	this.gfnSetCheckBoxNoDataset(this.ds_tcmdfreq_list, this.Div03.form.tcmdfreq_Grid, 0);
	
	//강사정보 세팅
	this.ds_teacher_info.setColumn(0, 'tcCdNo', this.parent.tcCdNo);
	this.ds_memo.setColumn(0, 'tcCdNo', this.parent.tcCdNo);
	
	this.select_teacher_info();
	
	this.set_dataset();
	
	this.set_screen_view();
	
};
/**************************************************************************
 * 강사정보 불러오기
 **************************************************************************/
this.select_teacher_info = function(){

	var outData = 'ds_teacher_info=tcDtl ' +
				'ds_tcedu_list=eduList ' +
				'ds_tchist_list=histList ' +
				'ds_tcawrd_list=awrdList ' +
				'ds_tcathctf_list=authctfList ' +
				'ds_tcmdfreq_list=tcmdfreqList ' +
				'ds_tclect_dtl=tclectDtl ';

	var tranObj = {};
	tranObj.strSvcId	= "select_teacher";
	tranObj.strSvcUrl   = this.application.Admin_svc + "::teacherMng/dtl.do";
	tranObj.inData      = "search=ds_teacher_info";
	tranObj.outData     = outData;
	tranObj.strArg      = "";
	tranObj.callBackFnc = "fnCallback";
	tranObj.isAsync     = true;
	tranObj.menuSeq 	= this.menuSeq;
	tranObj.isPopup     = true;
	
  if(this.gfn_btn_auth(this.menuSeq, "accSrchYn")) return;
	this.gfn_transaction(tranObj);
}

//강사정보 세팅
this.set_teacher_info = function(){

	//승인여부
	var apvlCd = this.ds_teacher_info.getColumn(0, 'apvlStatCd');
	//강사유형
	var tcTpCd = this.ds_teacher_info.getColumn(0, 'tcTpCd');

	this.set_teacher_basic_info(apvlCd, tcTpCd);	//기본정보
	
	this.set_teacher_pay_info(tcTpCd);	//지급정보

	this.set_teacher_lecture_info();	//출강정보
	
	this.set_screen_view_after();	//데이터 수신이후
	
}
this.set_teacher_basic_info = function(apvlCd, tcTpCd){
	//강사유형 라디오
	this.ds_tcTp_cdList.filter('cd ==' + tcTpCd);
	
	if(tcTpCd != '1'){
		//사업자
		var div_txfpBizpr = this.Div01.form.Div01.form.Div_txfpBizprYn; //면세 체크 div
		var tcNm = this.Div01.form.Div02.form.Div00.form.Static00; //강사명, 사업자명 변경
		var bday = this.Div01.form.Div04.form.Div00.form.Static00 //생년월일 필수입력 변경
		
		div_txfpBizpr.set_visible(true);

		tcNm.set_text("사업자명 <fc v ='#ED5757'>*</fc>");
		if(tcTpCd == '2'){
			//개인사업자
			bday.set_text("생년월일 <fc v ='#ED5757'>*</fc>");
		} else {
			bday.set_text("생년월일");
		}
	}
	
	//이메일
	var emailAddrCd = this.ds_teacher_info.getColumn(0, 'emailAddrCd');
	if(emailAddrCd){
		this.Div01.form.Div05.form.Div00.form.Edit00_00.set_readonly(true);
	}
	
	//섭외정보
	var pbrlRow = this.ds_spot_list.findRow("cd", this.ds_teacher_info.getColumn(0, 'pbrltnBrchCd'));
	var pbrltnBrchCdNm = this.ds_spot_list.getColumn(pbrlRow, 'cdNm');
	var pbrlId = this.ds_teacher_info.getColumn(0, 'pbrltnChrgprId')
	this.Div01.form.Div04.form.Div01.form.pbrltnInfo.set_text(pbrltnBrchCdNm + "(" + pbrlId + ")");
	
	//등록자
	var rgpr_info = this.ds_teacher_info.getColumn(0, 'rgprId') + '(' + this.ds_teacher_info.getColumn(0, 'rgprNm') + ')';
	this.Div06.form.Div00.form.Div00.form.rgprInfo.set_text(rgpr_info);
	var mdf_info = this.ds_teacher_info.getColumn(0, 'mdfId') + '(' + this.ds_teacher_info.getColumn(0, 'mdfNm') + ')';
	this.Div06.form.Div01.form.Div00.form.mdfInfo.set_text(mdf_info);
	
	//등록, 수정일시 소수점 초 제거
	var rgprDtm = this.ds_teacher_info.getColumn(0, 'rgprDtm');
	var mdfDtm = this.ds_teacher_info.getColumn(0, 'mdfDtm');
	if(rgprDtm){
		this.ds_teacher_info.setColumn(0, 'rgprDtm', rgprDtm.split(".")[0]);
	}
	if(mdfDtm){
		this.ds_teacher_info.setColumn(0, 'mdfDtm', mdfDtm.split(".")[0]);
	}
	
	
}
/**************************************************************************
 * 지급정보
 **************************************************************************/
this.rrno_onchanged = function(obj:nexacro.Edit,e:nexacro.ChangeEventInfo)
{
	this.isRrnoDupChk = false;
}
this.set_teacher_pay_info = function(apvlCd, tcTpCd){

	var rrno = this.ds_teacher_info.getColumn(0, "rrno");
	if(rrno){
		this.isRrnoDupChk = true;
	}
	
	if(tcTpCd != '3'){
		//개인
		this.Div_PayInfo.form.Div_b.set_height(0);
 		var top = this.Div_PayInfo.form.Div01.getOffsetTop();
 		this.Div_PayInfo.form.Div01.setOffsetTop(Number(top)-33);
		
		//주민번호
		if(rrno){
			this.Div_PayInfo.form.Div_p.form.Div00.form.rrno1.set_value(rrno.substring(0, 6));
			this.Div_PayInfo.form.Div_p.form.Div00.form.rrno2.set_value(rrno.substring(6));
		}
		
	} else {
		//법인
		this.Div_PayInfo.form.Div_p.set_height(0);
	}

	this.resetScroll();
}
//주민번호 중복체크 
