    <Script type="xscript5.1"><![CDATA[/**************************************************************************
 * include 영역 
 **************************************************************************/
include "Base::libCommon.xjs";

/**************************************************************************
 * FORM 변수 선언 영역
 **************************************************************************/
this.application = null;


/**************************************************************************
 * FORM EVENT 영역(onload)
 **************************************************************************/
this.SystemUseLog_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	this.objPageingDs = this.application.gdsPageing;	
	
	this.txtMenuNm.set_text(obj.parent.parent.fvMenuNM);	
	
	// 코드 데이터
	this.fn_data_list();
	
	//this.fn_list();	
};


this.Div05_btnSearch_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.fn_searchValueInit();
	
	this.fn_list(1);	
};


this.Div05_searchQuery_onkeydown = function(obj:nexacro.Edit,e:nexacro.KeyEventInfo)
{
	if(e.keycode == 13){
		this.fn_searchValueInit();
		
		this.fn_list(1);
	}
	
};


this.Div05_btnInit_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{	
	var objCalendar = this.Div02.form.Div00.form;
	objCalendar.strtDtm.set_value('');
	objCalendar.endDtm.set_value('');
	
	this.row02.form.Div03.form.chkAll.set_value(0);
	this.row02.form.Div03.form.chkC.set_value(0);
	this.row02.form.Div03.form.chkU.set_value(0);
	this.row02.form.Div03.form.chkD.set_value(0);
	this.row02.form.Div03.form.chkR.set_value(0);
	this.row02.form.Div03.form.chkE.set_value(0);
	
	this.Div05.form.searchField.set_value('');
	this.Div05.form.searchQuery.set_value('');
	
	this.ds_log.clearData();
	this.txtTotal.set_text("총 0개");				
	
	this.pageingDiv.set_visible(false);
	
};


this.row02_Div03_chkAll_onclick = function(obj:nexacro.CheckBox,e:nexacro.ClickEventInfo)
{
	if(obj.isChecked()){
		obj.parent.chkC.set_value(1);
		obj.parent.chkU.set_value(1);
		obj.parent.chkD.set_value(1);
		obj.parent.chkR.set_value(1);
		obj.parent.chkE.set_value(1);		
	}else{
		obj.parent.chkC.set_value(0);
		obj.parent.chkU.set_value(0);
		obj.parent.chkD.set_value(0);
		obj.parent.chkR.set_value(0);
		obj.parent.chkE.set_value(0);
	}
};



/**************************************************************************
 * 사용자 FUNCTION 영역
 **************************************************************************/
 
this.fn_list = function(page){
	
	this.objPageingDs.setColumn(0, "pageIndex", page);
	
	var tranObj = {
		strSvcId : "list",
		strSvcUrl : this.application.Admin_svc + "::syslog/use/list.do",
		inData : "search=ds_search pageing=gdsPageing",
		outData : "ds_log=output1 gdsPageing=output2",
		strArg : "",
		callBackFnc : "fn_callback",
		isAsync : true		
	};
	
	this.gfn_transaction(tranObj);
}
 


this.fn_data_list = function ()
{	
	var strSvcId    = "dataList";
	var strSvcUrl   = this.application.Admin_svc + "::syslog/getCommonDataList.do";
	var inData      = "";
	var outData     = "ds_spot_list=output1";
	var strArg      = "";
	var callBackFnc = "fn_callback";
	var isAsync     = true;

	this.transaction(strSvcId ,   // transaction을 구분하기 위한 svc id값
            strSvcUrl ,       // trabsaction을 요청할 주소
            inData ,         // 입력값으로 보낼 dataset id , a=b형태로 실제이름과 입력이름을 매칭
            outData ,         // 처리결과값으로 받을 dataset id, a=b형태로 실제이름과 입력이름을 매칭
            strArg,         // 입력값으로 보낼 arguments, a=b
            callBackFnc,       // transaction의 결과를 받을 Function 이름
            isAsync);         // 비동기통신 여부 [생략가능]
};

this.fn_callback = function(svcID,errorCode,errorMsg){
	
	if(errorCode != 0)
	{    
		this.alert(errorCode+"\n"+errorMsg);
		return;
	}

	switch(svcID)
	{    
		case "list":			
			this.txtTotal.set_text("총 " + this.objPageingDs.getColumn(0, "totCnt")  + "개");					
			
			if(this.ds_log.rowcount < 1){
				//this.alert("조회된 결과가 없습니다.");
				this.Grid00.set_nodatatext("조회된 결과가 없습니다.")
				this.pageingDiv.set_visible(false);
			}else{
			 
				var nAllRowCount = this.objPageingDs.getColumn(0, "totCnt");
				var nPageRowCount = this.objPageingDs.getColumn(0, "pageRowCnt");
				var pageIndex = this.objPageingDs.getColumn(0, "pageIndex");
				
				this.pageingDiv.form.initPages(nAllRowCount, nPageRowCount);
				this.pageingDiv.form.setPage(pageIndex);
				
				for(var i = 0 ; i < this.ds_log.rowcount ; i++){
					this.ds_log.setColumn(i, 0, ( nAllRowCount - (( pageIndex - 1 ) * nPageRowCount)) - i );
				}	
			}

		break; 
		case "dataList":
			this.row02.form.Div04.form.ComboSpot.set_index(0);
		break;
	}
}

this.fn_searchValueInit = function(){
	var objCalendar = this.Div02.form.Div00.form;
	var arrActivityType = new Array();
	this.ds_search.setColumn(0, "strtDtm", objCalendar.strtDtm.value);
	this.ds_search.setColumn(0, "endDtm", objCalendar.endDtm.value);
	
	if(this.row02.form.Div03.form.chkC.value == true) arrActivityType.push("'C'");
	if(this.row02.form.Div03.form.chkU.value == true) arrActivityType.push("'U'");
	if(this.row02.form.Div03.form.chkD.value == true) arrActivityType.push("'D'");
	if(this.row02.form.Div03.form.chkR.value == true) arrActivityType.push("'R'");
	if(this.row02.form.Div03.form.chkE.value == true) arrActivityType.push("'E'");
	
	this.ds_search.setColumn(0, "activityType", arrActivityType.join(","));
	this.ds_search.setColumn(0, "spotCd", this.row02.form.Div04.form.ComboSpot.value);
	this.ds_search.setColumn(0, "f", this.Div05.form.searchField.value);
	this.ds_search.setColumn(0, "q", this.Div05.form.searchQuery.value);
}

this.fn_checkbox_click = function(obj:nexacro.CheckBox,e:nexacro.ClickEventInfo){	
	if(!obj.isChecked()){
		this.row02.form.Div03.form.chkAll.set_value(0);
	}
}
]]></Script>
