        <Div id="Div01" taborder="5" text="Div01" left="10" top="194" height="49" right="10">
          <Layouts>
            <Layout>
              <Static id="txtTotal" taborder="0" left="9" top="10" height="21" text="목록 (0)개" right="1169"/>
              <Button id="btnUseN" taborder="1" text="미노출" top="5" height="30" cssclass="btn_WF_High" right="149" width="60" onclick="Div01_btnPerminN_onclick"/>
              <Button id="btnIns" taborder="2" text="등록" top="5" height="30" width="50" cssclass="btn_WF_Medium" right="80" onclick="Div01_btnIns_onclick"/>
              <Button id="btnUseY" taborder="3" text="노출" top="5" height="30" cssclass="btn_WF_Medium" width="50" right="226" onclick="Div01_btnSave_onclick" visible="true"/>
              <Button id="btnDel" taborder="4" text="삭제" top="5" height="30" cssclass="btn_WF_High" width="50" onclick="Div01_btnDel_onclick" right="10"/>
            </Layout>
          </Layouts>
        </Div>
        <Div id="permitDiv" taborder="6" text="Div01" height="33" cssclass="div_WF_CompWrap" top="114" left="10" right="10">
          <Layouts>
            <Layout>
              <Static id="Static00" taborder="0" text="노출상태" left="0" top="0" width="109" height="31" usedecorate="true" cssclass="sta_WF_TableLabel" onclick="Div00_Div00_Static00_onclick"/>
              <CheckBox id="chkPermitAll" taborder="1" text="전체" left="116" top="4" width="45" height="20" onclick="row03_permitDiv_permitAll_onclick" value="false"/>
              <CheckBox id="chkPermitY" taborder="2" text="노출" left="chkPermitAll:3" top="4" width="60" height="20" value="false" onclick="fn_checkbox_click02"/>
              <CheckBox id="chkPermitN" taborder="3" text="미노출" left="chkPermitY:3" top="4" width="70" height="20" value="false" onclick="fn_checkbox_click02"/>
            </Layout>
          </Layouts>
        </Div>
      </Layout>
    </Layouts>
    <Objects>
      <Dataset id="ds_visual_list">
        <ColumnInfo>
          <Column id="seqno" type="STRING" size="256"/>
          <Column id="tit" type="STRING" size="256"/>
          <Column id="expsrStDtm" type="STRING" size="256"/>
          <Column id="expsrEndDtm" type="STRING" size="256"/>
          <Column id="alwsExpsrYn" type="STRING" size="256"/>
          <Column id="expsrYn" type="STRING" size="256"/>
          <Column id="rgprId" type="STRING" size="256"/>
          <Column id="rgprDtm" type="STRING" size="256"/>
          <Column id="mdfId" type="STRING" size="256"/>
          <Column id="mdfDtm" type="STRING" size="256"/>
          <Column id="chk" type="STRING" size="256"/>
        </ColumnInfo>
      </Dataset>
      <Dataset id="ds_search">
        <ColumnInfo>
          <Column id="strtDtm" type="STRING" size="256"/>
          <Column id="endDtm" type="STRING" size="256"/>
          <Column id="permitType" type="STRING" size="256"/>
          <Column id="f" type="STRING" size="256"/>
          <Column id="q" type="STRING" size="256"/>
        </ColumnInfo>
        <Rows>
          <Row/>
        </Rows>
      </Dataset>
    </Objects>
    <Script type="xscript5.1"><![CDATA[
/**************************************************************************
 * include 영역 
 **************************************************************************/
include "Base::libCommon.xjs";

/**************************************************************************
 * FORM 변수 선언 영역
 **************************************************************************/
this.application = null;

/**************************************************************************
 * FORM EVENT 영역(onload)
 **************************************************************************/
this.VisualMng_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	
	var objOwnerFrame = this.getOwnerFrame();
	
	this.txtMenuNm.set_text(obj.parent.parent.fvMenuNM);
	
	// 코드 데이터
	//this.fn_data_list();
	
	this.fn_list(1);
};

//리스트 검색버튼
this.Div06_btnSearch_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{

	this.fn_searchValueInit();
	
	this.fn_list(1);
};

//엔터키 검색
this.Div06_searchQuery_onkeydown = function(obj:nexacro.Edit,e:nexacro.KeyEventInfo)
{
	if(e.keycode ==13)
	{
		this.fn_searchValueInit();
		
		this.fn_list(1);
	}
};

//리스트 초기화 버튼
this.Div06_btnInit_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	
	var objCalendar = this.periodDiv.form.Div00.form; //캘린더
	
	objCalendar.strtDtm.set_value('');
	objCalendar.endDtm.set_value('');
	
	this.Div06.form.searchField.set_index(0);
	this.Div06.form.searchQuery.set_value('');
	
	//this.ds_notice_list.clearData();
	//this.txtTotal.set_text("총 0개");	

	this.permitDiv.form.chkPermitAll.set_value(0);
	this.permitDiv.form.chkPermitY.set_value(0);
	this.permitDiv.form.chkPermitN.set_value(0);
	
};

this.permitDiv_permitAll_onclick = function(obj:nexacro.CheckBox,e:nexacro.ClickEventInfo)
{
	if(obj.isChecked())
		{
			obj.parent.chkPermitY.set_value(1);
			obj.parent.chkPermitN.set_value(1);
		}
		else
		{
			obj.parent.chkPermitY.set_value(0);
			obj.parent.chkPermitN.set_value(0);
		}
};

/**************************************************************************
 * 사용자 FUNCTION 영역
 **************************************************************************/

this.fn_list = function (page)
{	
	
	var strSvcId    = "list";
	var strSvcUrl   = this.application.Admin_svc + "::/main/visualMngList.do";
	var inData      = "search=ds_search";
	var outData     = "ds_visual_list=output1";
	var strArg      = "";
	var callBackFnc = "fnCallback";
	var isAsync     = true;

	this.transaction(strSvcId ,   // transaction을 구분하기 위한 svc id값
            strSvcUrl ,       // trabsaction을 요청할 주소
            inData ,         // 입력값으로 보낼 dataset id , a=b형태로 실제이름과 입력이름을 매칭
            outData ,         // 처리결과값으로 받을 dataset id, a=b형태로 실제이름과 입력이름을 매칭
            strArg,         // 입력값으로 보낼 arguments, a=b
            callBackFnc,       // transaction의 결과를 받을 Function 이름
            isAsync);         // 비동기통신 여부 [생략가능]
	
};

this.fn_searchValueInit = function() {

	//this.alert("검색조건초기화");
	var objCalendar = this.periodDiv.form.Div00.form; //캘린더
	var arrPermitType = new Array();
	
	this.ds_search.setColumn(0, "strtDtm", objCalendar.strtDtm.value);
	this.ds_search.setColumn(0, "endDtm", objCalendar.endDtm.value);
	
	if(this.permitDiv.form.chkPermitY == true) arrPermitType.push("'Y'");
	if(this.permitDiv.form.chkPermitN == true) arrPermitType.push("'N'");
	//권한 처리예정
	
	this.ds_search.setColumn(0, "permitType", arrPermitType.join(","));
	this.ds_search.setColumn(0, "q", this.Div06.form.searchQuery.value);
	this.ds_search.setColumn(0, "f", this.Div06.form.searchField.value);

}

this.fn_checkbox_click02 = function(obj:nexacro.CheckBox,e:nexacro.ClickEventInfo){	
	if(!obj.isChecked()){
		
		this.permitDiv.form.chkPermitAll.set_value(0);
		//this.row03.form.permitDiv.form.chkPermitAll.set_value(0);
	}
}

this.fnCallback = function(svcID,errorCode,errorMsg)
{
  // 에러 시 화면 처리 내역
  if(errorCode != 0)
  {
    this.alert(errorCode+"\n"+errorMsg);
	
	switch(svcID)
	{
		case "save":
			this.ds_visual_list.reset();
			break;
	}
	
    return;
  }

  switch(svcID)
  {
    case "list":
      if(this.ds_visual_list.rowcount < 1){
        this.alert("조회된 결과가 없습니다.");
      }else{
	  
 		this.ds_visual_list.set_enableevent(false);
		for(var i = 0 ; i < this.ds_admin_list.getRowCount() ; i++){
			this.ds_visual_list.addColumn("chk", "String", 256);
			this.ds_visual_list.setColumn(i, "chk","N");
		}
		this.ds_visual_list.set_enableevent(true);
	  }
	case "save":
		this.alert("변경완료");
		break;
  }
};

this.orderType = "desc";
this.Div01_btnIns_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.parent.param = {
		seq : ""
	}
	this.parent.set_url("SystemMng::VisualMngDtl.xfdl");
};
]]></Script>
  </Form>
</FDL>
