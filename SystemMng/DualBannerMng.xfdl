        <Div id="Div00_00" taborder="1" top="0" height="21" url="FrameBase::include/favorite.xfdl" right="0" width="21"/>
        <Static id="Static00" taborder="2" text="▶ 광고 콘텐츠 설정" left="30" top="79" width="120" height="21" cssclass="sta_WF_TitGray"/>
        <Grid id="Grid00" taborder="3" left="30" top="Static00:10" right="30" bottom="173" binddataset="dualBannerDs" autofittype="col" oncellclick="Grid00_oncellclick" cssclass="grd_WF_TableType">
          <Formats>
            <Format id="default">
              <Columns>
                <Column size="40" band="left"/>
                <Column size="50"/>
                <Column size="561"/>
                <Column size="214"/>
                <Column size="150"/>
              </Columns>
              <Rows>
                <Row size="35" band="head"/>
                <Row size="40"/>
              </Rows>
              <Band id="head">
                <Cell displaytype="checkboxcontrol" edittype="checkbox"/>
                <Cell col="1" text="번호"/>
                <Cell col="2" text="제목"/>
