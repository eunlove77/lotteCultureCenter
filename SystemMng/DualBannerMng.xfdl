<?xml version="1.0" encoding="utf-8"?>
<FDL version="2.1">
  <Form id="DualBannerMng" width="1280" height="720" titletext="New Form" onload="DualBannerMng_onload">
    <Layouts>
      <Layout height="720" mobileorientation="landscape" width="1280">
