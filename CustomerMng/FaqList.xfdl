	this.Div00_00.form.conDiv.form.ctgyDiv.form.chkAll.set_value(false);
	this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd1.set_value(false);
	this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd2.set_value(false);
	this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd3.set_value(false);
	this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd4.set_value(false);
	this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd5.set_value(false);
	
	this.searchDs.setColumn(0, "strtDt", "");
	this.searchDs.setColumn(0, "endDt", "");
	this.searchDs.setColumn(0, "clCdAll", "");
	this.searchDs.setColumn(0, "clCdList", "");
	this.searchDs.setColumn(0, "f", 0);
	this.searchDs.setColumn(0, "q", "");
}

this.fn_checkboxAll = function(obj:nexacro.CheckBox,e:nexacro.ClickEventInfo)
{
	if(obj.isChecked()){
		var trueVal1 = Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd1.truevalue;
		var trueVal2 = Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd2.truevalue;
		var trueVal3 = Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd3.truevalue;
		var trueVal4 = Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd4.truevalue;
		var trueVal5 = Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd5.truevalue;
		
		this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd1.set_value(trueVal1);
		this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd2.set_value(trueVal2);
		this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd3.set_value(trueVal3);
		this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd4.set_value(trueVal4);
		this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd5.set_value(trueVal5);
		
	} else {
		this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd1.set_value(false);
		this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd2.set_value(false);
		this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd3.set_value(false);
		this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd4.set_value(false);
		this.Div00_00.form.conDiv.form.ctgyDiv.form.chkClCd5.set_value(false);
	}
};

this.fn_delete = function ()
{		
	var strSvcId    = "delete";
	var strSvcUrl   = this.application.Admin_svc + "::/customer/deleteNotice.do";
	var inData      = "faqList=faqDs:U";
	var outData     = "faqDs=output1";
	var strArg      = "";
	var callBackFnc = "fnCallback";
	var isAsync     = true;

	this.transaction(strSvcId ,   // transaction을 구분하기 위한 svc id값
            strSvcUrl ,       // trabsaction을 요청할 주소
            inData ,         // 입력값으로 보낼 dataset id , a=b형태로 실제이름과 입력이름을 매칭
            outData ,         // 처리결과값으로 받을 dataset id, a=b형태로 실제이름과 입력이름을 매칭
