                    <Band id="head">
                      <Cell text="번호" edittype="checkbox" displaytype="checkboxcontrol"/>
                      <Cell col="1" text="주문번호"/>
                      <Cell col="2" text="실수강자"/>
                      <Cell col="3" text="지점명"/>
                      <Cell col="4" text="강좌유형"/>
                      <Cell col="5" text="학기"/>
                      <Cell col="6" text="강좌코드"/>
                      <Cell col="7" text="강좌명"/>
                      <Cell col="8" text="강사명"/>
                      <Cell col="9" text="옵션명"/>
                      <Cell col="10" text="강좌료"/>
                      <Cell col="11" text="추가금액"/>
                      <Cell col="12" text="오프라인할인"/>
                      <Cell col="13" text="오프라인 할인금액"/>
                      <Cell col="14" text="등급할인명"/>
                      <Cell col="15" text="등급할인금액"/>
                      <Cell col="16" text="쿠폰명"/>
                      <Cell col="17" text="쿠폰 할인금액"/>
                      <Cell col="18" text="추가금액할인"/>
