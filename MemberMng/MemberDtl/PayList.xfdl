              <Static id="Static00" taborder="0" left="0" top="5" height="21" text="결제 내역" width="200"/>
              <Grid id="Grid00" taborder="1" left="0" top="Static00:10" binddataset="ds_coupon_list" autofittype="none" right="0" bottom="0">
