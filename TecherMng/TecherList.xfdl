/**************************************************************************
 * 검색조건 변경 이벤트
 **************************************************************************/
this.techer_type_onchanged = function(obj:nexacro.CheckBox,e:nexacro.CheckBoxChangedEventInfo){
	var value = '';
	var obj_all = this.Div03.form.Div00.form.tcTpCdAll;
	var obj_1 = this.Div03.form.Div00.form.tcTpCd1;
	var obj_2 = this.Div03.form.Div00.form.tcTpCd2;
	var obj_3 = this.Div03.form.Div00.form.tcTpCd3;
	
	switch(obj.name) {
	case 'tcTpCdAll':
		if(obj.value){
			obj_1.set_value('1');
			obj_2.set_value('2');
			obj_3.set_value('3');
		} else {
			obj_1.set_value(false);
			obj_2.set_value(false);
			obj_3.set_value(false);
		}
		break;
	default:
		obj_all.set_value(false);
		if(obj_1.value && obj_2.value && obj_3.value){
			obj_all.set_value(true);
			value = '';
		} else {
			var comma = '';
			if(obj_1.isChecked()){
				value += comma + obj_1.value;
				comma = ',';
			}
			if(obj_2.isChecked()){
				value += comma + obj_2.value;
				comma = ',';
			}
			if(obj_3.isChecked()){
				value += comma + obj_3.value;
			}
		}
	}
	this.ds_search.setColumn(0, 'tcTpCdList', value);
}
this.modify_request_onchanged = function(obj:nexacro.CheckBox,e:nexacro.CheckBoxChangedEventInfo){
	var value = '';
	var obj_chkAll = this.Div04.form.Div00.form.mdfreqAll;
	var obj_chkY = this.Div04.form.Div00.form.mdfreqY;
	var obj_chkN = this.Div04.form.Div00.form.mdfreqN;
	switch(obj.name) {
	case 'mdfreqAll':
		if(obj.value){
			obj_chkY.set_value(true);
			obj_chkN.set_value(true);
		} else {
			obj_chkY.set_value(false);
			obj_chkN.set_value(false);
		}
		value = '';
		break;
	default:
		obj_chkAll.set_value(false);
		if(obj_chkY.value && obj_chkN.value){
			obj_chkAll.set_value(true);
			value = '';
		} else if(obj_chkY.value) {
			value = 'Y';
		} else if(obj_chkN.value){
			value = 'N';
		} else {
			value = '';
		}
	}
	this.ds_search.setColumn(0, 'tcmdfreqYn', value);
}
