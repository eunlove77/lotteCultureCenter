	}
};
/**************************************************************************
 * UTIL
 **************************************************************************/
this.set_combobox_default = function(ds_list){	
	for(var i = 0; i < ds_list.length; i++){
		var info = ds_list[i];
		var ds = this[info.ds];
		ds.insertRow(0);
		ds.setColumn(0, 'cd', '');
		ds.setColumn(0, 'cdNm', info.cdNm);
		
 		this.set_combobox_default_index(info);
	}
}
this.set_combobox_default_index = function(info){
	if(Array.isArray(info.id)){
		for(var id of info.id){
			this.get_componentById(id).set_index(0);
			this.get_componentById(id).set_text(info.cdNm);
		}
	} else {
		this.get_componentById(info.id).set_index(0);
		this.get_componentById(info.id).set_text(info.cdNm);
	}
}
this.get_componentById = function(str){
	var comp = this;
	var list = str.split('.');
	for(var id of list){
		comp = comp[id];
	}
	return comp;
