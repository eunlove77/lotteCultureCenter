 **************************************************************************/
this.TecherList_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	this.txtMenuNm.set_text(obj.parent.parent.fvMenuNM);
	
	//강사유형
	this.Div03.form.Div00.form.tcTpCdAll.set_value(true);
	this.techer_type_onchanged(this.Div03.form.Div00.form.tcTpCdAll);

	this.set_dataset();
	
};
/**************************************************************************
 * 데이터 세팅
 **************************************************************************/
 this.set_dataset = function(){
	//코드 데이터셋 설정
	this.set_code_dataset();
	
	//콤보박스 기본값 설정
	var ds_list = [
		{ds : 'ds_spot_list',	cdNm: '전체',
		 id : ['Div01.form.Div00.form.hopeBrchCd'
			, 'Div03.form.Div01.form.pbrltnBrchCd']},		//지점코드
		{ds : 'ds_day_cdList',	cdNm: '전체',
		 id : 'Div01.form.Div02.form.hopeDaywCd'},			//희망요일
		{ds : 'ds_lgctgry_cdList',cdNm: '대분류',
		 id : 'Div02.form.Div00.form.lrclsCtegryCd'}		//대분류
	];
	this.set_combobox_default(ds_list);
 }
 this.set_code_dataset= function(){
	var ds_codes_list = [
		{ds : 'ds_tcTp_list',		cd: "'CODE_62'"},	//강사유형
		{ds : 'ds_spot_list',	cd: "'spotCd'"},		//지점
		{ds : 'ds_day_cdList',	cd: "'008'"},			//요일
		{ds : 'ds_lgctgry_cdList', 	cd: "'027'"},		//대분류
	];

	for(var i = 0; i < ds_codes_list.length; i++){
		this.set_dataset_from_code(ds_codes_list[i]);
	}
 }
 this.set_dataset_from_code = function(info){
	var gdsCode = this.application.gdsCode;
	gdsCode.filter('cdId != cd && cdId == ' + info.cd);
	this[info.ds].copyData(gdsCode, true);
 }

/**************************************************************************
 * 검색, 초기화
 **************************************************************************/
this.Div05_btnSearch_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.set_search_info();
	this.select_techer_list();
};
this.Div05_btnReset_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.reset_search_form();
	this.select_techer_list();
};
this.set_search_info = function(){
	//검색조건 - 희망시간
	var hopeSt = this.Div02.form.Div01.form.hopeSt.value;
	var hopeEnd = this.Div02.form.Div01.form.hopeEnd.value;
	if(hopeSt && hopeSt.length == 4){
		this.ds_search.setColumn(0, 'hopeStHh', hopeSt.slice(0, 2));
		this.ds_search.setColumn(0, 'hopeStMi', hopeSt.slice(2, 4));
	}
	if(hopeEnd && hopeEnd.length == 4){
		this.ds_search.setColumn(0, 'hopeEndHh', hopeEnd.slice(0, 2));
		this.ds_search.setColumn(0, 'hopeEndMi', hopeEnd.slice(2, 4));
	}
	
	//검색조건 - 휴대전화
	var hpNo = this.Div04.form.Div02.form.hpNo.value;
	var arr = this.parse_phone_number(hpNo);
	
	this.ds_search.setColumn(0, 'hpStNo', arr[0] ? arr[0] : '');
	this.ds_search.setColumn(0, 'hpMidNo', arr[1] ? arr[1] : '');
	this.ds_search.setColumn(0, 'hpLastNo', arr[2] ? arr[2] : '');

}
this.parse_phone_number = function(str){
	if(!str) return [];
	
	var arr = str.split('-');
	if(arr.length > 1){
		if(arr[0].length == 4){
			var tmp = arr[0];
			arr[2] = arr[1];
			arr[1] = arr[2];
		}
	} else {
		if(str.length == 11){
			arr[0] = str.slice(0, 3);
			arr[1] = str.slice(3, 7);
			arr[2] = str.slice(7, 11);
		} else if(str.length == 8){
			arr[1] = str.slice(0, 4);
			arr[2] = str.slice(4, 8);
		}
	}
	
	return arr;
}
this.select_techer_list = function(){
	var tranObj = {};
	tranObj.strSvcId	= "select_techer";
	tranObj.strSvcUrl   = this.application.Admin_svc + "::techerMng/list.do";
	tranObj.inData      = "search=ds_search";
	tranObj.outData     = "ds_techer_list=output1";
	tranObj.strArg      = "";
	tranObj.callBackFnc = "fnCallback_techer_list";
	tranObj.isAsync     = true;
	tranObj.menuSeq 	= this.getOwnerFrame().param.MENU_ID;
	
	this.gfn_transaction(tranObj);
}
this.fnCallback_techer_list = function(svcId, errorCode, errorMsg){
	if(errorCode != 0){
		// 에러 시 화면 처리 내역
		this.alert(errorCode+"\n"+errorMsg);
		return false;
	}
	
}
this.reset_search_form = function(){
	
	this.ds_search.copyData(this.ds_search_default);
	
	//강사유형
	this.Div03.form.Div00.form.tcTpCdAll.set_value(true);
	this.techer_type_onchanged(this.Div03.form.Div00.form.tcTpCdAll);
	
	var ds_list = [{
			ds: 'ds_mdctgry_cdList', cdNm: '중분류', 
			id: 'Div02.form.Div00.form.mdclsCtegryCd'
		}, {
			ds: 'ds_smctgry_cdList', cdNm: '소분류',
			id: 'Div02.form.Div00.form.smclsCtegryCd'
	}];
	this.ds_mdctgry_cdList.clearData();
	this.ds_smctgry_cdList.clearData();
	this.set_combobox_default(ds_list);
	
	//시간
	this.Div02.form.Div01.form.hopeSt.set_value("");
	this.Div02.form.Div01.form.hopeEnd.set_value("");
	//수정요청
	this.Div04.form.Div00.form.mdfreqAll.set_value(true);
	this.Div04.form.Div00.form.mdfreqN.set_value(true);
	this.Div04.form.Div00.form.mdfreqY.set_value(true);
	
	
	//휴대전화
	this.Div04.form.Div02.form.hpNo.set_value("");
}
