		['telStNo', '전화번호를'], ['telMidNo', '전화번호를'], ['telLastNo', '전화번호를'],
	]
	for(var vali of vali_arr2){
		if(this.gfn_nvl(ds.getColumn(0, vali[0])) == "" || nexacro.trim(ds.getColumn(0, vali[0])) == ""){
			msg = vali[1] + " 입력하세요.";
		} else {
			isInput = true;
		}
	}
	if(isInput && msg){
		alert(msg);
		return false;
	}
	
	//이메일 - 하나입력하면 체크
	var email = this.ds_techer_info.getColumn(0, "email");
	var emailAddrCd = this.ds_techer_info.getColumn(0, "emailAddrCd");
	if(email && emailAddrCd){
		
	}
	
	return true;
}
//학력
this.validate_techer_edu = function(){
	var vali_arr= [['yy', '연도를'], ['schlNm', '학교명을'], ['mjrNm', '전공을']];
	return this.validate_techer_ds_list("ds_tcedu_list", vali_arr);
}
//경력
this.validate_techer_hist = function(){
	var vali_arr= [['histStDt', '기간을'], ['histEndDt', '기간을'], ['histPlcNm', '기관명을']]
	return this.validate_techer_ds_list("ds_tchist_list", vali_arr);
}
//수상
this.validate_techer_awrd = function(){
	var vali_arr= [['issueDt', '기간을'], ['issueAgncNm', '기관명을'], ['issueNm', '수상내역을']]
	return this.validate_techer_ds_list("ds_tcawrd_list", vali_arr);
}
//자격
this.validate_techer_athctf = function(){
	var vali_arr= [['issueDt', '기간을'], ['issueAgncNm', '기관명을'], ['issueNm', '자격증명을']];
	return this.validate_techer_ds_list("ds_tcathctf_list", vali_arr);
}

this.validate_techer_ds_list = function(ds, vali_arr){
	var isInput = false;
	var msg = "";
	var ds = this[ds];
	for(var i = 0; i < ds.rowcount; i++){
		for(var vali of vali_arr){
			if(vali && (this.gfn_nvl(ds.getColumn(i, vali[0])) == "" || nexacro.trim(ds.getColumn(i, vali[0])) == "")){
				msg = vali[1] + " 입력하세요.";
			} else {
				isInput = true;
			}
		}
		if(isInput && msg){
			//하나라도 입력했다면 검사대상
			alert(msg);
			return false;
		}
	}
	return true;
}
