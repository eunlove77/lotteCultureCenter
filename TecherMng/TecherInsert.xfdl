this.btnSave_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	//데이터 세팅
	//첨부파일
	this.ds_techer_info.setColumn(0, 'phtFileId', this.ds_thnl_dtl.getColumn(0, 'atchFileId'));
	this.ds_techer_info.setColumn(0, 'signImgFileId', this.ds_sign_dtl.getColumn(0, 'atchFileId'));
	
	//이메일주소
	var emailAddrCd = this.ds_techer_info.getColumn(0, 'emailAddrCd');
	var emailAddr = this.Div01.form.Div05.form.Div00.form.Edit00_00.value;
	if(!emailAddrCd && emailAddr){
		//직접 입력 시 email에 전체 정보
		var email = this.ds_techer_info.getColumn(0, 'email');
		email += "@" + this.Div01.form.Div05.form.Div00.form.Edit00_00.value;
		this.ds_techer_info.setColumn(0, 'email', email);
	}
	
	//유효성 검사
	var vali_res = this.validate_techer();
	if(!vali_res){
		return false;
	}
	
	//submit
	if(confirm("강사정보를 등록 하시겠습니까?")){
		this.delete_empty_row();
		this.submit_techer_info();
	}
};
this.delete_empty_row = function(){
	//필수값이 아닌 그리드 데이터 로우 삭제
	var ds_list = [this.ds_tcedu_list, this.ds_tchist_list, this.ds_tcawrd_list, this.ds_tcathctf_list];
	for(var ds of ds_list)
		this.delete_empty_row_2(ds);
}
this.delete_empty_row_2 = function(ds){
	var isInput = false;
	var isEmpty = false;
	
	var i = ds.rowcount - 1;	//마지막 로우만 체크
	for(var j = 1; j < ds.colcount; j++){ //키값 제외
		if(this.gfn_nvl(ds.getColumn(i, j)) == "" || nexacro.trim(ds.getColumn(i, j) == "")){
			isEmpty = true;
		} else {
			isInput = true;
		}
	}
	if(isEmpty){
		ds.deleteRow(i);
	}
}
this.submit_techer_info = function(){

	var inData = "";
	inData += "techerDtl=ds_techer_info";
	inData += " tceduList=ds_tcedu_list";
	inData += " tchistList=ds_tchist_list";
	inData += " tcawrdList=ds_tcawrd_list";
	inData += " tcathctfList=ds_tcathctf_list";
	
	var tranObj = {};
	tranObj.strSvcId	= "insert_techer";
	tranObj.strSvcUrl   = this.application.Admin_svc + "::techerMng/insert.do";
	tranObj.inData      = inData;
	tranObj.outData     = "";
	tranObj.strArg      = "";
	tranObj.callBackFnc = "fnCallback_techer";
	tranObj.isAsync     = true;
	tranObj.menuSeq 	= this.ownerFrame.param.MENU_ID;
	
	this.gfn_transaction(tranObj);
}
this.fnCallback_techer = function(svcID, errorCode, errorMsg){
	if(errorCode != 0){
		// 에러 시 화면 처리 내역
		this.alert(errorCode+"\n"+errorMsg);
		return;
	}
	alert('등록됐습니다.');
	this.close();
}
