this.grid_button_click = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	var ds = obj.getBindDataset();
	
	if(e.col != obj.getFormatColCount() - 1){
		//버튼은 마지막에 위치
	}
	else if(e.row > 0){
		ds.deleteRow(e.row);
		this.remove_grid_row(obj, ds);
	} else {
		//첫째 row버튼 = 추가
		if(!this.validate_before_addRow(ds)) return false;
		
		var row = ds.addRow();
		this.add_grid_row(obj, ds, row);
	}
};
this.validate_before_addRow = function(ds){
	switch(ds.name) {
	case 'ds_tcedu_list':
		return this.validate_techer_edu();
	case 'ds_tchist_list':
		return this.validate_techer_hist();
	case 'ds_tcawrd_list':
		return this.validate_techer_awrd();
	case 'ds_tcathctf_list':
		return this.validate_techer_athctf();
	}
	return false;
}
this.add_grid_row = function(grid, ds, row){
	if(ds.name == 'ds_tcedu_list'){
		ds.setColumn(row, 'schlClCd', 0);
		ds.setColumn(row, 'grdtClCd', 0);
		ds.setColumn(row, 'yy', '');
	}
	grid.parent.parent.set_height(++row * 33);
	this.Div02.set_height(Number(this.Div02.height) + 33);
	
	this.resetScroll();
}
this.remove_grid_row = function(grid, ds){
	var t_row = grid.rowcount;
	grid.parent.parent.set_height(t_row * 33);
	this.Div02.set_height(Number(this.Div02.height) - 33);
	
	this.resetScroll();
}
