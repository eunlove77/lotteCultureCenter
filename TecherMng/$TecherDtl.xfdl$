};
this.Div03_btnDel_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var check_array = this.ds_tcmdfreq_list.checkarray;
	var checked_array = new Array();
	for(var i = 0; i < check_array.length; i++){
		if(check_array[i] > 0) checked_array.push(i);
	}
	
	if(checked_array.length == 0){
		alert("삭제대상을 선택하세요.");
		return false;
	}
	
	this.ds_tcmdfreq_list.deleteMultiRows(checked_array);
	for(i of checked_array){
		check_array[i] = 0;
	}
	
	this.Div03.form.tcmdfreq_Grid.redrawExprCell();
};
this.techer_modify_request_grid_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo){
	this.ds_atch_file.setColumn(0, "atchFileId", this.ds_tcmdfreq_list.getColumn(0, 'atchFileId'))
	this.gfn_fileDown(this.ds_atch_file, this.FileDownTransfer00);
}
