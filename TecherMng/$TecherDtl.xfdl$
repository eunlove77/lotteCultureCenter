    <Script type="xscript5.1"><![CDATA[/**************************************************************************
 * include 영역 
 **************************************************************************/
include "Base::libCommon.xjs";
include "Base::lib_base.xjs";
include "Base::lib_date.xjs";
include "Base::popup.xjs";

/**************************************************************************
 * FORM 변수 선언 영역
 **************************************************************************/
this.application = null;
this.domain = "";
this.rowcount = 0;
this.fileExtn = "jpg,jpeg,gif,png"; //확장자
this.fileMaxSize= 200; 	//최대 파일크기 200Kb
this.age = 0;
this.rrnoDupChk = false; //지급정보 중복체크
/**************************************************************************
 * FORM EVENT 영역(onload)
 **************************************************************************/
this.TecherDtl_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	
	this.application.getVariable("domain");
	
	//그리드의 체크박스를 Dataset 없이 사용하는 함수 실행
	this.gfnSetCheckBoxNoDataset(this.ds_tcmdfreq_list, this.Div03.form.tcmdfreq_Grid, 0);
	
	//강사정보 세팅
	this.select_techer_info();
	
	this.set_dataset();
	
	this.set_screen_view();
	
};
/**************************************************************************
 * 강사정보 불러오기
 **************************************************************************/
this.select_techer_info = function(){
	this.ds_techer_info.setColumn(0, 'tcCdNo', this.parent.tcCdNo);

	var outData = 'ds_techer_info=tcDtl ' +
				'ds_tcedu_list=eduList ' +
				'ds_tchist_list=histList ' +
				'ds_tcawrd_list=awrdList ' +
				'ds_tcathctf_list=authctfList ' +
				'ds_tclect_dtl=tclectDtl ';

	var tranObj = {};
	tranObj.strSvcId	= "select_techer";
	tranObj.strSvcUrl   = this.application.Admin_svc + "::techerMng/dtl.do";
	tranObj.inData      = "search=ds_techer_info";
	tranObj.outData     = outData;
	tranObj.strArg      = "";
	tranObj.callBackFnc = "fnCallback";
	tranObj.isAsync     = true;
	tranObj.menuSeq 	= this.parent.ownerFrame.param.MENU_ID;
	
	this.gfn_transaction(tranObj);
}

//강사정보 세팅
this.set_techer_info = function(){
	//계약완료 여부
	var apvlCd = this.ds_techer_info.getColumn(0, 'apvlStatCd');
	var tcTpCd = this.ds_techer_info.getColumn(0, 'tcTpCd');

	this.set_techer_basic_info(apvlCd, tcTpCd);	//기본정보
	this.set_techer_pay_info(apvlCd, tcTpCd);	//지급정보
	this.set_techer_lecture_info();	//출강정보
	
}
this.set_techer_basic_info = function(apvlCd, tcTpCd){
	//강사유형 라디오
	this.ds_tcTp_cdList.filter('cd ==' + tcTpCd);
	
	if(tcTpCd != '1'){
		//사업자
		var div_txfpBizpr = this.Div01.form.Div01.form.Div_txfpBizprYn; //면세 체크 div
		var tcNm = this.Div01.form.Div02.form.Div00.form.Static00; //강사명, 사업자명 변경
		var bday = this.Div01.form.Div04.form.Div00.form.Static00 //생년월일 필수입력 변경
		
		div_txfpBizpr.set_visible(true);

		tcNm.set_text("사업자명 <fc v ='#ED5757'>*</fc>");
		if(tcTpCd == '2'){
			//개인사업자
			bday.set_text("생년월일 <fc v ='#ED5757'>*</fc>");
		} else {
			bday.set_text("생년월일");
		}
	}
	
	//등록자
	var rgpr_info = this.ds_techer_info.getColumn(0, 'rgprId') + '(' + this.ds_techer_info.getColumn(0, 'rgprNm') + ')';
	this.Div06.form.Div00.form.Div00.form.rgprInfo.set_text(rgpr_info);
	var mdf_info = this.ds_techer_info.getColumn(0, 'mdfId') + '(' + this.ds_techer_info.getColumn(0, 'mdfNm') + ')';
	this.Div06.form.Div01.form.Div00.form.mdfInfo.set_text(mdf_info);
	
}
this.set_techer_pay_info = function(apvlCd, tcTpCd){
	if(apvlCd == '2'){
		var div = 
		this.Div_PayInfo.set_visible(true);
		
		if(tcTpCd == '1'){
			this.Div_PayInfo.form.Div_b.set_height(0);
		} else {
			this.Div_PayInfo.form.Div_p.set_height(0);
		}
		this.resetScroll();
	}
}
/**************************************************************************
 * 출강정보
 **************************************************************************/
this.set_techer_lecture_info = function(){
	var ds = this.ds_tclect_dtl;
	
	var row, cdNm, obj;
	//희망지점1
	row = this.ds_spot_list.findRow('cd', ds.getColumn(0, 'frstHopeBrchCd'));
	if(Number.isInteger(row)){
		cdNm = this.ds_spot_list.getColumn(row, 'cdNm');
		obj = this.Div05.form.Div00.form.Div00.form.frstHopeBrchCdNm;
		obj.set_text(cdNm);
	}
	//희망지점2
	row = this.ds_spot_list.findRow('cd', ds.getColumn(0, 'secHopeBrchCd'));
	if(Number.isInteger(row)){
		cdNm = this.ds_spot_list.getColumn(row, 'cdNm');
		obj = this.Div05.form.Div00.form.Div01.form.secHopeBrchCdNm;
		obj.set_text(cdNm);
	}
	//희망분야1
	obj = this.Div05.form.Div01.form.Div00.form.frstCtegry;
	var large = ds.getColumn(0, 'frstLrclsCtegryNm');
	var middle = ds.getColumn(0, 'frstMdclsCtegryNm');
	var small = ds.getColumn(0, 'frstSmclsCtegryNm');
	
	cdNm = "";
	if(large) cdNm += large;
	if(middle) cdNm += ' > ' + middle;
	if(small) cdNm += ' > ' + small;
	obj.set_text(cdNm);
	
	//희망분야2
	cdNm = "";
	obj = this.Div05.form.Div01.form.Div01.form.secCtegry;
	large = ds.getColumn(0, 'secLrclsCtegryNm');
	middle = ds.getColumn(0, 'secLrclsCtegryNm');
	small = ds.getColumn(0, 'secSmclsCtegryNm');
	
	if(large) cdNm += large;
	if(middle) cdNm += ' > ' + middle;
	if(small) cdNm += ' > ' + small;
	obj.set_text(cdNm);
	
	//희망요일
	row = this.ds_day_cdList.findRow('cd', ds.getColumn(0, 'hopeDaywCd'));
	if(Number.isInteger(row)){
		cdNm = this.ds_day_cdList.getColumn(row, 'cdNm');
		obj = this.Div05.form.Div02.form.Div00.form.hopeDaywCdNm;
		obj.set_text(cdNm);
	}
	//희망시간
	var str = ""
	obj = this.Div05.form.Div02.form.Div01.form.hopeTime;
	if(Number.isInteger(ds.getColumn(0, 'hopeStHh'))){
		var str = ds.getColumn(0, 'hopeStHh') + ':' + ds.getColumn(0, 'hopeStMi') + ' ~ ';
		str += ds.getColumn(0, 'hopeEndHh') + ':' + ds.getColumn(0, 'hopeEndMi');
	}
	obj.set_text(str);
	
	//강의계획서
	var id = ds.getColumn(0, 'lectPlanFileId');
	if(id){
		this.get_file_detail('ds_lectPlan_dtl');
	} else {
		var obj = this.Div05.form.Div03.form.Div00.form.lectPlanFile;
		obj.set_visible(false);
	}
}
this.fn_callback_lectureplan_getFile = function(){
	var obj = this.Div05.form.Div03.form.Div00.form.lectPlanFile;
	var fileNm = this.ds_lectPlan_dtl.getColumn(0, 'realFileNm');
	if(fileNm){
		this.FileDownTransfer00.set_url(this.domain + "/admin/file/filedownload.do");
		obj.set_text(realFileNm);
	} else {
		obj.set_text("실제 파일이 존재하지 않습니다.");
	}
}
this.Div05_Div03_Div00_lectPlanFile_onclick = function(obj:nexacro.Static,e:nexacro.ClickEventInfo)
{
	this.gfn_fileDown(this.ds_lectPlan_dtl, this.FileDownTransfer00);
};
/**************************************************************************
 * 데이터 세팅
 **************************************************************************/
 this.set_dataset = function(){
	var pbrltnBrchCd = this.application.gdsLoginInfo.getColumn(0, 'brchCd');
	var pbrltnChrgprNm = this.application.gdsLoginInfo.getColumn(0, 'name');
	
	this.ds_techer_info.setColumn(0, 'pbrltnBrchCd', pbrltnBrchCd);
	this.ds_techer_info.setColumn(0, 'pbrltnChrgprNm', pbrltnChrgprNm);
	
	this.set_dataset_from_code();
	this.set_dataset_year();
	
	this.set_dataset_default_value();
 }
 this.set_dataset_from_code = function(){
	var ds_codes_list = [
		{ds : 'ds_tcTp_cdList',		cd: "'CODE_62'"},	//강사유형
		{ds : 'ds_hpStNo_cdList',	cd: "'002'"},		//핸드폰통신사
		{ds : 'ds_telNo_cdList', 	cd: "'001'"},		//지역번호
		{ds : 'ds_lsc_cdList', 		cd: "'017'"},		//양력음력
		{ds : 'ds_emailAddr_cdList',cd: "'CODE_80'"},	//이메일주소
		{ds : 'ds_schlCl_cdList', 	cd: "'CODE_81'"},	//학교구분
		{ds : 'ds_grdtCl_cdList',	cd: "'CODE_82'"},	//졸업구분
		{ds : 'ds_day_cdList',	cd: "'008'"},			//요일
	];
	
	var gdsCode = this.application.gdsCode;
	var filter_txt = 'cdId != cd && cdId == ';
	for(var i = 0; i < ds_codes_list.length; i++){
		obj = ds_codes_list[i];
		gdsCode.filter(filter_txt + obj.cd);
		this[obj.ds].copyData(gdsCode, true);
	}
 }
 this.set_dataset_year = function(){
	var today = new Date();
	for(var i = today.getYear(); i >= 1960; i--){
		var r = this.ds_year_list.addRow();
		this.ds_year_list.setColumn(r, 'cd', i);
		this.ds_year_list.setColumn(r, 'cdNm', i);
	}
 }
 this.set_dataset_default_value = function(){
	//콤보박스 기본값 설정
	var ds_list = [
		{ds : 'ds_telNo_cdList',	cdNm: '선택',
		 id : 'Div01.form.Div03.form.Div01.form.telStNo'},		//지역번호
		{ds : 'ds_emailAddr_cdList',cdNm: '직접입력',
		 id : 'Div01.form.Div05.form.Div00.form.Combo00'},		//이메일주소
		{ds : 'ds_year_list',		cdNm: '연도', 
		 id : 'Div02.form.Div03.form.Grid00', isGrid: true} //연도
	];
		
	for(var i = 0; i < ds_list.length; i++){
		var ds = this[ds_list[i].ds];
		ds.insertRow(0);
		ds.setColumn(0, 'cd', '');
		ds.setColumn(0, 'cdNm', ds_list[i].cdNm);
		
		//초기에 ds data가 없어서 첫 index설정을 추가로 해줘야 한다.
		var compId = ds_list[i].id;
		if(ds_list[i].isGrid){
			/*this.get_componentById(compId).setCellProperty('body', ds_list[i].col, 'text', '0');*/
		} else {
			this.get_componentById(compId).set_index(0);
		}
	}
 }

 /**************************************************************************
 * 뷰 세팅
 **************************************************************************/
this.set_screen_view = function(){
	var pbrltnBrchNm = this.application.gdsLoginInfo.getColumn(0, 'brchNm');
	var pbrltnChrgprid = this.application.gdsLoginInfo.getColumn(0, 'id');
	
	var pbrltnInfo_txt = pbrltnBrchNm + '(' + pbrltnChrgprid + ')';
	this.Div01.form.Div04.form.Div01.form.pbrltnInfo.set_text(pbrltnInfo_txt);
	
	//캘린더 키인 방지
	this.Div01.form.Div04.form.Div00.form.bday.calendaredit.set_readonly(true); 

}
//그리드 내부 캘린더 키인 방지 oninput
this.prevent_gridCalendar_keyin_oninput = function(obj:nexacro.Grid,e:nexacro.InputEventInfo) {
	var ds = obj.getBindDataset();
	
	switch(ds.name) {
	case "ds_tchist_list":
		if(obj.getCellPos() == 0) this.rollback_dataset_data_onGrid(obj, "histStDt");
		else if(obj.getCellPos() == 2) this.rollback_dataset_data_onGrid(obj, "histEndDt");
		break;
	case "ds_tcawrd_list": case "ds_tcathctf_list":
		if(obj.getCellPos() == 0) this.rollback_dataset_data_onGrid(obj, "issueDt");
		break;
	}
}
this.rollback_dataset_data_onGrid = function(obj, dsColNm){
	var ds = obj.getBindDataset();
	var curRow = obj.getSelectedDatasetRows();
	var oriValue = ds.getColumn(dsColNm, curRow);
	
	ds.setColumn(curRow, dsColNm, oriValue);
	obj.setEditingValue(oriValue);
}
//그리드 내부 캘린더 키인 방지 oncloseup
this.prevent_gridCalendar_keyin_oncloseup= function(obj:nexacro.Grid,e:nexacro.GridEditEventInfo) {
	var ds = obj.getBindDataset();
	var curRow = obj.getSelectedDatasetRows();
	
	switch(ds.name) {
	case "ds_tchist_list":
		if(obj.getCellPos() == 0) ds.setColumn(curRow, "histStDt", e.value);
		else if(obj.getCellPos() == 2) ds.setColumn(curRow, "histEndDt", e.value);
		break;
	case "ds_tcawrd_list": case "ds_tcathctf_list":
		if(obj.getCellPos() == 0) ds.setColumn(curRow, "issueDt", e.value);
		break;
	}
}
/**************************************************************************
 * 사진 등록
 **************************************************************************/
this.Div01_btnFileInsert_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.FileDialog00.open("Nexacro N", FileDialog.MULTILOAD);
};
this.FileDialog00_onclose = function(obj:nexacro.FileDialog,e:nexacro.FileDialogEventInfo)
{
	this.addFile(e.virtualfiles);
};
this.addFile = function(filelist){
	var vFile = filelist[0];
	
	if(this.fileExtn.indexOf(vFile.filename.split('.').pop().toLowerCase()) < 0){
		alert("등록가능한 확장자가 아닙니다.");
		return false;
	}
	
	vFile.addEventHandler("onsuccess", this.atch_file_onsuccess, this);
	vFile.addEventHandler("onerror", this.atch_file_onerror, this);	
	vFile.open(null, 1);
}
this.atch_file_onsuccess = function(obj:nexacro.VirtualFile, e:nexacro.VirtualFileEventInfo){
	switch (e.reason)
	{
		case 1:
			obj.getFileSize();
			break;
		case 9:
			if(this.fileMaxSize * 1024 < e.filesize){
				alert("첨부파일은 최대 " + this.fileMaxSize + "Kb까지 등록 가능합니다.");
			} else {
				this.ds_atch_file.setColumn(0, "atchFileId", "");
				this.ds_atch_file.setColumn(0, "realFileNm", obj.filename);			
				this.ds_atch_file.setColumn(0, "fileExtn", obj.filename.split('.').pop().toLowerCase());
				this.ds_atch_file.setColumn(0, "fileSize", this.cutFileSize(e.filesize));

				this.FileUpTransfer00.addFile(obj.filename, obj);
				
				this.fn_upload();
			}
			break;
	}
}
this.atch_file_onerror = function(obj:nexacro.VirtualFile, e:nexacro.VirtualFileErrorEventInfo){
	trace("errortype: "+e.errortype);
	trace("errormsg: "+e.errormsg);
	trace("statuscode: "+e.statuscode);
}
this.cutFileSize = function(filesize)
{
	var sizeMB = nexacro.round(filesize / 1024, 2); // 파일크기 KB로 환산 (2자리 반올림)
	
	return sizeMB + "KB";
};
this.fn_upload = function()
{
	var atchFileList = this.FileUpTransfer00.filelist;
	
	// 업로드할 첨부파일이 있을 경우
	if(atchFileList.length > 0 || this.FileUpTransfer00.getPostData("delFileSeq") != null)
	{
		this.FileUpTransfer00.upload(this.domain + '/admin/file/fileupload.do?atchFileId=');
	}
}
this.FileUpTransfer00_onsuccess = function(obj:nexacro.FileUpTransfer,e:nexacro.FileUpTransferEventInfo)
{
	alert('성공');
	
	var objCallDs = e.datasets[0];
	var atchFileId = objCallDs.getColumn(0,"atchFileId");

	this.ds_techer_info.setColumn(0, 'signImgFileId', atchFileId);
	this.ds_thnl_dtl.setColumn(0, "atchFileId", atchFileId);
	
	this.get_file_detail('ds_thnl_dtl', 'ds_thnl_dtl');
};

/**************************************************************************
 * 기본정보 등록
 **************************************************************************/
this.Div01_Div04_Div00_bday_onchanged = function(obj:nexacro.Calendar,e:nexacro.ChangeEventInfo)
{
	//나이 계산
	var today = new Date();
	var birthDate = new Date(obj.getYear(), obj.getMonth() - 1, obj.getDay());
	if(today.getTime() < birthDate.getTime()){
		obj.set_value(e.prevalue)
		return false;
	}
	
	this.age = today.getFullYear() - birthDate.getFullYear() + 1;
};
this.Div01_Div05_Div00_Combo00_onitemchanged = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
{
	//이메일 도메인
	if(obj.value){
		var row = this.ds_emailAddr_cdList.findRow('cd', obj.value)
		var nm = this.ds_emailAddr_cdList.getColumn(row, 'cdNm')
		this.Div01.form.Div05.form.Div00.form.Edit00_00.set_value(nm);
		this.Div01.form.Div05.form.Div00.form.Edit00_00.set_readonly(true);
	} else {
		this.Div01.form.Div05.form.Div00.form.Edit00_00.set_value("");
		this.Div01.form.Div05.form.Div00.form.Edit00_00.set_readonly(false);
	}
};
