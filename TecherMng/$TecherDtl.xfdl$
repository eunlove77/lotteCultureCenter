}
this.check_duplication_businessNumber = function(){
	
}
/**************************************************************************
 * UTIL
 **************************************************************************/
this.get_file_detail = function(ds, svcId){
	var tranObj = {};
	tranObj.strSvcId	= svcId ? svcId : "get_fileDtl";
	tranObj.strSvcUrl   = this.application.Admin_svc + "::techerMng/selectFileDtl.do";
	tranObj.inData      = "file_detail=" + ds;
	tranObj.outData     = ds + "=file_detail";
	tranObj.strArg      = "";
	tranObj.callBackFnc = "fnCallback";
	tranObj.isAsync     = true;
	tranObj.menuSeq 	= this.parent.ownerFrame.param.MENU_ID
	
	this.gfn_transaction(tranObj);
}
this.fnCallback = function(svcID, errorCode, errorMsg){
	if(errorCode != 0){
		this.alert(errorCode+"\n"+errorMsg);
		return false;
	}
	
}
this.get_componentById = function(str){
	var comp = this;
	var list = str.split('.');
	for(var id of list){
		comp = comp[id];
	}
	return comp;
}

this.fnCallback = function(svcId, errorCode, errorMsg){
	if(errorCode != 0){
		this.alert(errorCode+"\n"+errorMsg);
		return;
	}
	
	if(svcId == "select_techer"){
		this.set_techer_info();
		
		//기본정보 사진파일 가져오기
		this.ds_thnl_dtl.setColumn(0, 'atchFileId', this.ds_techer_info.getColumn(0, 'phtFileId'));
		this.get_file_detail('ds_thnl_dtl', 'ds_thnl_dtl');
		//서명정보 사진파일 가져오기
		this.ds_thnl_dtl.setColumn(0, 'atchFileId', this.ds_techer_info.getColumn(0, 'signImgFileId'));
		this.get_file_detail('ds_sign_dtl');
	}
	else if(svcId = 'ds_sign_dtl'){
		
	}
	else if(svcID == 'get_lect_plan'){
		
		this.fn_callback_lectureplan_getFile();
		
	}else if(svcID == 'ds_thnl_dtl'){
		this.Div01.form.Div00.form.ImageViewer00.set_text('');
		
	}
}
