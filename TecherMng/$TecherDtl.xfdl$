{
	this.close();
};
this.btnSave_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	//데이터 세팅
	this.ds_techer_info.setColumn(0, 'signImgFileId', this.ds_sign_dtl.getColumn(0, 'atchFileId'));
	//지급정보
	this.set_techer_info_rrno();
	
	//유효성 검사
	var vali_res = this.validate_techer();
	if(!vali_res){
		return false;
	}
	
	//submit
	if(confirm("강사정보를 등록 하시겠습니까?")){
		this.submit_techer_info();
	}
};
this.submit_techer_info = function(){

	var inData = "";
	inData += "techerDtl=ds_techer_info";
	inData += " tceduList=ds_tcedu_list:U";
	inData += " tchistList=ds_tchist_list:U";
	inData += " tcawrdList=ds_tcawrd_list:U";
	inData += " tcathctfList=ds_tcathctf_list:U";
	inData += " tcmdfreqList=ds_tcmdfreq_list:U";
	
	var tranObj = {};
	tranObj.strSvcId	= "update_techer";
	tranObj.strSvcUrl   = this.application.Admin_svc + "::techerMng/update.do";
	tranObj.inData      = inData;
	tranObj.outData     = "";
	tranObj.strArg      = "";
	tranObj.callBackFnc = "fnCallback_techer";
	tranObj.isAsync     = true;
	tranObj.menuSeq 	= this.parent.ownerFrame.param.MENU_ID
	
	this.gfn_transaction(tranObj);
}
this.fnCallback_techer = function(svcID, errorCode, errorMsg){
	if(errorCode != 0){
		// 에러 시 화면 처리 내역
		this.alert(errorCode+"\n"+errorMsg);
		return;
	}
	alert('저장됐습니다.');
	//this.close();
}
/**************************************************************************
 * 전송 전 데이터 가공
 **************************************************************************/
 this.set_techer_info_rrno = function(){
	var rrno = "";
	var tcTpCd = this.ds_techer_info.getColumn(0, 'tcTpCd');
	
	this.ds_techer_info.setColumn(0, 'rrno', this.ds_sign_dtl.getColumn(0, 'atchFileId'));
 }
 
/**************************************************************************
 * 유효성 검사
 **************************************************************************/
this.validate_techer = function(){

	if(!this.validate_techer_basic()) return false;
	
	if(!this.validate_techer_edu()) return false;
	if(!this.validate_techer_hist()) return false;
	if(!this.validate_techer_awrd()) return false;
	if(!this.validate_techer_athctf()) return false;
	
	return true;
}
this.validate_techer_basic = function(){

	// 강사 기본정보 검사
	var ds = this.ds_techer_info;
	// 조건별 필수항목
	var tcNm = "강사명을";
	var bday = ['bday', '생년월일을'];
	var tcTpCd = ds.getColumn(0, "tcTpCd");
	if(tcTpCd == 2){
		tcNm = "사업자명을";
	} else if(tcTpCd == 3){
		tcNm = "사업자명을";
		bday = null;
	}
	var vali_arr = [
		['tcNm', tcNm], ['hpStNo', '휴대전화를'], ['hpMidNo', '휴대전화를'], ['hpLastNo', '휴대전화를'],
		bday, ['addr', '주소를']
	]
	
	for(var vali of vali_arr){
		if(vali && (this.gfn_nvl(ds.getColumn(0, vali[0])) == "" || nexacro.trim(ds.getColumn(0, vali[0])) == "")){
			alert(vali[1] + " 입력하세요.");
			return false;
		}
	}

	return true;
}
//학력
this.validate_techer_edu = function(){
	var vali_arr= [['yy', '연도를'], ['schlNm', '학교명을'], ['mjrNm', '전공을']];
	return this.validate_techer_ds_list("ds_tcedu_list", vali_arr);
}
//경력
this.validate_techer_hist = function(){
	var vali_arr= [['histStDt', '기간을'], ['histEndDt', '기간을'], ['histPlcNm', '기관명을']]
	return this.validate_techer_ds_list("ds_tchist_list", vali_arr);
}
//수상
this.validate_techer_awrd = function(){
	var vali_arr= [['issueDt', '기간을'], ['issueAgncNm', '기관명을'], ['issueNm', '수상내역을']]
	return this.validate_techer_ds_list("ds_tcawrd_list", vali_arr);
}
//자격
this.validate_techer_athctf = function(){
	var vali_arr= [['issueDt', '기간을'], ['issueAgncNm', '기관명을'], ['issueNm', '자격증명을']];
	return this.validate_techer_ds_list("ds_tcathctf_list", vali_arr);
}

this.validate_techer_ds_list = function(ds, vali_arr){
	var ds = this[ds];
	for(var i = 0; i < ds.rowcount; i++){
		for(var vali of vali_arr){
			if(vali && (this.gfn_nvl(ds.getColumn(i, vali[0])) == "" || nexacro.trim(ds.getColumn(i, vali[0])) == "")){
				alert(vali[1] + " 입력하세요.");
				return false;
			}
		}
	}
	return true;
}
