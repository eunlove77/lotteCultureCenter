    <Script type="xscript5.1"><![CDATA[/**************************************************************************
 * include 영역 
 **************************************************************************/
include "Base::libCommon.xjs";
include "Base::popup.xjs";

/**************************************************************************
 * FORM 변수 선언 영역
 **************************************************************************/
this.application = null;

/**************************************************************************
 * FORM EVENT 영역(onload)
 **************************************************************************/
this.TecherLectureList_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.application = nexacro.getApplication();
	this.txtMenuNm.set_text(obj.parent.parent.fvMenuNM);
	
	//그리드 체크박스 세팅
	this.gfnSetCheckBoxNoDataset(this.ds_tclect_list, this.Grid00, 0);
	
	// 코드 데이터
	this.set_dataset_from_code();
	this.set_dataset_others();
	
	//체크박스 세팅
	this.set_dynamic_checkbox();
	
	//지점명 세팅
	this.set_branch_on_user();
	
	this.reset_search_form();
};

/**************************************************************************
 * 사용자 FUNCTION 영역
 **************************************************************************/
this.set_dataset_from_code = function(){
	var ds_codes_list = [
		{ds : 'ds_spot_list',		cd: "'spotCd'"},	//지점
		{ds : 'ds_tcTp_list',		cd: "'CODE_62'"},	//강사유형
		{ds : 'ds_semester_list',	cd: "'034'"},		//학기구분
		{ds : 'ds_classGb_list',	cd: "'006'"},		//강좌구분
		{ds : 'ds_classCtgy_list',	cd: "'CLASS_02'"},	//강좌유형
	];
	
	var gdsCode = this.application.gdsCode;
	var filter_txt = 'cdId != cd && cdId == ';
	for(var i = 0; i < ds_codes_list.length; i++){
		obj = ds_codes_list[i];
		gdsCode.filter(filter_txt + obj.cd);
		this[obj.ds].copyData(gdsCode, true);
	}
	
	//지점명 표기수정
	for(var i = 0; i < this.ds_spot_list.rowcount; i++){
		var cd = this.ds_spot_list.getColumn(i, 'cd');
		var cdNm = this.ds_spot_list.getColumn(i, 'cdNm');
		this.ds_spot_list.setColumn(i, 'cdNm', '[' + cd + '] ' + cdNm);
	}
}
this.set_dataset_default_value = function(){
	//콤보박스 기본값 설정
	var ds_list = [
		{ds : 'ds_telNo_cdList',	cdNm: '선택',
		 id : 'Div01.form.Div03.form.Div01.form.telStNo'},		//지역번호
		{ds : 'ds_emailAddr_cdList',cdNm: '직접입력',
		 id : 'Div01.form.Div05.form.Div00.form.Combo00'},		//이메일주소
		{ds : 'ds_year_list',		cdNm: '연도', 
		 id : 'Div02.form.Div03.form.Grid00', isGrid: true} //연도
	];
	
	for(var i = 0; i < ds_list.length; i++){
		var ds = this[ds_list[i].ds];
		ds.insertRow(0);
		ds.setColumn(0, 'cd', '');
		ds.setColumn(0, 'cdNm', ds_list[i].cdNm);
		
		var compId = ds_list[i].id;
		if(ds_list[i].isGrid){
			/*this.get_componentById(compId).setCellProperty('body', ds_list[i].col, 'text', '0');*/
		} else {
			this.get_componentById(compId).set_index(0);
		}
	}
}

this.set_dataset_others = function(){
	this.set_dataset_year();
	this.set_dataset_semester_default();
}
this.set_dataset_year = function(){
	var year = new nexacro.Date().getFullYear();
	var obj = this.Div01.form.Div01.form.Combo00;
	
	this.ds_year_list.set_enableevent(false);
	this.ds_year_list.clearData();
	for(var i =0; i <= 15; i++){
		this.ds_year_list.addRow();
		this.ds_year_list.setColumn(i, "cd", year-i);
		this.ds_year_list.setColumn(i, "cdNm", (year-i) + "년");
	}
	this.ds_year_list.set_enableevent(true);
	
	obj.set_index(0);
}
this.set_dataset_semester_default = function(){
	var month = new nexacro.Date().getMonth()+1;
	var obj = this.Div01.form.Div01.form.Combo01;
	var index = 0;
	
	if(3 <= month && month <= 5) index = 0;	//봄
	else if(6 <= month && month <= 8) index = 1; //여름
	else if(9 <= month && month <= 11) index = 2; //가을
	else index = 3; //겨울
	
	obj.set_index(index);
}

//동적 체크박스 추가
this.set_dynamic_checkbox = function()
{
	this.set_dynamic_checkbox_from_dataset(this.ds_classGb_list, this.Div01.form.Div02);		//강좌구분
	this.set_dynamic_checkbox_from_dataset(this.ds_classCtgy_list, this.Div02.form.Div01);	//강좌유형
	this.set_dynamic_checkbox_from_dataset(this.ds_tcTp_list, this.Div03.form.Div00.form.Div00);	//강사유형
	
	//클릭 이벤트 추가
	this.fn_click_add(this);
}
this.set_dynamic_checkbox_from_dataset = function(ds, p_div){
	
	var content = ["chkAll"];
	for(var i=0; i < ds.rowcount; i++) 
	{
		var id = "chk" + i;
		var obj = new CheckBox();
		obj.init(id, content[i] + ":6", 5, 90, 20);
		p_div.addChild(id, obj);
		obj.set_text(ds.getColumn(i, "cdNm"));		
		obj.set_truevalue(ds.getColumn(i, "cd"));
		obj.set_falsevalue("");
		obj.set_fittocontents("width");
		obj.show();	
		content.push(id);
	}
}

this.set_branch_on_user = function(){
	var user_brchCd = this.application.gdsLoginInfo.getColumn(0, 'brchCd');
	var user_brchCdNm = this.application.gdsLoginInfo.getColumn(0, 'brchCdNm');
	var ds = this.ds_spot_list;
	
	if(user_brchCdNm != '본사'){
		ds.filter('cd = ' + user_brchCd);
	}
	
	this.ds_search.setColumn(0, 'brchCd', user_brchCd);
}

