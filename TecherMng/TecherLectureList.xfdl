	md_ds.set_enableevent(false);
	md_ds.insertRow(0);
	md_ds.setColumn(0, "cdId", "");
	md_ds.setColumn(0, "cd", "");
	md_ds.setColumn(0, "cdNm", "중분류");
	middle_obj.set_index(0);
